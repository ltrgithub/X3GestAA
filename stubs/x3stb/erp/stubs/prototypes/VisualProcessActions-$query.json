{
	"$baseUrl": "http://syra_server/x3/demo",
	"$baseType": "application/json; vnd.sage.syracuse.representation=x3.erp.demo",
	"$url": "{$baseUrl}/conssta?&codeStat={$codeStat}&dateStart={$dateStart}&dateEnd={$dateEnd}&keyValue1={$keyValue1}&keyValue2={$keyValue2}&keyValue3={$keyValue3}",
	"$title": "Actions",
	"$methods": "GET, POST, PUT, DELETE",
	"$": {
        "description" :
                {
                        "$title": "Test case description",
                        "$type": "application/x-string",
			
		},
		"process": {
			"$type": "application/x-process",
			"$title": "Visual Process Player"
		}
		
	},
	"$article": {
		"$layout": {
			"$items": [
                        { 
                        	"$bind": "description",
				"$isEditMode": false,
				"$labelWidth": "auto",
				"$value": " when mouseover : blue rectangle appears, when klick : show/hide another clipshape "
                        },
			
                        {
				"$bind": "process",
				"$isEditMode": false,
				"$labelWidth": "auto"
			}]
		}
	}
}
