{
	"$baseUrl": "/sdata/x3/erp/SUPERV",
	"$url": "{$baseUrl}/STAT('DOM')?representation=STAT~DOM.$cube&role={$role}",
	"$title": "Statistiques fichier",
	"$type": "application/json; vnd.sage=syracuse.presentation=x3.erp.superv/STAT('DOM')",
	"$methods": "GET",
	"codeStat": "DOM",
	"$": {
		"$resources": {
			"$type": "application/x-collection",
			"$provType": "PORTALSTA",
			"$criteria": {
				"FACILITY": {
					"$type": "application/x-reference",
					"$url": "{$baseUrl}/FACILITY('{$key}')?representation=FACILITY.$details&role={$role}"
				},
				"COMPANY": {
					"$type": "application/x-reference",
					"$url": "{$baseUrl}/COMPANY('{$key}')?representation=COMPANY.$details&role={$role}"
				},
				"DATBEG1": {
					"$type": "application/x-date",
					"$title": ""
				},
				"DATEND1": {
					"$type": "application/x-date",
					"$title": ""
				},
				"DATBEG2": {
					"$type": "application/x-date",
					"$title": ""
				},
				"DATEND2": {
					"$type": "application/x-date",
					"$title": ""
				}
			},
			"$item": {
				"$type": "application/json",
				"$url": "{$baseUrl}/stats('DOM')?representation=$cube('DOM')&role={$role}",
				"$title": "Statistiques fichier",
				"$": {
					"dateStat": {
						"$type": "application/x-date",
						"$capabilities": "sort,filter"
					},
					"MODULE": {
						"$title": "Module",
						"$type": "application/x-choice",
						"$value": {
							"$type": "application/x-integer",
							"$constraints": {
								"$enum": [{
									"$value": 1,
									"$title": "Tronc commun"
								}, {
									"$value": 2,
									"$title": "Interne superviseur"
								}, {
									"$value": 3,
									"$title": "Interface compta"
								}, {
									"$value": 4,
									"$title": "Pièces automatiques"
								}, {
									"$value": 5,
									"$title": "Requêteur"
								}, {
									"$value": 6,
									"$title": "Traductions"
								}, {
									"$value": 7,
									"$title": "Pyramides"
								}, {
									"$value": 8,
									"$title": "A réutiliser"
								}, {
									"$value": 9,
									"$title": "Développement"
								}, {
									"$value": 10,
									"$title": "Spécial Produits"
								}, {
									"$value": 11,
									"$title": "Portail"
								}, {
									"$value": 12,
									"$title": "XTEND"
								}, {
									"$value": 13,
									"$title": "Business Intelligence"
								}, {
									"$value": 14,
									"$title": "S-Data"
								}, {
									"$value": 15,
									"$title": "#"
								}, {
									"$value": 16,
									"$title": "#"
								}, {
									"$value": 17,
									"$title": "#"
								}, {
									"$value": 18,
									"$title": "#"
								}, {
									"$value": 19,
									"$title": "#"
								}, {
									"$value": 20,
									"$title": "#"
								}]
							}
						}
					},
					"CLENUM1": {
						"$title": "Clé primaire de l'objet",
						"$type": "application/x-string",
						"$constraints": {
							"$maxLength": 15
						},
						"$capabilities": "sort,filter",
						"$isHidden": false,
						"$isParameter": true
					},
					"UPDUSR": {
						"$title": "Utilisateur",
						"$type": "application/x-reference",
						"$url": "{$baseUrl}/AUTILIS('{$key}')?representation=AUTILIS.$thumb&role={$role}",
						"$capabilities": "sort,filter",
						"$links": {
							"$details": {
								"$type": "application/json;vnd.sage=syracuse",
								"$url": "{$baseUrl}/AUTILIS('{$key}')?representation=AUTILIS.$details&role={$role}"
							}
						},
						"$": {
							"USR": {
								"$isMandatory": true,
								"$type": "application/x-string",
								"$constraints": {
									"$maxLength": 5
								}
							}
						}
					},
					"CPY": {
						"$title": "Société",
						"$type": "application/x-reference",
						"$url": "{$baseUrl}/COMPANY('{$key}')?representation=COMPANY.$thumb&role={$role}",
						"$capabilities": "sort,filter",
						"$links": {
							"$details": {
								"$type": "application/json;vnd.sage=syracuse",
								"$url": "{$baseUrl}/COMPANY('{$key}')?representation=COMPANY.$details&role={$role}"
							}
						},
						"$": {
							"CPY": {
								"$isMandatory": true,
								"$type": "application/x-string",
								"$constraints": {
									"$maxLength": 5
								}
							}
						}
					},
					"CUR": {
						"$title": "Devise",
						"$type": "application/x-reference",
						"$url": "{$baseUrl}/TABCUR('{$key}')?representation=TABCUR.$thumb&role={$role}",
						"$capabilities": "sort,filter",
						"$links": {
							"$details": {
								"$type": "application/json;vnd.sage=syracuse",
								"$url": "{$baseUrl}/TABCUR('{$key}')?representation=TABCUR.$details&role={$role}"
							}
						},
						"$": {
							"CUR": {
								"$isMandatory": true,
								"$type": "application/x-string",
								"$constraints": {
									"$maxLength": 3
								}
							}
						}
					},
					"amount1": {
						"$type": "application/x-decimal",
						"$format": "#.00",
						"$title": "Nombre 1"
					},
					"amount2": {
						"$type": "application/x-decimal",
						"$format": "#",
						"$title": "Nombre 2"
					},
					"amount3": {
						"$type": "application/x-decimal",
						"$format": "#.000",
						"$title": "Nombre 3"
					},
					"amount4": {
						"$type": "application/x-decimal",
						"$format": "#.00",
						"$title": "Nombre 4"
					}
				}
			},
			"$cube": {
				"$mode": "table",
				"$title": "Stats fiches",
				"$displaysOneMeasure": true,
				"$hierarchies": {
					"MODULE": {
						"$title": "Module",
						"$properties": ["MODULE"]
					},
					"CLENUM1_UPDUSR_CPY": {
						"$title": "Clé primaire de l'objet/Utilisateur/Société",
						"$properties": ["CLENUM1", "UPDUSR", "CPY"]
					},
					"CUR": {
						"$title": "Devise",
						"$properties": ["CUR"]
					},
					"date": {
						"properties": ["dateStat"],
						"periods": "YSQMD"
					}
				},
				"$measures": {
					"amount1": {
						"$isDefault": true
					},
					"amount2": {},
					"amount3": {},
					"amount4": {}
				}
			},
			"$axes": [{
				"$title": "Module",
				"$hierarchies": [
					["MODULE", 1, 0],
					["CLENUM1_UPDUSR_CPY", 0, 0],
					["CUR", 0, 0]
				]
			}, {
				"$title": "Measures",
				"$hierarchies": [
					["$measures"]
				]
			}],
			"$slicer": {
				"$title": "MODULE",
				"$hierarchies": [
					["MODULE", 1, 0]
				]
			}
		}
	},
	"$article": {
		"$layout": {
			"$items": [
				{
					"$bind": "$resources",
					"$chartType": "fusionChartsXT"
				}
			]
		}
	}
}
