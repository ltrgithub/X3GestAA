{
    "$descriptor": "mobile prototype x3stb.erp.stubs.employee.$details,$page",
    "$type": "application/json",
    "$baseType": "application/json;vnd.sage=syracuse",
    "$baseUrl": "http://localhost:8126/sdata/sprint1/settings/sprint1",
    "$url": "{$baseUrl}/{$pluralType}('{$key}')",
    "$value": "{firstName} {lastName}",
    "$title": "{firstName} {lastName}",
    "$pluralType": "employees",
    "$singularType": "employee",
    "$properties": {
        "firstName": {
            "$title": "First Name",
            "$type": "application/x-string",
            
                "$maxLength": 30
            ,
            "$capabilities": "sort,filter"
        },
        "lastName": {
            "$title": "Last Name",
            "$type": "application/x-string",
            
                "$maxLength": 30
            ,
			"$isEditMode" : true,
            "$isMandatory": true,
            "$capabilities": "sort,filter",
            "$links": {
                "$details": {
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$singularType}.$details&role={$role}"
                },
				"$details1": {
					"$title" : "$details1",
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$singularType}.$summary&role={$role}"
                },
				"$details2": {
					"$title" : "$details2",
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$singularType}.$details&role={$role}"
                },
				"$details3": {
					"$title" : "$details3",
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$singularType}.$details&role={$role}"
                }
            }
        },
        "title": {
            "$title": "Title",
            "$type": "application/x-choice",
            "$value": {
                "$type": "application/x-integer",
                
                    "$enum": [
                        {
                            "$value": 1,
                            "$title": "Monsieur"
                        },
                        {
                            "$value": 2,
                            "$title": "Mademoiselle"
                        },
                        {
                            "$value": 3,
                            "$title": "Madame"
                        }
                    ]
                },
            "$capabilities": "sort,filter"
        },
        "maritalStatus": {
            "$title": "Marital Status",
            "$type": "application/x-choice",
            "$value": {
                "$type": "application/x-integer",
                
                    "$enum": [
                        {
                            "$value": 0,
                            "$title": "Célibataire"
                        },
                        {
                            "$value": 1,
                            "$title": "Marié"
                        },
                        {
                            "$value": 2,
                            "$title": "Divorcé"
                        },
                        {
                            "$value": 3,
                            "$title": "Séparé"
                        },
                        {
                            "$value": 4,
                            "$title": "Veuf"
                        },
                        {
                            "$value": 5,
                            "$title": "Vie maritale"
                        },
                        {
                            "$value": 6,
                            "$title": "PACS"
                        },
                        {
                            "$value": 7,
                            "$title": "Non connu"
                        },
                        {
                            "$value": 8,
                            "$title": "Non applicable"
                        }
                    ]
                },
            "$capabilities": "sort,filter"
        },
        "birthDate": {
            "$title": "Birth Date",
            "$type": "application/x-date",
            "$isMandatory": false,
            "$capabilities": "sort,filter"
        },
        "foreignerWorker": {
            "$title": "Foreigner worker",
            "$type": "application/x-boolean",
            "$capabilities": "sort,filter"
        },
        "email": {
            "$title": "Email",
            "$type": "application/x-string",
            "$format": "$email",
            "$capabilities": "sort,filter"
        },
        "phoneNumber": {
            "$title": "Phone Number",
            "$type": "application/x-string",
            "$format": "$phone",
            "$capabilities": "sort,filter"
        },
        "photo": {
            "$title": "Photo",
            "$type": "image",
            "$url": "{$baseUrl}/$workingCopies('{$trackingId}')/photo",
            "$capabilities": ""
        },
        "addressStreet": {
            "$title": "Street",
            "$type": "application/x-string",
            "$capabilities": "sort,filter"
        },
        "addressZipCode": {
            "$title": "Zip code",
            "$type": "application/x-string",
            "$capabilities": "sort,filter"
        },
        "addressCity": {
            "$title": "City",
            "$type": "application/x-string",
            "$capabilities": "sort,filter"
        },
        "country": {
            "$title": "Country",
            "$isMandatory": false,
            "$capabilities": "sort,filter",
            "$type": "application/x-reference",
            "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={singularType}.$thumb",
            "$value": "{code}",
            "$description": "Country",
            "$properties": {
                "code": {
                    "$type": "application/x-string"
                },
                "description": {
                    "$type": "application/x-string"
                }
            },
            "$links": {
                "$details": {
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/countries('{$key}')?representation=country.$details&role={$role}"
                },
                "$lookup": {
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/countries?representation=country.$lookup&role={$role}&trackingId={$trackingId}&binding=country"
                }
            }
        },
        "company": {
            "$title": "Company",
            "$isMandatory": true,
            "$capabilities": "sort,filter",
            "$type": "application/x-reference",
            "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={singularType}.$thumb",
            "$value": "{code}",
            "$description": "Company",
            "$properties": {
                "code": {
                    "$type": "application/x-string"
                },
                "description": {
                    "$type": "application/x-string"
                }
            },
            "$links": {
                "$details": {
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/companies('{$key}')?representation=company.$details&role={$role}"
                },
                "$lookup": {
                    "$type": "application/json;vnd.sage=syracuse",
                    "$url": "{$baseUrl}/companies?representation=company.$lookup&role={$role}&trackingId={$trackingId}&binding=company"
                }
            }
        },
        "children": {
            "$title": "Children",
            "$capabilities": "sort,filter",
            "$type": "application/x-array",
            "$item": {
                "$descriptor": "prototype sprint1.settings.sprint1.employee.$edit,$page",
                "$type": "application/json",
                "$pluralType": "children",
                "$singularType": "child",
                "$properties": {
                    "firstName": {
                        "$title": "First Name",
                        "$type": "application/x-string",
                        
                            "$maxLength": 30
                        ,
                        "$capabilities": "sort,filter"
                    },
                    "gender": {
                        "$title": "Gender",
                        "$type": "application/x-choice",
                        "$value": {
                            "$type": "application/x-integer",
                            
                                "$enum": [
                                    {
                                        "$value": 1,
                                        "$title": "Homme"
                                    },
                                    {
                                        "$value": 2,
                                        "$title": "Femme"
                                    }
                                ]
                            },
                        "$capabilities": "sort,filter"
                    },
                    "birthDate": {
                        "$title": "Birth Date",
                        "$type": "application/x-date",
                        "$isMandatory": false,
                        "$capabilities": "sort,filter"
                    },
                    "$creUser": {
                        "$title": "Created by",
                        "$type": "application/x-string",
                        "$isReadOnly": true,
                        "$isHidden": true,
                        "$capabilities": "sort,filter"
                    },
                    "$updUser": {
                        "$title": "Updated by",
                        "$type": "application/x-string",
                        "$isReadOnly": true,
                        "$isHidden": true,
                        "$capabilities": "sort,filter"
                    },
                    "$creDate": {
                        "$title": "Created on",
                        "$type": "application/x-datetime",
                        "$isReadOnly": true,
                        "$isHidden": true,
                        "$capabilities": "sort,filter"
                    },
                    "$updDate": {
                        "$title": "Updated on",
                        "$type": "application/x-datetime",
                        "$isReadOnly": true,
                        "$isHidden": true,
                        "$capabilities": "sort,filter"
                    }
                },
                "$links": {}
            }
        },
        "$creUser": {
            "$title": "Created by",
            "$type": "application/x-string",
            "$isReadOnly": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
        },
        "$updUser": {
            "$title": "Updated by",
            "$type": "application/x-string",
            "$isReadOnly": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
        },
        "$creDate": {
            "$title": "Created on",
            "$type": "application/x-datetime",
            "$isReadOnly": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
        },
        "$updDate": {
            "$title": "Updated on",
            "$type": "application/x-datetime",
            "$isReadOnly": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
        }
    },
    "title": {
        "$title": "Title",
        "$type": "application/x-choice",
        "$value": {
            "$type": "application/x-integer",
            
                "$enum": [
                    {
                        "$value": 1,
                        "$title": "Monsieur"
                    },
                    {
                        "$value": 2,
                        "$title": "Mademoiselle"
                    },
                    {
                        "$value": 3,
                        "$title": "Madame"
                    }
                ]
            },
        "$capabilities": "sort,filter"
    },
    "maritalStatus": {
        "$title": "Marital Status",
        "$type": "application/x-choice",
        "$value": {
            "$type": "application/x-integer",
            
                "$enum": [
                    {
                        "$value": 0,
                        "$title": "Célibataire"
                    },
                    {
                        "$value": 1,
                        "$title": "Marié"
                    },
                    {
                        "$value": 2,
                        "$title": "Divorcé"
                    },
                    {
                        "$value": 3,
                        "$title": "Séparé"
                    },
                    {
                        "$value": 4,
                        "$title": "Veuf"
                    },
                    {
                        "$value": 5,
                        "$title": "Vie maritale"
                    },
                    {
                        "$value": 6,
                        "$title": "PACS"
                    },
                    {
                        "$value": 7,
                        "$title": "Non connu"
                    },
                    {
                        "$value": 8,
                        "$title": "Non applicable"
                    }
                ]
            },
        "$capabilities": "sort,filter"
    },
    "foreignerWorker": {
        "$title": "Foreigner worker",
        "$type": "application/x-boolean",
        "$capabilities": "sort,filter"
    },
    "$links": {
        "$delete": {
            "$title": "Supprimer",
            "$confirm": "L'enregistrement  sera supprimé. Confirmez-vous la suppression ?",
            "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$singularType}.$details&role={$role}",
            "$type": "application/json;vnd.sage=syracuse",
            "$method": "DELETE"
        },
        "$query": {
            "$type": "application/json;vnd.sage=syracuse",
            "$title": "Liste de employees",
            "$url": "{$baseUrl}/{$pluralType}?representation={$singularType}.$query&role={$role}"
        },
        "$print": {
            "$type": "application/pdf",
            "$title": "Imprimer",
            "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$singularType}.$details&role={$role}"
        }
    },
    "$article": {
        "$layout": {
            "$items": [
                {
                    "$title": "General",
                    "$category": "section",
                    "$layout": {
                        "$items": [
                            {
                                "$title": "General",
                                "$category": "block",
                                "$layout": {
                                    "$items": [
                                        {
                                            "$bind": "photo"
                                        }
                                    ]
                                }
                            },
                            {
                                "$title": "Personnal informations",
                                "$category": "block",
                                "$layout": {
                                    "$items": [
                                        {
                                            "$bind": "firstName"
                                        },
                                        {
                                            "$bind": "lastName",
											"$isEditMode" : true
                                        },
                                        {
                                            "$bind": "title"
                                        },
                                        {
                                            "$bind": "maritalStatus"
                                        },
                                        {
                                            "$bind": "birthDate"
                                        }
                                    ]
                                }
                            },
                            {
                                "$title": "Children",
                                "$category": "block",
                                "$layout": {
                                    "$items": [
                                        {
                                            "$bind": "children",
											"$isTitleHidden": true
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                },
                {
                    "$title": "Professional info",
                    "$category": "section",
                    "$layout": {
                        "$items": [
                            {
                                "$title": "Address",
                                "$category": "block",
                                "$layout": {
                                    "$items": [
                                        {
                                            "$bind": "addressStreet"
                                        },
                                        {
                                            "$bind": "addressZipCode"
                                        },
                                        {
                                            "$bind": "addressCity"
                                        },
                                        {
                                            "$bind": "country"
                                        }
                                    ]
                                }
                            },
                            {
                                "$title": "Misc.",
                                "$category": "block",
                                "$layout": {
                                    "$items": [
                                        {
                                            "$bind": "foreignerWorker"
                                        },
                                        {
                                            "$bind": "email"
                                        },
                                        {
                                            "$bind": "phoneNumber"
                                        },
                                        {
                                            "$bind": "company"
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            ]
        }
    }
}