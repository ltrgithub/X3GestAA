    "version": "0.2.0",
    "configurations": [
        {
            "type": "node2",
            "request": "launch",
            "name": "Launch",
            "program": "${workspaceRoot}/index.js",
            "cwd": "${workspaceRoot}",
            "outFiles": ["${workspaceRoot}/.streamline/**/*"],
            "sourceMaps": true,
            "diagnosticLogging": false,
            "verboseDiagnosticLogging": false
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 5858,
            "outFiles": ["${workspaceRoot}/.streamline/**/*"],
            "sourceMaps": true
        }
    ]
}