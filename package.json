{
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/mongodb": "^2.1.33",
    "@types/node": "^6.0.45",
    "streamline-node": "^2.0.1"
  },
  "dependencies": {
    "@sage/syracuse-core": "^2.0.12",
    "ez-streams": "^3.2.2",
    "streamline-runtime": "^1.0.37"
  },
  "scripts": {
    "test": "mocha --opts mocha.opts node_modules/syracuse-[a-t]*/test/server/**.ts node_modules/@sage/syracuse-lib/test/**/server/**.ts",
    "test:jenkins": "mocha --opts mocha.opts --reporter mocha-jenkins-reporter node_modules/syracuse-[a-t]*/test/server/**.ts node_modules/@sage/syracuse-lib/test/**/server/**.ts",
  	"test:coverage": "node build-coverage-tree && pushd \"../syracuse-coverage/node_modules/@sage/syracuse-lib\" && npm install mocha && istanbul cover -i \"./src/**\" node_modules/mocha/bin/_mocha -- --opts ./test/mocha_cov.opts -t 60000 test/start.js test/**/server/*.js && popd",
    "security:retire-win": "retire --jspath node_modules --nodepath shadow-modules/win32-x64-v8-4.5 --exitwith 0",
    "security:retire-linux": "retire --jspath node_modules --nodepath shadow-modules/linux-x64-v8-4.5 --exitwith 0",
    "customer-image": "node apatch direct --image ../syracuse_customer_image --desc \"Syracuse\" --no-check"
  }
}
