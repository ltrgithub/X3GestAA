"use strict";

function entities() {
	if (/^v0\.1/.test(process.version)) {
		console.error("node 0.12 is supported in degraded mode - skipping SCM helper registration");
		return {};
	} else {
		return {
			scmConfig: require('./entities/scmConfig'),
		};
	}
}

module.exports = {
	application: "syracuse",
	extends: "collaboration",
	entities: entities(),
};