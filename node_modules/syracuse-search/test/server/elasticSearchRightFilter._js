	strictEqual(JSON.stringify(search.query.filtered.filter), '{"and":[{"or":[{"regexp":{"representationRef.entity":"test1"}}]},{"missing":{"field":"representationRef.entity"}}]}', "x3right right filter : " + JSON.stringify(search.query.filtered.filter));

	//only $representation without condition,site with multiple accessCode and multiple representation
	x3Right = {
		$representations: {
			TEST1: {
				$accessCodes: "TEST1"
			},
			TEST2: {
				$accessCodes: "TEST1,TEST2"

			},
			TEST3: {
				$accessCodes: "TEST1,TEST3"
			}
		},

	};
