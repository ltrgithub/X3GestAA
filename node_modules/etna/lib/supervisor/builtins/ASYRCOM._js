"use strict";
var glob = require('streamline/lib/globals');

module.exports = {
	TRACE: {
		params: ['MESSAGE'],
		body(_) {
			var frame = glob.context.x3frame;
			var message = frame.values.MESSAGE;
			console.log(message.toString());
		},
	},
	SYRSND: {
		params: ['ASND'],
		body(_) {
			var frame = glob.context.x3frame;
			var asnd = frame.values.ASND;
			asnd.$send(_);
		},
	}
};