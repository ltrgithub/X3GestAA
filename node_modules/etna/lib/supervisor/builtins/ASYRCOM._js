"use strict";

const glob = require('streamline/lib/globals');

module.exports = {
	TRACE: {
		params: ['MESSAGE'],
		body(_) {
			const frame = glob.context.x3frame;
			const message = frame.values.MESSAGE;

			console.log(message.toString());
		}
	},
	SYRSND: {
		params: ['ASND'],
		body(_) {
			const frame = glob.context.x3frame;
			const asnd = frame.values.ASND;

			asnd.$send(_);
		}
	}
};