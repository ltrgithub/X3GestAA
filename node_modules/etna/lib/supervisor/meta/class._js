					// add parameters:
					methArgs = Object.keys(meth.params).reduce(function(r, p) {
						if (meth.params[p].type) {
							r[p] = {
								type: meth.params[p].type,
								value: meth.params[p].get(vals),
								isParam: true,
							};
							if (meth.params[p].set) r[p].set = meth.params[p].set;
						}
						return r;
					}, methArgs );

