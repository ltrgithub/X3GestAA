"use strict";

var generic = require('ez-streams').devices.generic;

var c1 = [1,2,3,4]
var c2 = ["toto1","toto2", "toto3", "toto4"]

module.exports = {
	reader: function(arrays, names) {
		var pos = 0;
		return generic.reader(function(_) {
			if (pos < c1.length) {
				var row = {};

				row[names[0]] = c1[pos];
				row[names[1]] = c2[pos];
				pos += 1;
				return row;
			}
			return undefined;
		}, function stop(_) {

		});
	},
};