#<AdxTL>@(#)8.0.0.0 $Revision$
Local Clbfile RESULT
RESULT = func TESTSUITE()
Call FERME_TRACE From LECFIC
#GTRACE = "QLFAC_USERPREFERENCES_"+GACTX.USER
Call LEC_TRACE From LECFIC
End

#------------------------------------------------------------------------------#
Funprog TESTSUITE
Call ADD_TESTCASE("TEST_USERPREFERENCES","Test context datas AINTL group",9) From AXUNIT
End func AXUNIT.RUN_TESTSUITE("REQ-85655","Format context for logs")

#------------------------------------------------------------------------------#
Subprog TEST_USERPREFERENCES

Call CHECK_EQUAL("twodigityear="+num$(adxdcs),"twodigityear="+num$(GACTX.AINTL.TWODIGITYEAR)) From AXUNIT
Call CHECK_EQUAL("shortdate="+ADXFMT2(0),"shortdate="+GACTX.AINTL.SHORTDATE) From AXUNIT
Call CHECK_EQUAL("longdate="+ADXFMT2(1),"longdate="+GACTX.AINTL.LONGDATE) From AXUNIT
Call CHECK_EQUAL("shorttime="+ADXFMT2(2),"shorttime="+GACTX.AINTL.SHORTTIME) From AXUNIT
Call CHECK_EQUAL("longtime="+ADXFMT2(3),"longtime="+GACTX.AINTL.LONGTIME) From AXUNIT
Call CHECK_EQUAL("shortdatetim="+ADXFMT2(4),"shortdatetim="+GACTX.AINTL.SHORTDATETIM) From AXUNIT
Call CHECK_EQUAL("longdatetim="+ADXFMT2(5),"longdatetim="+GACTX.AINTL.LONGDATETIM) From AXUNIT
Call CHECK_EQUAL("decimalsep="+ADXFMT2(6),"decimalsep="+GACTX.AINTL.DECIMALSEP) From AXUNIT
Call CHECK_EQUAL("thousandsep="+ADXFMT2(7),"thousandsep="+GACTX.AINTL.THOUSANDSEP) From AXUNIT

End
