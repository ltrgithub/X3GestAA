	// 		strictEqual(resource.REPCRY, "US", "resource.REPCRY");
	// 	});
	// 	start();
	// });

	asyncTest("AQCPROT1 - Decimal formats" + x3Helper.gitHubIssue(5923), _ => {
		var representation = new Representation("AQCPROT1");
		var query = representation.query(_, "&filter=noFilters");

		function bcdVal(val) {
			// X3 returns a number and etna a string 
			return typeof val === 'string' ? val : '"' + val + '"';
		}
		query.$resources.forEach(resource => {

			strictEqual(bcdVal(resource.VAL2), "123456789012345", "MD0 format");
			if (resource.FCY === 'ZUSA') {
				strictEqual(bcdVal(resource.VAL1), "123456.789", "MD1 format");
			} else {
				strictEqual(bcdVal(resource.VAL1), "1234567890123.12", "MD1 format");
			}
			strictEqual(bcdVal(resource.QTY1), "123456789012345", "QTY format");
			strictEqual(bcdVal(resource.COE1), "12345.123456", "COE format");
			strictEqual(bcdVal(resource.MD2), "123456.12", "MD2 format");
			strictEqual(bcdVal(resource.MD3), "1234567890123.12", "MD3 format");
			strictEqual(bcdVal(resource.MD4), "123456789.12", "MD4 format");
			strictEqual(bcdVal(resource.MD5), "12345678901.1234", "MD5 format");
			strictEqual(bcdVal(resource.RCU), "123456789012345", "RCU format");
		});
		start();
	});
}

