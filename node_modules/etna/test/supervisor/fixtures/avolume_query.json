{
  "$actxUser": "AQC",
  "$actxLogin": "aqcore",
  "$actxFolder": "SUPERV",
  "$actxSolution": "SUPDVLP",
  "$actxLan": "FRA",
  "$actxLanDef": "FRA",
  "$actxLanIso": "fr-FR",
  "$actxNbLeg": "undefined",
  "$baseType": "application/json;vnd.sage=syracuse;vnd.sage.syracuse.representation=x3.erp.SUPERV",
  "$type": "{$baseType}.AVOLUME.$query",
  "$representation": "AVOLUME",
  "$prototype": "{$baseUrl}/$prototype('{$representation}.$thumb')",
  "$device": "desktop",
  "$title": "Volume",
  "$collaborationUrl": "/sdata/syracuse/collaboration/syracuse",
  "$baseUrl": "/sdata/x3/erp/SUPERV",
  "$repUrl": "{$baseUrl}/AVOLUME",
  "$instanceUrl": "{$repUrl}/AVOLUME",
  "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$query",
  "$itemsPerPage": 300,
  "$properties": {
    "$resources": {
      "$type": "application/x-array",
      "$item": {
        "$url": "{$baseUrl}/AVOLUME('{$key}')?representation=AVOLUME.$queryItem",
        "$type": "application/json;vnd.sage=syracuse",
        "$key": "{VOLUME}",
        "$value": "{VOLUME}",
        "$title": "{DES}",
        "$description": "{DES}",
        "$properties": {
          "VOLUME": {
            "$title": "Volume",
            "$shortTitle": "Volume",
            "$isMandatory": true,
            "$capabilities": "sort,filter,filter_upper",
            "$type": "application/x-string",
            "$maxLength": 30,
            "$links": {
              "$details": {
                "$title": "Afficher",
                "$type": "application/json;vnd.sage=syracuse",
                "$target": "",
                "$url": "{$baseUrl}/AVOLUME('{VOLUME}')?representation=AVOLUME.$details"
              },
              "$lookup": {
                "$title": "Sélection",
                "$type": "application/json;vnd.sage=syracuse",
                "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$lookup"
              }
            }
          },
          "DES": {
            "$title": "Description",
            "$shortTitle": "Description",
            "$capabilities": "sort,filter,filter_upper",
            "$type": "application/x-string",
            "$maxLength": 30
          }
        },
        "$links": {
          "$delete": {
            "$title": "Supprimer",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$method": "DELETE",
            "$url": "{$baseUrl}/AVOLUME('{VOLUME}')?representation=AVOLUME.$query",
            "$confirm": "Voulez-vous supprimer l'enregistrement"
          },
          "$details": {
            "$title": "Afficher",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/AVOLUME('{VOLUME}')?representation=AVOLUME.$details"
          },
          "$edit": {
            "$title": "Modifier",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$method": "POST",
            "$url": "{$baseUrl}/AVOLUME('{VOLUME}')/$workingCopies?representation=AVOLUME.$edit"
          },
          "ABROWSE": {
            "$title": "Liste des fichiers",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/AVOLFIL?representation=AVOLFIL.$query&VOLUME={VOLUME}&where=(VOLUME eq '{VOLUME}')"
          }
        }
      }
    }
  },
  "$links": {
    "$create": {
      "$title": "Créer",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$method": "POST",
      "$url": "{$baseUrl}/AVOLUME/$template/$workingCopies?representation=AVOLUME.$edit"
    },
    "$excel": {
      "$title": "Excel",
      "$type": "application/syracuse-excel-worksheet",
      "$target": "",
      "$confirm": "Avez-vous installé le SyracuseAddins pour Office ?",
      "$officeAddinSetup": "/msoffice/lib/general/addIn/SyracuseOfficeAddinsSetup.EXE",
      "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$query&role={$role}&where={$where}&filter={$filter}&excelReportMode={excelReportMode}&doc_uuid={document}",
      "$parameters": {
        "$url": "{$collaborationUrl}/msoExcelReportModes/$template/$workingCopies?representation=msoExcelReportMode.$query&role={$role}&where={$where}&filter={$filter}&excelReportMode={excelReportMode}&doc_uuid={document}",
        "$method": "POST",
        "msoCurrentRepresentation": "{$representation}.$query",
        "$properties": {
          "dummy": {},
          "$title": "dummy",
          "$type": "application/x-string"
        }
      }
    },
    "$wordmailmerge": {
      "$title": "Publipostage",
      "$type": "application/syracuse-word-mailmerge",
      "$target": "",
      "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$query&role={$role}&where={$where}&filter={$filter}&createMode={creationMode}&doc_uuid={document}",
      "$parameters": {
        "$url": "{$collaborationUrl}/msoMailMergeDocSels/$template/$workingCopies?representation=msoMailMergeDocSel.$query&role={$role}&where={$where}&filter={$filter}",
        "$method": "POST",
        "msoCurrentRepresentation": "{$representation}.$query",
        "$properties": {
          "dummy": {},
          "$title": "dummy",
          "$type": "application/x-string"
        }
      }
    },
    "$wordreport": {
      "$title": "Word",
      "$type": "application/syracuse-word-report",
      "$target": "",
      "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$query&role={$role}&where={$where}&filter={$filter}&reportMode={reportMode}&doc_uuid={document}",
      "$parameters": {
        "$url": "{$collaborationUrl}/msoReportModes/$template/$workingCopies?representation=msoReportMode.$query&role={$role}&where={$where}&filter={$filter}&reportMode={reportMode}&doc_uuid={document}",
        "$method": "POST",
        "msoCurrentRepresentation": "{$representation}.$query",
        "$properties": {
          "dummy": {},
          "$title": "dummy",
          "$type": "application/x-string"
        }
      }
    }
  },
  "$article": {
    "$menus": {
      "$layout": {
        "$items": [
          {
            "$bind": "$create"
          },
          {
            "$bind": "$excel"
          },
          {
            "$bind": "$wordreport"
          },
          {
            "$bind": "$wordmailmerge"
          }
        ]
      }
    }
  }
}