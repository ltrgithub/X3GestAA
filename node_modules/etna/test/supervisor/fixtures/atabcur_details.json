{
  "$actxUser": "AQC",
  "$actxLogin": "aqcore",
  "$actxFolder": "SUPERV",
  "$actxSolution": "SUPDVLP",
  "$actxLan": "FRA",
  "$actxLanDef": "FRA",
  "$actxLanIso": "fr-FR",
  "$actxNbLeg": "undefined",
  "$baseType": "application/json;vnd.sage=syracuse;vnd.sage.syracuse.representation=x3.erp.SUPERV",
  "$type": "{$baseType}.ATABCUR.$details",
  "$representation": "ATABCUR",
  "$prototype": "{$baseUrl}/$prototype('{$representation}.$thumb')",
  "$device": "desktop",
  "$title": "Devise",
  "$collaborationUrl": "/sdata/syracuse/collaboration/syracuse",
  "$baseUrl": "/sdata/x3/erp/SUPERV",
  "$repUrl": "{$baseUrl}/ATABCUR",
  "$instanceUrl": "{$repUrl}/TCU",
  "$url": "{$baseUrl}/ATABCUR('{$key}')?representation=ATABCUR.$details",
  "$key": "{CUR}",
  "$value": "{CUR}",
  "$properties": {
    "CUR": {
      "$type": "application/x-string",
      "$maxLength": 3,
      "$isExcluded": true
    },
    "CUR_REF": {
      "$type": "application/x-reference",
      "$item": {
        "$url": "{$baseUrl}/TABCUR('{$key}')?representation=TABCUR.$lookup",
        "$properties": {
          "CUR": {
            "$type": "application/x-string"
          }
        },
        "$key": "{CUR}",
        "$value": "{CUR}",
        "CUR": "{CUR}",
        "$links": {
          "$details": {
            "$title": "Afficher",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/TABCUR('{CUR}')?representation=TABCUR.$details"
          },
          "$lookup": {
            "$title": "Sélection",
            "$type": "application/json;vnd.sage=syracuse",
            "$url": "{$baseUrl}/TABCUR?representation=TABCUR.$lookup"
          },
          "$query": {
            "$title": "Lister",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/TABCUR?representation=TABCUR.$query"
          },
          "$summary": {
            "$title": "Résumé",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/TABCUR('{CUR}')?representation=TABCUR.$summary"
          }
        }
      },
      "$title": "Devise",
      "$shortTitle": "Devise",
      "$capabilities": "sort,filter,filter_upper",
      "$isMandatory": true
    },
    "INTDES": {
      "$title": "Intitulé",
      "$shortTitle": "Intitulé",
      "$isMandatory": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 30
    },
    "INTSHO": {
      "$title": "Intitulé court",
      "$shortTitle": "Intitulé court",
      "$isMandatory": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 12
    },
    "CURSYM": {
      "$title": "Symbole monétaire",
      "$shortTitle": "Symbole monétaire",
      "$isMandatory": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 10
    },
    "ISOCOD": {
      "$title": "Code ISO",
      "$shortTitle": "Code ISO",
      "$isMandatory": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 3
    },
    "DECNBR": {
      "$title": "Nombre décimales",
      "$shortTitle": "Nombre décimales",
      "$isMandatory": true,
      "$capabilities": "sort,filter",
      "$type": "application/x-integer",
      "$maxLength": 1
    },
    "CURRND": {
      "$title": "Arrondi",
      "$shortTitle": "Arrondi",
      "$isMandatory": true,
      "$capabilities": "sort,filter",
      "$type": "application/x-decimal",
      "$maxLength": 5,
      "$precision": 5,
      "$scale": 5,
      "$format": "0.00000"
    },
    "EURFLG": {
      "$title": "Euro",
      "$shortTitle": "Euro",
      "$capabilities": "sort,filter",
      "$type": "application/x-boolean"
    },
    "EURRAT": {
      "$title": "Cours Euro",
      "$shortTitle": "Cours Euro",
      "$capabilities": "sort,filter",
      "$type": "application/x-decimal",
      "$maxLength": 11,
      "$precision": 11,
      "$scale": 5,
      "$format": "0.00000"
    },
    "EURDAT": {
      "$title": "Date passage à l'Euro",
      "$shortTitle": "Date passage à l'Euro",
      "$capabilities": "sort,filter",
      "$type": "application/x-date"
    },
    "CURENDDAT": {
      "$title": "Date disparition devise",
      "$shortTitle": "Date disparition devise",
      "$capabilities": "sort,filter",
      "$type": "application/x-date"
    },
    "TCUCOLTCH": {
      "$title": "Cours devise",
      "$shortTitle": "Cours devise",
      "$capabilities": "append,insert,delete,reorder",
      "$type": "application/x-array",
      "$item": {
        "$type": "application/json",
        "$properties": {
          "CHGTYP": {
            "$title": "Type cours",
            "$shortTitle": "Type cours",
            "$capabilities": "sort,filter",
            "$type": "application/x-choice",
            "$value": {
              "$type": "application/x-integer",
              "$enum": [
                {
                  "$value": 1,
                  "$title": "Cours du jour"
                },
                {
                  "$value": 2,
                  "$title": "Cours du mois"
                },
                {
                  "$value": 3,
                  "$title": "Cours moyen"
                },
                {
                  "$value": 4,
                  "$title": "Cours de DIS001"
                },
                {
                  "$value": 5,
                  "$title": "Cours CEE"
                }
              ]
            }
          }
        },
        "$instanceUrl": "{$instanceUrl}/COLTCH('{$uuid}')"
      }
    }
  },
  "$links": {
    "$create": {
      "$title": "Créer",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$method": "POST",
      "$url": "{$baseUrl}/ATABCUR/$template/$workingCopies?representation=ATABCUR.$edit"
    },
    "$query": {
      "$title": "Lister",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$url": "{$baseUrl}/ATABCUR?representation=ATABCUR.$query"
    },
    "$delete": {
      "$title": "Supprimer",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$method": "DELETE",
      "$url": "{$baseUrl}/ATABCUR('{CUR}')?representation=ATABCUR.$query",
      "$confirm": "Voulez-vous supprimer l'enregistrement"
    },
    "$edit": {
      "$title": "Modifier",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$method": "POST",
      "$url": "{$baseUrl}/ATABCUR('{CUR}')/$workingCopies?representation=ATABCUR.$edit"
    }
  },
  "$article": {
    "$layout": {
      "$items": [
        {
          "$category": "section",
          "$title": "Entête",
          "$layout": {
            "$items": [
              {
                "$category": "block",
                "$title": "Entête",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "CUR_REF"
                    },
                    {
                      "$bind": "INTDES"
                    },
                    {
                      "$bind": "INTSHO"
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "$category": "section",
          "$title": "Général",
          "$layout": {
            "$items": [
              {
                "$category": "block",
                "$title": "Général",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "CURSYM"
                    },
                    {
                      "$bind": "ISOCOD"
                    }
                  ]
                }
              },
              {
                "$category": "block",
                "$title": "Détail",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "DECNBR"
                    },
                    {
                      "$bind": "CURRND"
                    }
                  ]
                }
              },
              {
                "$category": "block",
                "$title": "Euro",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "EURFLG"
                    },
                    {
                      "$bind": "EURRAT"
                    },
                    {
                      "$bind": "EURDAT"
                    },
                    {
                      "$bind": "CURENDDAT"
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "$category": "section",
          "$title": "Cours devise",
          "$layout": {
            "$items": [
              {
                "$category": "block",
                "$title": "Cours",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "TCUCOLTCH"
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "$menus": {
      "$layout": {
        "$items": [
          {
            "$bind": "$create"
          },
          {
            "$bind": "$edit",
            "$kind": "main"
          },
          {
            "$bind": "$delete"
          },
          {
            "$bind": "$query"
          }
        ]
      }
    }
  }
}