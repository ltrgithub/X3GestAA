{
  "$actxUser": "AQC",
  "$actxLogin": "aqcore",
  "$actxFolder": "SUPERV",
  "$actxSolution": "SUPDVLP",
  "$actxLan": "FRA",
  "$actxLanDef": "FRA",
  "$actxLanIso": "fr-FR",
  "$actxNbLeg": "undefined",
  "$baseType": "application/json;vnd.sage=syracuse;vnd.sage.syracuse.representation=x3.erp.SUPERV",
  "$type": "{$baseType}.ATRACE.$details",
  "$representation": "ATRACE",
  "$prototype": "{$baseUrl}/$prototype('{$representation}.$thumb')",
  "$device": "desktop",
  "$title": "Fichier de trace",
  "$collaborationUrl": "/sdata/syracuse/collaboration/syracuse",
  "$baseUrl": "/sdata/x3/erp/SUPERV",
  "$repUrl": "{$baseUrl}/ATRACE",
  "$instanceUrl": "{$repUrl}/ATRACE",
  "$url": "{$baseUrl}/ATRACE('{$key}')?representation=ATRACE.$details",
  "$key": "{NAME}",
  "$value": "{NAME}",
  "$properties": {
    "NAME": {
      "$title": "Nom",
      "$shortTitle": "Nom",
      "$isReadOnly": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 30,
      "$links": {
        "$details": {
          "$title": "Afficher",
          "$type": "application/json;vnd.sage=syracuse",
          "$target": "",
          "$url": "{$baseUrl}/ATRACE('{NAME}')?representation=ATRACE.$details"
        }
      }
    },
    "COMMENT": {
      "$title": "Commentaire",
      "$shortTitle": "Commentaire",
      "$isReadOnly": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 80
    },
    "SIZE": {
      "$title": "Taille",
      "$shortTitle": "Taille",
      "$isReadOnly": true,
      "$capabilities": "sort,filter",
      "$type": "application/x-integer",
      "$maxLength": 8
    },
    "TIMESTAMP": {
      "$title": "Timestamp",
      "$shortTitle": "Timestamp",
      "$isReadOnly": true,
      "$capabilities": "sort,filter",
      "$type": "application/x-datetime"
    },
    "TRACEFILE": {
      "$title": "Fichier de trace",
      "$shortTitle": "Fichier de trace",
      "$isReadOnly": true,
      "$type": "application/x-document",
      "$url": "{$baseUrl}/CDOC('R_ATRACE~%C2%A8~TRACEFILE~%C2%A8~TRA~%C2%A8~{NAME}')"
    },
    "VOLUME": {
      "$type": "application/x-string",
      "$maxLength": 30,
      "$isExcluded": true
    },
    "VOLUME_REF": {
      "$type": "application/x-reference",
      "$item": {
        "$url": "{$baseUrl}/AVOLUME('{$key}')?representation=AVOLUME.$lookup",
        "$properties": {
          "VOLUME": {
            "$type": "application/x-string"
          }
        },
        "$key": "{VOLUME}",
        "$value": "{VOLUME}",
        "VOLUME": "{VOLUME}",
        "$links": {
          "$details": {
            "$title": "Afficher",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/AVOLUME('{VOLUME}')?representation=AVOLUME.$details"
          },
          "$lookup": {
            "$title": "Sélection",
            "$type": "application/json;vnd.sage=syracuse",
            "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$lookup"
          },
          "$query": {
            "$title": "Lister",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$query"
          }
        }
      },
      "$title": "Volume",
      "$shortTitle": "Volume",
      "$capabilities": "sort,filter,filter_upper",
      "$isReadOnly": true,
      "$isHidden": true
    },
    "EXTEND": {
      "$title": "Extension",
      "$shortTitle": "Extension",
      "$isReadOnly": true,
      "$isHidden": true,
      "$capabilities": "sort,filter,filter_upper",
      "$type": "application/x-string",
      "$maxLength": 10
    }
  },
  "$links": {
    "$query": {
      "$title": "Lister",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$url": "{$baseUrl}/ATRACE?representation=ATRACE.$query"
    },
    "$delete": {
      "$title": "Supprimer",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$method": "DELETE",
      "$url": "{$baseUrl}/ATRACE('{NAME}')?representation=ATRACE.$query",
      "$confirm": "Voulez-vous supprimer l'enregistrement"
    }
  },
  "$article": {
    "$layout": {
      "$items": [
        {
          "$category": "section",
          "$title": "Général",
          "$layout": {
            "$items": [
              {
                "$category": "block",
                "$title": "Entête",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "NAME"
                    },
                    {
                      "$bind": "COMMENT"
                    },
                    {
                      "$bind": "SIZE"
                    },
                    {
                      "$bind": "TIMESTAMP"
                    },
                    {
                      "$bind": "TRACEFILE"
                    },
                    {
                      "$bind": "VOLUME_REF"
                    },
                    {
                      "$bind": "EXTEND"
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "$menus": {
      "$layout": {
        "$items": [
          {
            "$bind": "$delete",
            "$kind": "main"
          },
          {
            "$bind": "$query"
          }
        ]
      }
    }
  }
}