                },
                {
                  "$value": 2,
                  "$title": "Surface"
                },
                {
                  "$value": 3,
                  "$title": "Volume"
                },
                {
                  "$value": 4,
                  "$title": "Masse/poids"
                },
                {
                  "$value": 5,
                  "$title": "Temps"
                },
                {
                  "$value": 6,
