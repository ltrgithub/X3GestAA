        "$value": "{CODACC}",
        "CODACC": "{CODACC}",
        "$links": {
          "$details": {
            "$title": "Afficher",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/ACCCOD('{CODACC}')?representation=ACCCOD.$details"
          },
          "$lookup": {
            "$title": "Sélection",
            "$type": "application/json;vnd.sage=syracuse",
            "$url": "{$baseUrl}/ACCCOD?representation=ACCCOD.$lookup"
          },
          "$query": {
            "$title": "Lister",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/ACCCOD?representation=ACCCOD.$query"
          },
          "$summary": {
            "$title": "Résumé",
            "$type": "application/json;vnd.sage=syracuse",
            "$target": "",
            "$url": "{$baseUrl}/ACCCOD('{CODACC}')?representation=ACCCOD.$summary"
          }
        }
      },
      "$title": "Code d'accès",
      "$shortTitle": "Code d'accès",
      "$capabilities": "sort,filter,filter_upper"
    }
  },
  "$links": {
    "$create": {
      "$title": "Créer",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$method": "POST",
      "$url": "{$baseUrl}/AVOLUME/$template/$workingCopies?representation=AVOLUME.$edit"
    },
    "$query": {
      "$title": "Lister",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$url": "{$baseUrl}/AVOLUME?representation=AVOLUME.$query"
    },
    "$delete": {
      "$title": "Supprimer",
      "$type": "application/json;vnd.sage=syracuse",
