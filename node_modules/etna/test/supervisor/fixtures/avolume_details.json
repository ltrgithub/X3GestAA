        "msoCurrentRepresentation": "{$representation}.$details",
        "$properties": {
          "dummy": {},
          "$title": "dummy",
          "$type": "application/x-string"
        }
      }
    },
    "$wordreport": {
      "$title": "Word",
      "$type": "application/syracuse-word-report",
      "$target": "",
      "$url": "{$baseUrl}/AVOLUME('{VOLUME}')?representation=AVOLUME.$details&role={$role}&where={$where}&filter={$filter}&reportMode={reportMode}&doc_uuid={document}",
      "$parameters": {
        "$method": "POST",
        "msoCurrentRepresentation": "{$representation}.$details",
        "$url": "{$collaborationUrl}/msoReportModes/$template/$workingCopies?representation=msoReportMode.$details&role={$role}&where={$where}&filter={$filter}&reportMode={reportMode}&doc_uuid={document}",
        "$properties": {
          "dummy": {},
          "$title": "dummy",
          "$type": "application/x-string"
        }
      }
    },
    "ABROWSE_LIG44444": {
      "$title": "Liste des fichiers",
      "$type": "application/json;vnd.sage=syracuse",
      "$target": "",
      "$url": "{$baseUrl}/AVOLFIL?representation=AVOLFIL.$query&VOLUME={VOLUME}&where=(VOLUME eq '{VOLUME}')"
    }
  },
  "$article": {
    "$layout": {
      "$items": [
        {
          "$category": "section",
          "$title": "Général",
          "$layout": {
            "$items": [
              {
                "$category": "block",
                "$title": "Entête",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "VOLUME"
                    },
                    {
                      "$bind": "DES"
                    },
                    {
                      "$bind": "ROOT"
                    },
                    {
                      "$bind": "CODACC_REF"
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "$menus": {
      "$layout": {
        "$items": [
          {
            "$bind": "ABROWSE_LIG44444"
          },
          {
            "$bind": "$create"
          },
          {
            "$bind": "$edit",
            "$kind": "main"
          },
          {
            "$bind": "$delete"
          },
          {
            "$bind": "$query"
          },
          {
            "$bind": "$excel"
          },
          {
            "$bind": "$wordreport"
          },
          {
            "$bind": "$wordmailmerge"
          }
        ]
      }
    }
