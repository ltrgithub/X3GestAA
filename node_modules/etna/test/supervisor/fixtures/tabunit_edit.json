{
  "$actxUser": "AQC",
  "$actxLogin": "aqcore",
  "$actxFolder": "SUPERV",
  "$actxSolution": "SUPDVLP",
  "$actxLan": "FRA",
  "$actxLanDef": "FRA",
  "$actxLanIso": "fr-FR",
  "$actxNbLeg": "undefined",
  "$baseType": "application/json;vnd.sage=syracuse;vnd.sage.syracuse.representation=x3.erp.SUPERV",
  "$type": "{$baseType}.TABUNIT.$edit",
  "$representation": "TABUNIT",
  "$prototype": "{$baseUrl}/$prototype('{$representation}.$thumb')",
  "$device": "desktop",
  "$title": "Table des unités",
  "$collaborationUrl": "/sdata/syracuse/collaboration/syracuse",
  "$baseUrl": "/sdata/x3/erp/SUPERV",
  "$repUrl": "{$baseUrl}/$workingCopies('{$trackingId}')",
  "$instanceUrl": "{$repUrl}/TUN",
  "$x3FormatExt": {
    "c": {
      "stdType": [
        "-",
        "/",
        "_",
        ","
      ],
      "chars": [
        "A",
        "#"
      ]
    },
    "d": {
      "stdType": [
        "*",
        "$"
      ],
      "chars": [
        "c"
      ]
    },
    "u": {
      "stdType": [
        "_"
      ],
      "chars": [
        "B"
      ]
    },
    "g": {
      "stdType": [],
      "chars": [
        "A",
        "X"
      ]
    },
    "v": {
      "stdType": [
        "-"
      ],
      "chars": [
        "u"
      ]
    }
  },
  "$url": "{$baseUrl}/TABUNIT('{$key}')?representation=TABUNIT.$edit",
  "$key": "{UOM}",
  "$value": "{UOM}",
  "$properties": {
    "UOM": {
      "$title": "Unité",
      "$shortTitle": "Unité",
      "$isMandatory": true,
      "$x3Format": "K:3X",
      "$capabilities": "sort",
      "$type": "application/x-string",
      "$maxLength": 3
    },
    "DES": {
      "$title": "Intitulé",
      "$shortTitle": "Intitulé",
      "$isMandatory": true,
      "$x3Format": "K:30X",
      "$capabilities": "sort,filter,filter_upper,localize",
      "$type": "application/x-string",
      "$maxLength": 30,
      "$links": {
        "$localize": {
          "$type": "application/json;vnd.sage=syracuse",
          "$title": "Traduction",
          "$url": "{$instanceUrl}/DES/$localize"
        }
      }
    },
    "UOMDEC": {
      "$title": "Nombre décimales",
      "$shortTitle": "Nombre décimales",
      "$capabilities": "sort",
      "$type": "application/x-integer",
      "$maxLength": 1
    },
    "UOMTYP": {
      "$title": "Type unité",
      "$shortTitle": "Type unité",
      "$capabilities": "sort",
      "$type": "application/x-choice",
      "$value": {
        "$type": "application/x-integer",
        "$enum": [
          {
            "$value": 1,
            "$title": "Longueur "
          },
          {
            "$value": 2,
            "$title": "Surface"
          },
          {
            "$value": 3,
            "$title": "Volume"
          },
          {
            "$value": 4,
            "$title": "Masse/poids"
          },
          {
            "$value": 5,
            "$title": "Temps"
          },
          {
            "$value": 6,
            "$title": "Numération"
          },
          {
            "$value": 7,
            "$title": "Conditionnement"
          },
          {
            "$value": 8,
            "$title": "Autres"
          }
        ]
      }
    },
    "UOMSYM": {
      "$title": "Symbole",
      "$shortTitle": "Symbole",
      "$capabilities": "sort",
      "$type": "application/x-string",
      "$maxLength": 5
    },
    "DESSHO": {
      "$title": "Intitulé court",
      "$shortTitle": "Intitulé court",
      "$x3Format": "K:12X",
      "$capabilities": "sort,filter,filter_upper,localize",
      "$type": "application/x-string",
      "$maxLength": 12,
      "$links": {
        "$localize": {
          "$type": "application/json;vnd.sage=syracuse",
          "$title": "Traduction",
          "$url": "{$instanceUrl}/DESSHO/$localize"
        }
      }
    }
  },
  "$actions": {
    "$save": {
      "$title": "Enregistrer",
      "$kind": "main",
      "$isHidden": true,
      "$isDisabled": false,
      "$links": {
        "$redirect": {
          "$title": "Afficher",
          "$type": "application/json;vnd.sage=syracuse",
          "$url": "{$baseUrl}/TABUNIT('{UOM}')?representation=TABUNIT.$details"
        }
      }
    }
  },
  "$article": {
    "$layout": {
      "$items": [
        {
          "$category": "section",
          "$title": "Unités",
          "$layout": {
            "$items": [
              {
                "$category": "block",
                "$title": "Unités",
                "$layout": {
                  "$items": [
                    {
                      "$bind": "UOM"
                    },
                    {
                      "$bind": "DES"
                    },
                    {
                      "$bind": "UOMDEC"
                    },
                    {
                      "$bind": "UOMTYP"
                    },
                    {
                      "$bind": "UOMSYM"
                    },
                    {
                      "$bind": "DESSHO"
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "$menus": {
      "$layout": {
        "$items": [
          {
            "$bind": "$save",
            "$kind": "main"
          }
        ]
      }
    }
  }
}