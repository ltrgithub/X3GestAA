# ---------- TEST_SUITE vtb function       -------------------------
# ------------------------------------------------------------------

Funprog TESTSUITE()
    Call ADD_TESTCASE("FROMVAR", "Test base", 1) From AXUNIT
End func AXUNIT.RUN_TESTSUITE("Virtual table", "Common")

# -----------------------TEST_WR_READ -------------------------------------
# #########################################################################

Subprog FROMVAR( )

Local Integer TA(1..5)
Local Char TB(25)(1..5)


Local Integer I,J

For I = 1 To 5
    TA(I) = 1
    TB(I) = "STR"+num$(I)
Next

Local File    (
&   Integer A,
&   Char    B(25))
&             From Variable
&   TA(1..3),TB(1..3) As [ANB]


For [ANB]
    Infbox num$([F:ANB]A)-[F:ANB]B
Next

Close Local File [ANB]

Call CHECK_EQUAL( "ok", "ok") From AXUNIT
End

