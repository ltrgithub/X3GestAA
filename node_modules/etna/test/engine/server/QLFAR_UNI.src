TOTO = func TESTSUITE()
Infbox TOTO

End

# ---------- TEST_SUITE uni function       -------------------------
# ------------------------------------------------------------------

Funprog TESTSUITE()

    Call ADD_TESTCASE("TEST_UNI", "uni tests", 9) From AXUNIT

End func AXUNIT.RUN_TESTSUITE("uni", "Common")

# -----------------------TEST_UNI -------------------------------------
# #####################################################################
Subprog TEST_UNI( )

    Local Integer ST
    Local Integer MEM

    ST = uni( 1, 2, 3 ,4 )
    Call CHECK_EQUAL( ST, 0 ) From AXUNIT

    ST = uni( 1, 2, 1 ,4 )
    Call CHECK_EQUAL( ST, 3 ) From AXUNIT

    ST = uni( 1, 2, 3 ,2 )
    Call CHECK_EQUAL( ST, 4 ) From AXUNIT


    MEM = freemem
    ST = uni( "toto", "tata", "titi" , "tutu" )
    Call CHECK_EQUAL( ST, 0 ) From AXUNIT
    ST = freemem
    Call CHECK_EQUAL( ST, MEM ) From AXUNIT

    MEM = freemem
    ST = uni( "toto", "tata", "toto" , "tutu" )
    Call CHECK_EQUAL( ST, 3 ) From AXUNIT
    ST = freemem
    Call CHECK_EQUAL( ST, MEM ) From AXUNIT

    MEM = freemem
    ST = uni( "toto", "tata", "titi" , "tata" )
    Call CHECK_EQUAL( ST, 4 ) From AXUNIT
    ST = freemem
    Call CHECK_EQUAL( ST, MEM ) From AXUNIT


End
