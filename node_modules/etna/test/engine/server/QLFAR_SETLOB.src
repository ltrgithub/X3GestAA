Local Clbfile RESULT
RESULT=func TESTSUITE()
Call FERME_TRACE From LECFIC
GTRACE = "QLFAR_SETLOB_"+GACTX.USER
Call LEC_TRACE From LECFIC
End

#**
#* The main entry point of the unit test suite for Requirement 69072
#*

Funprog TESTSUITE()
Call ADD_TESTCASE("TC_BASIC", "TC_BASIC", 5) From AXUNIT
End func AXUNIT.RUN_TESTSUITE("QLFAR_SETLOB", "QLFAR_SETLOB")

Subprog TC_BASIC
Local Clbfile MY_CLOB1(1)
Local Char ARRAY1(1)(0..)

Setlob MY_CLOB1 With "this is a CLOB"
Call CHECK_EQUAL(MY_CLOB1,"this is a CLOB") From AXUNIT
Setlob ARRAY1 With MY_CLOB1
Call CHECK_EQUAL(ARRAY1(0),"t") From AXUNIT
Call CHECK_EQUAL(ARRAY1(1),"h") From AXUNIT
Call CHECK_EQUAL(ARRAY1(2),"i") From AXUNIT
Call CHECK_EQUAL(ARRAY1(3),"s") From AXUNIT

End