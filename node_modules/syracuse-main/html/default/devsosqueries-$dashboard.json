{
    "$prototype": {
        "$isDefaultDashboard": true,
        "$title": "Welcome",
        "$properties": {
            "sage": {
                "$type": "application/x-vignette",
                "$title": "Sage",
                "$location": {
                    "$type": "html",
                    "$url": "http://www.sage.fr/"
                }
            },
            "countries": {
                "$type": "application/x-vignette",
                "$title": "Countries",
                "$location": {
                    "$url": "/sdata/sprint1/settings/sprint1/countries?representation=country.$query.portlet"
                }
            },
            "companies": {
                "$type": "application/x-vignette",
                "$title": "Companies",
                "$location": {
                    "$url": "/sdata/sprint1/settings/sprint1/companies?representation=company.$query"
                }
            },
            "stubFGRPGraphTAB": {
                "$type": "application/x-vignette",
                "$title": "Stub FGRPGraphTAB-$query",
                "$location": {
                    "$url": "/sdata/x3stb/erp/stubs/FGRPGraphTAB?representation=FGRPGraphTAB.$query"
                }
            },
            "x3SaleStats": {
                "$type": "application/x-vignette",
                "$title": "Sales statistics",
                "$location": {
                    "$url": "/sdata/x3/erp/SUPERV/STATS('DOM')?representation=STATS~DOM.$cube"
                }
            }
        }
    },
    "$article": {
        "$category": "dashboard",
        "$layout": {
            "$layoutType": "tabs",
            "$items": [
                               
                {
                    "$category": "section",
                    "$isContentLoaded": true,
                    "$title": "Sprint",
                    "$layout": {
                        "$items": [                            
                            {
                                "$layoutType": "row",
                                "$items": [
                                    {
                                        "$bind": "countries"
                                    },
                                    {
                                        "$bind": "companies"
                                    }
                                ]
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Flash Demo",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "stubFGRPGraphTAB"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Statistics",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "x3SaleStats"
                            }
                        ]
                    }
                },                
                {
                    "$category": "section",
                    "$title": "HTML pages",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "sage"
                            }
                        ]
                    }
                }
            ]
        }
    }
}