{
    "$prototype": {
        "$isDefaultDashboard": true,
        "$title": "Welcome",
        "$properties": {
            "sage": {
                "$type": "application/x-vignette",
                "$title": "Sage",
                "$location": {
                    "$type": "html",
                    "$url": "http://www.sage.fr/"
                }
            },
            "countries": {
                "$type": "application/x-vignette",
                "$title": "Countries",
                "$location": {
                    "$url": "/sdata/sprint1/settings/sprint1/countries?representation=country.$query.portlet"
                }
            },
            "companies": {
                "$type": "application/x-vignette",
                "$title": "Companies",
                "$location": {
                    "$url": "/sdata/sprint1/settings/sprint1/companies?representation=company.$query"
                }
            },
            "stubFGRPGraphTAB": {
                "$type": "application/x-vignette",
                "$title": "Stub FGRPGraphTAB-$query",
                "$location": {
                    "$url": "/sdata/x3stb/erp/stubs/FGRPGraphTAB?representation=FGRPGraphTAB.$query"
                }
            },
            "x3SaleStats": {
                "$type": "application/x-vignette",
                "$title": "Sales statistics",
                "$location": {
                    "$url": "/sdata/x3/erp/SUPERV/STATS('DOM')?representation=STATS~DOM.$cube"
                }
            }
        },
		"$links":{
                      "home": {
                    "$title": "Home",
                    "$url": "?representation=home.$dashboard"
                },
                "devsos": {
                    "$title": "Developer samples/SOS",
                    "$url": "?representation=devsos.$dashboard"
                },
                "devsosqueries": {
                    "$title": "Developer queries/SOS",
                    "$url": "?representation=devsosqueries.$dashboard"
                },
				"officesos": {
                "$title": "Office samples/SOS",
                "$url": "?representation=office.$dashboard"
                },
                "quickAdmin": {
                    "$title": "Quick Administration",
                    "$url": "?representation=quickAdmin.$dashboard"
                },
                "office": {
                    "$title": "Office",
                    "$url": "/msoffice/lib/excel/html/config.html?url=%3Frepresentation%3Dexcelconfig.%24dashboard"
                },
                "mobileHome": {
                    "$title": "Mobile Home",
                    "$description": "Mobile application home page",
                    "$url": "/syracuse-mobile/html/mobile.html#start"
                },
                "mobileDesktopAuthoring": {
                    "$title": "Mobile Desktop Personalization",
                    "$description": "Mobile personalization with desktop client",
                    "$url": "/syracuse-main/html/main-mobile.html?url=%3Frepresentation%3Dhome.%24dashboard"
                },
                "uitest": {
                    "$title": "UI test Pages",
                    "$url": "?representation=s-uitest-gridArray.$test"
                },
                "perfmon": {
                    "$title": "Performance data",
                    "$url": "/syracuse-perfmon/html/render.html?dummy",
                    "$type": "text/html",
                    "$target": "blank"
                },
                "changeLog": {
                    "$title": "Changelog",
                    "$url": "/changelog.html",
                    "$type": "text/html",
                    "$target": "blank"
                }}
    },
    "$article": {
        "$category": "dashboard",
        "$layout": {
            "$layoutType": "tabs",
            "$items": [
                               
                {
                    "$category": "section",
                    "$title": "Sprint",
                    "$layout": {
                        "$items": [                            
                            {
                                "$layoutType": "row",
                                "$items": [
                                    {
                                        "$bind": "countries"
                                    },
                                    {
                                        "$bind": "companies"
                                    }
                                ]
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Flash Demo",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "stubFGRPGraphTAB"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Statistics",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "x3SaleStats"
                            }
                        ]
                    }
                },                
                {
                    "$category": "section",
                    "$title": "HTML pages",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "sage"
                            }
                        ]
                    }
                }
            ]
        }
    }
}