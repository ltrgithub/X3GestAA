
{
    "$prototype": {
        "$title": "sage ERP X3",
        "$": {
            "sprint": {
                "$type": "application/x-portlet",
                "$format": "$menu",
                "$title": "Sprint",
                "$links": {
                    "sprint": {
                        "$title": "Sprint",
                        "$icon": "sprint",
                        "$url": "?representation=mobileSprint.$dashboard"
                    },
					"erp":{
						"$title": "ERP",
						"$icon": "erp",
						"$url": "?representation=mobileErp.$dashboard"
					},
                    "syracuseAdmin": {
                        "$title": "Syracuse Admin",
                        "$icon": "admin",
                        "$url": "?representation=mobileSyracuseAdmin.$dashboard"
                    },
					"userProfile": {
                        "$title": "User profile test",
                        "$icon": "role",
                        "$url": "/sdata/syracuse/collaboration/syracuse/userProfiles/$template/$workingCopies?representation=userProfileMob.$edit",
                        "$method": "POST"
                    },
					"uiTests":{
						"$title": "UI Mobile Tests",
						"$icon": "test",
						"$url": "?representation=mobileUiTests.$dashboard"
					}
                }
            }
        }
    },
    "$article": {
        "$category": "dashboard",
        "$layout": {
            "$layoutType": "tabs",
            "$items": [
                {
                    "$category": "section",
                    "$title": "Icons Dashboard",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "sprint",
								"$isTitleHidden": true
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "List Dashboard",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "sprint",
                                "$menusFormat": "list",
								"$isTitleHidden": true
                            }
                        ]
                    }
                }
            ]
        }
    }
}