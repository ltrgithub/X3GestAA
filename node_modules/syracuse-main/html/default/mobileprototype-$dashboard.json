{
    "$prototype": {
        "$title": "sage ERP X3",
        "$": {
            "sprint": {
                "$type": "application/x-portlet",
                "$format": "$menu",
                "$title": "Sprint",
                "$links": {
                    "users": {
                        "$title": "Users",
                        "$icon": "user",
                        "$url": "/sdata/syracuse/collaboration/syracuse/users?representation=userMob.$query"
                    },
					"usersX3":{
						"$title": "Users X3",
						"$icon": "user",
						"$url": "/sdata/x3/erp/SUPERV/AUTILISM?representation=AUTILISM.$query"
					},
                    "country": {
                        "$title": "Countries",
                        "$target": "_self",
                        "$icon": "country",
                        "$url": "/sdata/sprint1/settings/sprint1/countries?representation=countryMob.$query"
                    },
                    "employee": {
                        "$title": "Employees",
                        "$target": "_self",
                        "$icon": "employee",
                        "$description": "Grid layout: default layout",
                        "$url": "/sdata/sprint1/settings/sprint1/employees?representation=employeeMob.$query"
                    },
                    "groups": {
                        "$title": "Groups",
                        "$icon": "group",
                        "$url": "/sdata/syracuse/collaboration/syracuse/groups?representation=groupMob.$query"
                    },
                    "company": {
                        "$title": "Companies",
                        "$description": "List of companies",
                        "$target": "_self",
                        "$icon": "company",
                        "$url": "/sdata/sprint1/settings/sprint1/companies?representation=companyMob.$query"
                    },
					"companyX3": {
                        "$title": "Companies X3",
                        "$description": "List of companies",
                        "$target": "_self",
                        "$icon": "company",
                        "$url": "/sdata/x3/erp/SUPERV/COMPANYM?representation=COMPANYM.$query"
                    },
                    "userProfile": {
                        "$title": "User profile test",
                        "$icon": "role",
                        "$url": "/sdata/syracuse/collaboration/syracuse/userProfiles/$template/$workingCopies?representation=userProfileMob.$edit",
                        "$method": "POST"
                    },
					"uiTests":{
						"$title": "UI Mobile Tests",
						"$icon": "test",
						"$url": "?representation=uiMobile.$dashboard"
					}
                }
            }
        }
    },
    "$article": {
        "$category": "dashboard",
        "$layout": {
            "$layoutType": "tabs",
            "$items": [
                {
                    "$category": "sectionDashboard",
                    "$title": "Icons Dashboard",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "sprint",
								"$isTitleHidden": true
                            }
                        ]
                    }
                },
                {
                    "$category": "sectionDashboard",
                    "$title": "List Dashboard",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "sprint",
                                "$menusFormat": "list",
								"$isTitleHidden": true
                            }
                        ]
                    }
                }
            ]
        }
    }
}