{
	"$prototype":{
		"$title": "",
		"$":{
			"actions": {
				"$type":"application/x-portlet",
				"$title": "Actions",
				"$format": "$menu",
				"$location": {
					"$url": "excel://worksheetActions?representation=worksheetAction.$query"
				}
			},
			"datasource": {
				"$type": "application/x-portlet",
				"$title": "Datasource",
				"$location": {
					"$url": "excel://excelDatasources('{$currentDatasource}')?representation=excelDatasource.$details"	
				}
			},
			"collaboration":{
				"$type":"application/x-portlet",
				"$format":"$menu",
				"$title":"Collaboration",
				"$links":{
					"users":{
						"$title":"Users",
						"$target":"_self",
						"$description":"Users",
						"$url":"/sdata/syracuse/collaboration/syracuse/users#?representation=user.$query"
					},
					"wsUsers":{
						"$title":"Users (to worksheet)",
						"$target":"worksheet",
						"$description":"Users",
						"$url":"/sdata/syracuse/collaboration/syracuse/users#?representation=user.$query"
					},
					"wsTags":{
						"$title":"Tags (to worksheet)",
						"$target":"worksheet",
						"$description":"Tags",
						"$url":"/sdata/syracuse/collaboration/syracuse/documentTags#?representation=documentTag.$query"
					},
					"ws1000lines":{
						"$title":"1000 lines to worksheet",
						"$target":"worksheet",
						"$description":"1000 lines",
						"$url":"/sdata/x3stb/erp/stubs/Request1000lines#?representation=Request1000line.$query"
					},
					"atable": {
						"$title": "Dictionnaire to ws",
						"$target":"worksheet",
						"$url":"/sdata/x3/erp/SUPERV/ATABLE?representation=ATABLE.$query"
					},
					"roles":{
						"$title":"Roles",
						"$target":"_self",
						"$url":"/sdata/syracuse/collaboration/syracuse/roles#?representation=role.$query"
					}
				}
			}
		}
	},
	"$article":{
		"$category":"dashboard",
		"$layout":{
			"$items":[{
				"$bind": "actions"
			}, {
				"$bind": "datasource"
			}
			]
		}
	}
}