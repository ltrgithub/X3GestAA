{
    "$prototype": {
        "$title": "UI tests",
        "$": {
            "diagnosesbox": {
                "$type": "application/x-portlet",
                "$title": "Diagnoses",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "diagnoses"
                }
            },
            "messagebox": {
                "$type": "application/x-portlet",
                "$title": "Message box",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "messageBox"
                }
            },
            "simpleFields": {
                "$type": "application/x-portlet",
                "$title": "Simple Fields",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "simpleFields"
                }
            },
            "collectionFields": {
                "$type": "application/x-portlet",
                "$title": "Collection",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "collectionFields"
                }
            },
			"referenceFields": {
                "$type": "application/x-portlet",
                "$title": "Reference",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "referenceFields"
                }
            }, 
			"arrayFields": {
                "$type": "application/x-portlet",
                "$title": "Array",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "arrayFields"
                }
            },
			"portlets": {
                "$type": "application/x-portlet",
                "$title": "Portlets",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "collectionFields"
                }
            },
            "layouts": {
                "$type": "application/x-portlet",
                "$title": "Layouts",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "layouts"
                }
            }
        }
    },
    "$article": {
        "$category": "dashboard",
        "$layout": {
            "$layoutType": "tabs",
            "$items": [{
                    "$category": "section",
                    "$title": "Simple Fields",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "simpleFields"
                            }
                        ]
                    }
                },{
                    "$category": "section",
                    "$title": "Collection",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "collectionFields"
                            }
                        ]
                    }
                },
				{
                    "$category": "section",
                    "$title": "Array",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "arrayFields"
                            }
                        ]
                    }
                },
				{
                    "$category": "section",
                    "$title": "Reference",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "referenceFields"
                            }
                        ]
                    }
                },		
				
                {
                    "$category": "section",
                    "$title": "Layouts",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "layouts"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Portlets",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "portlets"
                            }
                        ]
                    }
                },
                
                {
                    "$category": "section",
                    "$title": "Popup",
                    "$layout": {
                        "$layoutType": "columns",
                        "$layoutSubType": "40,60",
                        "$items": [
                            {
                                "$layoutType": "stack",
                                "$items": [
                                    {
                                        "$bind": "diagnosesbox"
                                    }
                                ]
                            },
                            {
                                "$layoutType": "stack",
                                "$items": [
                                    {
                                        "$bind": "messagebox"
                                    }
                                ]
                            }
                        ]
                    }
                }
            ]
        }
    }
}