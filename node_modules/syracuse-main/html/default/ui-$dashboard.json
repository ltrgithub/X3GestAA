{
    "$prototype": {
        "$title": "UI tests",
        "$properties": {
            "diagnosesbox": {
                "$type": "application/x-vignette",
                "$title": "Diagnoses",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "diagnoses"
                }
            },
            "messagebox": {
                "$type": "application/x-vignette",
                "$title": "Message box",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "messageBox"
                }
            },
            "childFieldTest": {
                "$type": "application/x-vignette",
                "$title": "Child Object Fields",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "childFieldTest"
                }
            },
            "simpleFields": {
                "$type": "application/x-vignette",
                "$title": "Simple Fields",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "simpleFields"
                }
            },
            "iconFields": {
                "$type": "application/x-vignette",
                "$title": "Icon Fields",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "iconFields"
                }
            },
            "layouts": {
                "$type": "application/x-vignette",
                "$title": "Layouts",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "layouts"
                }
            },
			"messageboxfk": {
                "$type": "application/x-vignette",
                "$title": "Message box fk",
                "$format": "$item",
                "$item": {
                    "$category": "tests",
                    "$format": "messageBoxFk"
                }
            }
        },
        "$links": {
            "uioverview": {
                "$title": "UI Overview",
                "$url": "?representation=ui.$dashboard"
            },
            "uilist": {
                "$title": "UI List Overview",
                "$url": "?representation=uilist.$dashboard"
            },
            "uiboost": {
                "$title": "UI Boost Overview",
                "$url": "?representation=uiboost.$dashboard"
            },
            "transitionTest": {
                "$title": "Json Page test",
                "$url": "?representation=uipagetest.$dashboard"
            },
            "JsonLint": {
                "$title": "JsonLint",
                "$url": "http://jsonlint.com/"
            },
            "jsonformatter": {
                "$title": "Jsonformatter Curiousconcept",
                "$url": "http://jsonformatter.curiousconcept.com/"
            }
        }
    },
    "$article": {
        "$category": "dashboard",
        "$layout": {
            "$layoutType": "tabs",
            "$items": [
                {
                    "$category": "section",
                    "$title": "Child Object Fields",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "childFieldTest"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Simple Fields",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "simpleFields"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Icon Fields",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "iconFields"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Layouts",
                    "$layout": {
                        "$items": [
                            {
                                "$bind": "layouts"
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Popup",
                    "$layout": {
                        "$layoutType": "columns",
                        "$layoutSubType": "40,60",
                        "$items": [
                            {
                                "$layoutType": "stack",
                                "$items": [
                                    {
                                        "$bind": "diagnosesbox"
                                    }
                                ]
                            },
                            {
                                "$layoutType": "stack",
                                "$items": [
                                    {
                                        "$bind": "messagebox"
                                    }
                                ]
                            }
                        ]
                    }
                },
                {
                    "$category": "section",
                    "$title": "Popup FK",
                    "$layout": {
                        "$layoutType": "stack",
                        "$items": [
                            {
                                "$bind": "messageboxfk"
                            }
                        ]
                    }
                }
            ]
        }
    }
}