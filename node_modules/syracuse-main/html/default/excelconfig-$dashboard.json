{
   	"$prototype":{
		"$title":"Workbook settings",
	  	"$":{
	  		"excelDs" : {
  				"$type": "application/x-portlet",
  				"$title": " ",
  				"$location": {
  					"$url": "excel://excelDatasources?representation=excelDatasource.$query"
  				}
  			},
  			"excelPublish": {
  				"$type": "application/x-portlet",
  				"$title": " ",
  				"$location": {
  					"$url": "excel://excelDocuments('{documentId}')?representation=excelDocument.$details"
  				}
			}
	  	}
   	},
   	"$article":{
	  	"$category":"dashboard",
	  	"$layout":{
			"$layoutType": "tabs",
			"$items":[{
				"$category": "section",
				"$title": "Datasources",
				"$layout": {
					"$items": [{
						"$bind": "excelDs"
					}]
				}
		 	}, {
				"$category": "section",
				"$title": "Publish",
				"$layout": {
					"$items": [{
						"$bind": "excelPublish"
					}]
				}
		 	}
		 	]
	  	}
   	}
}