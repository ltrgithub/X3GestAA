{
  "name": "syracuse-main",
  "description": "Sage ERP X3 main module",
  "version": "0.22.0",
  "homepage": "http://github.com/Sage-ERP-X3/Syracuse",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sage-ERP-X3/Syracuse.git"
  },
  "engines": { "node": ">=0.6.0" },
  "dependencies": {
    "streamline-runtime": "^1.0.37"
  },
  "author": "Sage",
  "private": true,
  "sage": {
    "x3": {
      "extensions": {
        "contracts": [{
          "module": "syracuse-main/lib/contract-extension"
        }, {
          "module": "@sage/syracuse-lib/src/flamegraph/contract-extension"
        }],
        "http": [{
          "module": "@sage/syracuse-lib/src/perfmon/dispatcher",
          "name": "dispatch",
          "prefix": "perfmon",
          "ez": true
        }, {
          "module": "@sage/syracuse-lib/src/memwatch/dispatcher",
          "name": "dispatch",
          "prefix": "memwatch",
          "ez": true
        }]
      }
    }
  }
}
