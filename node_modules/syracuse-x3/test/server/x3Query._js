		ok(true, "DBG query:" + JSON.stringify(query));

		var count = 20;
		ok(query.$resources.length != 0, "resources length");
		query.$resources.forEach(function(resource) {
			if (resource.C01P01 === "QUNIT_001") {
				strictEqual(resource.C01P02, "R01P01_02", "resource.C01P02");
				ok(resource.C01P02_REF !== undefined, true, "resource.C01P02_REF");
				strictEqual(resource.C01P02_REF.$title, "INTSHO 2", "resource.C01P02 $title");
				strictEqual(resource.C01P02_REF.$description, "INTDES 2", "resource.C01P02 $description");
			}
		});
		start();
	});

}

