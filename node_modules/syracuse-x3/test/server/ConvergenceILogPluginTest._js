	strictEqual(res, '{"sap":{"target":{"type":"plugin","plugin":{"class":"ILOG","name":"gantt","type":"asynchronous","id":null,"module":"Scheduling\\\\v6\\\\x3ilog.dll","method":"update","methodId":2}}}}', "get update ND_IHM for the plugin client");

