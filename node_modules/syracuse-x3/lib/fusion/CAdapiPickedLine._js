"use strict";
var helpers = require('syracuse-core/lib/helpers');

var CAdapiPickedLine = helpers.defineClass(function(aStructIst, aAction) {
	this.structIst = aStructIst;
	this.action = aAction;
}, null, {
	getIst: function() {
		return this.structIst;
	},
	isSelection: function() {
		return this.action == CDefAction.SRV_TREESELECTLINE || this.action == CDefAction.SRV_BRWSELECTLINE;
	}
})