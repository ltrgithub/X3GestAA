"use strict"; var helpers = require('syracuse-core/lib/helpers');

var CAdapiPickedLine = helpers.defineClass(function() {}, null, {
	private Integer this.action = null;
	private CStructIst this.structIst = null;

	public CAdapiPickedLine(aStructIst, aAction) {
		this.structIst = aStructIst;
		this.action = aAction;
	}, getAction: function() {
		return this.action;
	}, getIst: function() {
		return this.structIst;
	}

	/**
	 * SRV_TREESELECTLINE = 1061 SRV_BRWSELECTLINE = 1052
	 * 
	 * @return
	 */
	, isSelection: function() {
		return this.action == CDefAction.SRV_TREESELECTLINE || this.action == CDefAction.SRV_BRWSELECTLINE;
	}

}
