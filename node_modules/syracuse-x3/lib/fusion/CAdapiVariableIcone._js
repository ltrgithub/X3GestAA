"use strict"; var helpers = require('syracuse-core/lib/helpers');

var CAdapiVariableIcone = helpers.defineClass(function() {}, CAdapiVariable, {

	private static int NO_ICON = -1;

	protected CAdapiVariableIcone(srcLine, aEntityIndexBaseOne, aVarName, aID, aTdo, aFormat, aEntityState) {
		TODO_SUPER(srcLine, aEntityIndexBaseOne, aVarName, aID, aTdo, aFormat, aEntityState);
	}

	// Pas de format
	, acceptEditFormat: function() {
		return false;
	}, clone: function() {
		var wVariable = new CAdapiVariableIcone(getSrcLine(), getEntityIndexBaseOne(), getName(), this.getId(), getDataType(), getFormat(), getState());
		wVariable.setMustBeFormated(this.mustBeFormated());
		this.cloneValues(wVariable);
		wVariable.setServerValue(new CStructResu(getTracer(), CStructResu.X3TYP_INT, NO_ICON));
		wVariable.forceServerValueModification(false);
		return wVariable;
	}, formatX3ToDisplay: function(aData) {
		return String.valueOf(aData.getIntValue());
	}, setServerValue: function(aResu) {
		if (aResu.isNull()) {
			aResu.setIntValue(NO_ICON);
		}
		TODO_SUPER.setServerValue(aResu);
	}
}
