"use strict";
var helpers = require('syracuse-core/lib/helpers');
var CAdapiVariable = function() {};

var NO_ICON = -1;

var CAdapiVariableIcone = helpers.defineClass(function(srcLine, aEntityIndexBaseOne, aVarName, aID, aTdo, aFormat, aEntityState) {
	TODO_SUPER(srcLine, aEntityIndexBaseOne, aVarName, aID, aTdo, aFormat, aEntityState);
}, CAdapiVariable, {
	// Pas de format
	acceptEditFormat: function() {
		return false;
	},
	clone: function() {
		var wVariable = new CAdapiVariableIcone(this.srcLine, this.entityIndexBaseOne, this.name, this.id, this.dataType, this.format, this.state);
		wVariable.setMustBeFormated(this.mustBeFormated());
		this.cloneValues(wVariable);
		wVariable.setServerValue(new CStructResu(this.tracer, CStructResu.X3TYP_INT, NO_ICON));
		wVariable.forceServerValueModification(false);
		return wVariable;
	},
	formatX3ToDisplay: function(aData) {
		return String.valueOf(aData.intValue);
	},
	setServerValue: function(aResu) {
		if (aResu.isNull()) {
			aResu.setIntValue(NO_ICON);
		}
		TODO_SUPER.setServerValue(aResu);
	}
});