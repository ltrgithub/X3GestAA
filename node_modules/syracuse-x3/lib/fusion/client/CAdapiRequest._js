	removeAffSynchronePrefix: function(aMess, aPos) {
		var wPosStart = aPos + CAdapi.AFF_SYNCHRONE_PREFIX_LONG;
		if (wPosStart < aMess.length() - 1) {
			return aMess.substring(wPosStart);
		} else {
			return "";
		}
	},
