		var wDataSrcDeleter = new CDataSrcDeleter(this.adapiClient, wTargetLine, wNbLines);
		this.adapiClient.traverseSettable(wArgIst, wDataSrcDeleter);
		if (wDataSrcDeleter.hasLogicalErrMess()) {
