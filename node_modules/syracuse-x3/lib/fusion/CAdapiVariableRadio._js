"use strict";
var helpers = require('syracuse-core/lib/helpers');
var CAdapiVariableLocalMenu = function() {};

var CAdapiVariableRadio = helpers.defineClass(function(srcLine, aEntityIndexBaseOne, aVarName, aID, aTdo, aFormat, aEntityState, aLocalMenu) {
	TODO_SUPER(srcLine, aEntityIndexBaseOne, aVarName, aID, aTdo, aFormat, aEntityState, aLocalMenu);
}, CAdapiVariableLocalMenu, {
	clone: function() {
		var wVariable = new CAdapiVariableRadio(getSrcLine(), getEntityIndexBaseOne(), getName(), getId(), getDataType(), getFormat(), getState(), getLocalMenu());
		wVariable.setMustBeFormated(mustBeFormated());
		cloneValues(wVariable);
		return wVariable;
	},
	getDisplayValue: function() {
		if (getSrcLine().getAdapiClient().isInteractiveSession()) {
			if (getServerValResu() != null) {
				return getServerValResu().getValue();
			} else {
				return TODO_SUPER.getDisplayValue();
			}
		} else {
			if (isStateEditable()) {
				return getServerValResu().getValue();
			} else {
				return TODO_SUPER.getDisplayValue();
			}
		}
	}
});