"use strict";
var helpers = require('syracuse-core/lib/helpers');
var AdapiObservable = function() {};

var AdapiReboot = helpers.defineClass(function(client) {
	AdapiObservable.call(this, client);
	folder = client.getAdonixContext().getX3Folder();
	user = client.getAdonixContext().getX3UserId();
	lang = client.getAdonixContext().getLanguage();
	date = Calendar.getInstance().getTime().toString();
	// En dernier
	setTypeJsonNode('SESSION');
}, AdapiObservable, {
	toJson: function() {
		var close = new JSONArray();
		close.put(folder);
		close.put(user);
		close.put(lang);
		close.put(date);
		return close;

	},
	toJsonArray: function() {
		return toJson();
	}
});