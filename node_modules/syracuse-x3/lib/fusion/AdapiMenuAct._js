"use strict";
var helpers = require('syracuse-core/lib/helpers');
var AdapiObservable = function() {};

var AdapiMenuAct = helpers.defineClass(function(aclient, id, aEnabled) {
	AdapiObservable.call(this, aclient);
	this.id = id;
	this.enabled = aEnabled;
	// var dernier
	setTypeJsonNode('ACT');
}, AdapiObservable, {
	getAdapiObservableId: function() {
		return Integer.toString(this.id);
	},
	toJson: function() {
		var object = new JSONObject();
		object.put('id', String.valueOf(this.id));
		object.put('st', this.enabled);
		return object;
	},
	toString: function() {
		return toJson().toString();
	}

});