"use strict"; var helpers = require('syracuse-core/lib/helpers');

var CAdapiModifier = helpers.defineClass(function() {}, null, {
	protected boolean this.done;
	private String this.logicalErrMess = null;
	protected final ITracer this.tracer;

	public CAdapiModifier(aTracer) {
		this.tracer = aTracer;
	}, done: function() {
		return this.done;
	}, getLogicalErrMess: function() {
		return this.logicalErrMess;
	}, hasLogicalErrMess: function() {
		return this.logicalErrMess != null;
	}

	public , setAdapiModifiable: function(aStructIst, aSettable);

	, setLogicalErrMess: function(aLogicalErrMess) {
		///Trace
		if (this.tracer.traceLevelFilter(ITracer.LEVEL_CALLS)) {
			var wTB = this.tracer.popTraceBuffer();
			wTB.appendMethodName("setLogicalErrMess");
			wTB.appendDescr("LogicalErrMess", aLogicalErrMess);
			this.tracer.trace(this, ITracer.LEVEL_CALLS, wTB);
			this.tracer.pushTraceBuffer(wTB);
		}
		//FTrace*/
