"use strict";
var helpers = require('syracuse-core/lib/helpers');
var AdapiObservable = function() {};

var AdapiTargetSpecial = helpers.defineClass(function(name, client) {
	AdapiObservable.call(this, client);
	this.name = name;
	// En dernier
	setJsonType('TARGET');
}, AdapiObservable, {
	toJson: function() {
		var object = new JSONObject();
		object.put('type', name);
		return object;

	}
});