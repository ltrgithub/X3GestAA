"use strict"; var helpers = require('syracuse-core/lib/helpers');

var CAdapiWindow = helpers.defineClass(function() {}, null, {

	private static int WIN_MODELE_DIVERSE = 1;
	private static int WIN_TYPE_CLIENTE = 1;
	public final static char PREFIX_SPECIAL = '_';
	private int this.modele = WIN_MODELE_DIVERSE;
	private int this.nbColonnes;
	private int this.nbLignes;
	private int this.type = WIN_TYPE_CLIENTE;

	protected CAdapiWindow(aModele, aType, aNbLignes, aNbColonnes) {
		this.modele = aModele;
		this.type = aType;
		this.nbLignes = aNbLignes;
		this.nbColonnes = aNbColonnes;
	}, getModele: function() {
		return this.modele;
	}, getNbColonnes: function() {
		return this.nbColonnes;
	}, getNbLignes: function() {
		return this.nbLignes;
	}, getType: function() {
		return this.type;
	}, setNbColonnes: function(aNbColonnes) {
		try {
			this.nbColonnes = Integer.parseInt(aNbColonnes);
		} catch (e) {
			this.nbColonnes = -1;
		}
	}, setNbLignes: function(aNbLignes) {
		try {
			this.nbLignes = Integer.parseInt(aNbLignes);
		} catch (e) {
			this.nbLignes = -1;
		}
	}

}
