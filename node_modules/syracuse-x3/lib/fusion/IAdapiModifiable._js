"use strict"; var helpers = require('syracuse-core/lib/helpers');

import com.adonix.x3.comm.protocol.CNodeArgIst;
import com.adonix.x3.comm.protocol.CStructResu;
import com.adonix.x3.formater.IFormat;

public interface IAdapiModifiable {

	public static enum StyleApply {
		ENTITY, CHILDS, TITLE;
		public static StyleApply getApply(arg) {
			var idx = arg == null ? 0 : arg.getStructIst().getIdx();
			switch (idx) {
			case -1:
				return CHILDS;
			case -2:
				return TITLE;
			default:
				return ENTITY;

			}
		}
	}, entityStateSet: function(aEntityState, force);

	, getAdxId: function();

	, getFormat: function();

	, getState: function();

	, setServerFormat: function(aFormat);

	, setServerStyle: function(aStyle, applyOn);

	, setServerTitle: function(aTitle);

	, setServerValue: function(aStructResu);

}
