"use strict";
var globals = require('streamline-runtime').globals;

exports.$exported = true;
exports.getProperties = function(_) {
	var flags = {
		useEtna: false,
		useScm: false
	};
	var up = globals.context.session && globals.context.session.getUserProfile(_);
	if (up) {
		var endpoint = up.selectedEndpoint(_);
		flags.useEtna = endpoint.useEtna(_) || false;
		try {
			var scmConfig = require("scm-helper/nodelocal").config;
			flags.useScm = true;
		} catch (e) {}
	}
	return flags;
}