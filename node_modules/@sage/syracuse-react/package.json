{
    "name": "syracuse-react",
    "version": "1.0.0",
    "description": "Reactjs based UI for X3",
    "main": "index.js",
    "scripts": {
        "dev": "webpack --env=dev --config ./webpack/webpack.config.js",
        "watch": "webpack --env=dev --config ./webpack/webpack.config.js --watch",
        "prod": "webpack --env=prod --config ./webpack/webpack.config.js",
        "watchprod": "webpack --env=prod --config ./webpack/webpack.config.js --watch",
        "test": "cross-env JEST_SUITE_NAME=\"syracuse-react\" JEST_JUNIT_OUTPUT=\"./junit/junit.xml\" JEST_JUNIT_CLASSNAME=\"{classname}\" JEST_JUNIT_TITLE=\"{title}\" jest",
        "dist": "npm run dev && npm run prod"
    },
    "author": "",
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "@types/ambient-type-definitions": "file:./@types/ambient-type-definitions",
        "@types/cheerio": "^0.22.1",
        "@types/enzyme": "^2.7.7",
        "@types/enzyme-to-json": "file:./@types/enzyme-to-json",
        "@types/jest": "^19.2.2",
        "@types/jest-enzyme": "file:./@types/jest-enzyme",
        "@types/jquery": "^2.0.46",
        "@types/node": "^7.0.29",
        "@types/react": "15.0.33",
        "@types/react-dom": "^15.5.0",
        "@types/react-tap-event-plugin": "0.0.30",
        "babel-core": "^6.3.26",
        "babel-loader": "^7.0.0",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "case-sensitive-paths-webpack-plugin": "^2.0.0",
        "cross-env": "^5.0.1",
        "css-loader": "^0.28.0",
        "enzyme": "^2.8.0",
        "enzyme-to-json": "^1.5.0",
        "extract-text-webpack-plugin": "^2.1.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^20.0.4",
        "jest-cli": "^20.0.4",
        "jest-enzyme": "^3.0.0",
        "jest-junit": "^2.1.0",
        "less": "^2.7.2",
        "less-loader": "^4.0.3",
        "pretty-hrtime": "^1.0.1",
        "react-test-renderer": "^15.6.1",
        "require-dir": "^0.3.0",
        "style-loader": "^0.18.2",
        "svg-url-loader": "^2.1.1",
        "ts-jest": "^20.0.6",
        "ts-loader": "^2.0.3",
        "typescript": "^2.4.0",
        "typings": "^2.1.0",
        "underscore": "^1.8.3",
        "url-loader": "^0.5.8",
        "webpack": "^2.3.3",
        "webpack-dev-server": "^2.3.0",
        "webpack-merge": "^4.1.0"
    },
    "dependencies": {
        "file-loader": "^0.11.2",
        "jsurl": "^0.1.4",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "hash.js": "1.0.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "transform": {
            "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(__tests__)(.*).test.tsx$",
        "verbose": true,
        "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
        "testResultsProcessor": "<rootDir>/node_modules/jest-junit/index.js",
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}"
        ],
        "haste": {
            "providesModuleNodeModules": [
                ".*"
            ]
        },
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/typings/",
            "<rootDir>/coverage/",
            "<rootDir>/junit/",
            "<rootDir>/webpack/"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/typings/",
            "<rootDir>/coverage/",
            "<rootDir>/junit/",
            "<rootDir>/webpack/"
        ],
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/typings/",
            "<rootDir>/coverage/",
            "<rootDir>/junit/",
            "<rootDir>/webpack/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}