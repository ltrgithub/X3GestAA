            "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$representation}.$details&role={$role}"
        },
        "$wordreport": {
            "$title": "Word report",
            "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$representation}.$details&role={$role}&createMode={createMode}.&doc_uuid={doc_uuid}.&docx=print",
            "$type": "application/syracuse-word-report",
            "$parameters": {
                "$properties": {
                    "doc_uuid": {
                        "$title": "Doc ID",
                        "$type": "application/x-string",
                        "$value": "1"
                    }
                }
            }
        }
    },
    "$localization": {
        "@LoginSectionTitle1": "The",
        "@LoginSectionTitle2": "Login",
        "@InformationsSectionTitle": "Information",
        "@ExplorerSectionTitle": "Explorer",
        "@AdministrationSectionTitle": "Administration",
        "@LocalesSectionTitle": "Custom locales"
    },
    "$article": {
        "$layout": {
            "$items": [
                {
                    "$layoutType": "row",
                    "$items": [
                        {
                            "$items": [
                                {
                                    "$title": "{@LoginSectionTitle1} {@LoginSectionTitle2} section",
                                    "$category": "section",
                                    "$layout": {
                                        "$items": [
                                            {
                                                "$bind": "login"
                                            },
                                            {
                                                "$bind": "active"
                                            },
                                            {
                                                "$bind": "authentication"
                                            },
                                            {
                                                "$bind": "password"
                                            },
                                            {
                                                "$bind": "authenticationName"
                                            },
                                            {
                                                "$bind": "ldap"
                                            },
                                            {
                                                "$bind": "oauth2"
                                            }
                                        ]
                                    }
                                },
                                {
                                    "$title": "{@InformationsSectionTitle}",
                                    "$category": "section",
                                    "$layout": {
                                        "$items": [
                                            {
                                                "$bind": "title"
                                            },
                                            {
                                                "$bind": "firstName"
                                            },
                                            {
                                                "$bind": "lastName"
                                            },
                                            {
                                                "$bind": "comments"
                                            },
                                            {
                                                "$bind": "email"
                                            },
                                            {
                                                "$bind": "photo"
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        {
                            "$items": [
                                {
                                    "$title": "{@ExplorerSectionTitle}",
                                    "$category": "section",
                                    "$layout": {
                                        "$items": [
                                            {
                                                "$bind": "explorer",
                                                "$isTitleHidden": true
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                },
                {
                    "$layoutType": "stack",
                    "$items": [
                        {
                            "$title": "{@AdministrationSectionTitle}",
                            "$category": "section",
                            "$layout": {
                                "$layoutType": "row",
                                "$items": [
                                    {
                                        "$items": [
                                            {
                                                "$bind": "groups"
                                            },
                                            {
                                                "$bind": "endpoints"
                                            }
                                        ]
                                    },
                                    {
                                        "$items": [
                                            {
                                                "$bind": "adminTeams"
                                            },
                                            {
                                                "$bind": "authorTeams"
                                            },
                                            {
                                                "$bind": "memberTeams"
                                            }
                                        ]
                                    }
                                ]
                            }
                        },
                        {
                            "$title": "{@LocalesSectionTitle}",
                            "$category": "section",
                            "$layout": {
                                "$items": [
                                    {
                                        "$bind": "locales",
                                        "$isTitleHidden": true,
                                        "$format": "cards",
                                         "$layout": {
                                                "$layoutType": "row",
                                                "$items": [
                                                    {
                                                        "$layoutType": "stack",
                                                        "$items": [
                                                            {
                                                                "$bind": "code"
                                                            },
                                                            {
                                                                "$bind": "description"
                                                            },
                                                            {
                                                                "$bind": "enabled"
                                                            },
                                                            {
                                                                "$bind": "shortDate"
                                                            },
                                                            {
                                                                "$bind": "shortTime"
                                                            },
                                                            {
                                                                "$bind": "shortDatetime"
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        "$layoutType": "stack",
                                                        "$items": [
                                                            {
                                                                "$bind": "longDate"
                                                            },
                                                            {
                                                                "$bind": "longTime"
                                                            },
                                                            {
                                                                "$bind": "longDatetime"
                                                            },
                                                            {
                                                                "$bind": "firstDayOfWeek"
                                                            },
                                                            {
                                                                "$bind": "twoDigitYearMax"
                                                            }
                                                        ]
                                                    },
                                                    {
                                                        "$layoutType": "stack",
                                                        "$items": [
                                                            {
                                                                "$bind": "numberDecimalSeparator"
                                                            },
                                                            {
                                                                "$bind": "numberGroupSeparator"
                                                            },
                                                            {
                                                                "$bind": "numberGroupSize"
                                                            }
                                                        ]
                                                    }
                                                ]
                                            }
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    }
}