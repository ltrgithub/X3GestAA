{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2015",
        "outDir": "out",
        "sourceMap": false,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "rootDir": ".",
        "strictNullChecks": false,
        "experimentalDecorators": false
    },
    "filesGlob": [
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "out"
    ]
}