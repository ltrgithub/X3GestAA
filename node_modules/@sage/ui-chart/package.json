{
  "name": "ui-chart",
  "description": "chart widget extension",
  "version": "1.0.0",
  "author": "Sage",
  "private": true,
  "sage": {
    "x3": {
      "extensions": {
        "widgets": [{
          "module": "@sage/ui-chart/public/highchart",
          "type": "application/x-array-chart"
        },{
          "module": "@sage/ui-chart/public/orgChart",
          "type": "application/x-array-orgview"
        },{
          "module": "@sage/ui-chart/public/flashWidget",
          "type": "application/x-array-agenda"
        },{
          "module": "@sage/ui-chart/public/flashWidget",
          "type": "application/x-shockwave-flash"
        },{
          "module": "@sage/ui-chart/public/flashWidget",
          "type": "application/x-shockwave-flash-$safeV1"		  
        }
		]
      }
    }
  }
}