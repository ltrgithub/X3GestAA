"use strict";
var util = require("etna-supervisor/lib/util");

exports.constructor = util.defineClass(function(superv) {
	this.supervisor = superv;
	this.$exported = true;
	this.data = "";
}, null, {
	get: function(_, name) {
		switch (name) {
			case "DATA":
				return this.data;
		}
	},
	set: function(_, name, value) {
		switch (name) {
			case "DATA":
				return this.data = value;
		}
	}
});