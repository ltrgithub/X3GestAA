"use strict";
var util = require("etna-supervisor/lib/util");

exports.constructor = util.defineClass(function(superv) {
	this.supervisor = superv;
	this.$exported = true;
}, null, {
	get: function(_, name) {
		switch (name) {
			case "PREFIX":
				return "/sdata/x3/erp/" + this.supervisor.folderName;
		}
		return "";
	}
});