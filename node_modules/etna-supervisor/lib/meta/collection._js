"use strict";

var util = require("etna-supervisor/lib/util");

exports.constructor = util.defineClass(function(superv, clas, data) {
	this.supervisor = superv;
	this.class = clas;
	this.data = data;
}, null, {
	init: function(_) {
		return this;
	},
	name: {
		get: function() {
			return this.data.CODCOL;
		}
	},
	getSql: function(_, colNames, tableNames, wheres, params, descs, cache, raw) {
		var properties = this.class.properties;
		Object.keys(properties).forEach_(_, function(_, name) {
			properties[name].getSql(_, colNames, tableNames, wheres, params, descs, cache, raw);
		}, this);
	}
});