Local Clbfile TMP_CLBFILE(2)
Infbox "TEST"
TMP_CLBFILE= func TESTSUITE()
Call FERME_TRACE From LECFIC

GTRACE = "QLFAC_I_EVENTS_"+GACTX.USER
Call LEC_TRACE From LECFIC
End

Subprog SETUP
# delete old data
Local File AQCPROC01 [AQCP01]
Trbegin [AQCP01]
Delete [AQCP01] Where C01P01 = "TEST123456"
Commit
End


Funprog TESTSUITE()
  Call ADD_TESTCASE("TC_PROPERTY","TC_PROPERTY" ,28) From AXUNIT
  Call ADD_TESTCASE("TC_CLASS","TC_CLASS" ,107) From AXUNIT
  Call ADD_TESTCASE("TC_CLASS_AREAD","TC_CLASS_AREAD" ,16) From AXUNIT
  Call ADD_TESTCASE("TC_CLASS_COLLECTION","TC_CLASS_COLLECTION" ,257) From AXUNIT
End func AXUNIT.RUN_TESTSUITE("QLFAC_I_EVENTS", "QLFAC_I_EVENTS")

Subprog TC_PROPERTY
Local Char TC(255),EVENT(50)
Local Integer MY_STATUS,ASTATUS
Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01

Gosub EVENTS_DATA         # Load expected events data

TC="Basic tests"
Call CHECK_EQUAL(TC,TC) From AXUNIT


TC="Initialize Instance"
Call CHECK_EQUAL(TC,TC) From AXUNIT
[L]MY_STATUS = fmet INS1.AINIT()
Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT
EVENT="AINIT"
Gosub DISPLAY_MESSAGES

TC="Check property INIT  based on AINIT"
Call CHECK_EQUAL(TC,TC) From AXUNIT
Call CHECK_EQUAL(INS1.C01P01,"INITVALUE") From AXUNIT

TC="Check property  CONTROL & PROPAGATE - based on SET"
Call CHECK_EQUAL(TC,TC) From AXUNIT
INS1.C01P01 = "newVal"
Call CHECK_EQUAL(INS1.C01P01,"NEWVAL") From AXUNIT
Call CHECK_EQUAL(INS1.C01P02,"ResProp") From AXUNIT
TC=INS1.C01P02

TC="Check proeprty length when field depends on X3 datatype"
Call CHECK_EQUAL(TC,TC) From AXUNIT
INS1.C01P02 = "123456789012345678901234567890123456789012345"
Call CHECK_EQUAL(len(INS1.C01P02),30) From AXUNIT
Call CHECK_EQUAL(INS1.C01P02,"123456789012345678901234567890") From AXUNIT

# Instance
TC="Check property CONTROL - QUNIT invalid value"
Call CHECK_EQUAL(TC,TC) From AXUNIT
INS1.C01P01 = "QUNIT"
Local Integer I
TC=""
For I=1 To maxtab(INS1.AERROR)
  If INS1.AERROR(I)<>null & INS1.AERROR(I).PRO = "C01P01" & INS1.AERROR(I).DEL<>[V]CST_AYES
    TC=INS1.AERROR(I).MES
    Break
  Endif
Next
 Call CHECK_EQUAL(TC,"QUNIT not valid") From AXUNIT

End

Subprog TC_CLASS_AREAD
Local Char TC(255),EVENT(50)
Local Integer MY_STATUS,ASTATUS
Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01

Gosub EVENTS_DATA         # Load expected events data

TC="Check AREAD_BEFORE - not allowed to read record ADDD"
Call CHECK_EQUAL(TC,TC) From AXUNIT
[L]MY_STATUS = fmet INS1.AREAD("ADDD")
Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AERROR) From AXUNIT
EVENT="AREAD1"
Gosub DISPLAY_MESSAGES

TC="Check AREAD_AFTER- not allowed to read record CCL2"
Call CHECK_EQUAL(TC,TC) From AXUNIT
[L]MY_STATUS = fmet INS1.AREAD("CCL2")
Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AERROR) From AXUNIT
EVENT="AREAD2"
Gosub DISPLAY_MESSAGES

TC="Check AREAD - no errors on  CCLf"
Call CHECK_EQUAL(TC,TC) From AXUNIT
[L]MY_STATUS = fmet INS1.AREAD("CCLf")

If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
 Call CHECK_EQUAL([L]MY_STATUS,[L]MY_STATUS) From AXUNIT
Else
Call CHECK_NOTEQUAL([L]MY_STATUS,[L]MY_STATUS) From AXUNIT
Endif
EVENT="AREAD"
Gosub DISPLAY_MESSAGES

FreeGroup INS1
End

Subprog TC_CLASS
Local Char TC(255),EVENT(50)
Local Integer MY_STATUS,ASTATUS

Gosub EVENTS_DATA         # Load expected events data

Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01

[L]MY_STATUS = fmet INS1.AREAD("TEST1234")
EVENT="AREAD"
Gosub DISPLAY_MESSAGES
If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  [L]MY_STATUS = fmet INS1.ADELETE()
  EVENT="ADELETE"
  Gosub DISPLAY_MESSAGES
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT
  FreeInstance INS1
  INS1 = NewInstance C_AQCPROC01
Endif

[L]MY_STATUS = fmet INS1.AINIT()
EVENT="AINIT"
Gosub DISPLAY_MESSAGES
Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT

If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  INS1.C01P01 = "TEST1234"
  INS1.C01P02 = "PROPC01"
  INS1.PROPERTY = string$(250,"C")
  INS1.DESCRIPTION = "Description"

  #Litle hack to make sure not to have a propagate on CRESUR
  [L]MY_STATUS = fmet INS1.AINSERT()
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT
  EVENT="AINSERT"
  Gosub DISPLAY_MESSAGES

  #Try it twice
  [L]MY_STATUS = fmet INS1.AINSERT()
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AERROR) From AXUNIT
  EVENT="AINSERT_ABORT"
  Gosub DISPLAY_MESSAGES
Endif
FreeGroup INS1

##  Update instance
Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01
[L]MY_STATUS = fmet INS1.AREAD("TEST1234")
If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  INS1.PROPERTY = string$(250,"U")
  INS1.DESCRIPTION = "Upd description"
  [L]MY_STATUS = fmet INS1.AUPDATE()
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT
Endif
EVENT="AUPDATE"
Gosub DISPLAY_MESSAGES
FreeGroup INS1

# Instance for AUPDATE2
Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01
# 1st create new instance
INS1.C01P01 = "TEST123456"
INS1.C01P02 = "PROPC01"
INS1.PROPERTY = string$(250,"C")
INS1.DESCRIPTION = "Description"
[L]MY_STATUS = fmet INS1.AINSERT()
FreeGroup INS1

Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01
[L]MY_STATUS = fmet INS1.AREAD("TEST123456")
If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  [L]MY_STATUS = fmet INS1.AUPDATE()
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT
Endif
EVENT="AUPDATE2"
Gosub DISPLAY_MESSAGES
FreeGroup INS1
End

$DISPLAY_MESSAGES
  Local Integer I,NB_MSG
  Local Instance MESSAGES(1..) Using C_AERROR
  NB_MSG=func  AGETERRORS(INS1,MESSAGES)
  Call CHECK_NOTEQUAL("EVENT ["+EVENT+"]","") From AXUNIT
  # Check messages
  Case EVENT
    When "AINSERT"               : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AINSERT)
    When "AINSERT_ABORT"         : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AINSERT_ABORT)
    When "AREAD"                 : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AREAD)
    When "AUPDATE"               : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AUPDATE)
    When "ADELETE"               : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_ADELETE)
    When "AINIT"                 : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AINIT)
    When "AREAD1"                : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AREAD1)
    When "AREAD2"                : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AREAD2)
    When "AREAD_AQCORDERA"       : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AREAD_AQCORDERA)
    When "ADELETE_AQCORDERA"     : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_ADELETE_AQCORDERA)
    When "AINIT_AQCORDERA"       : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AINIT_AQCORDERA)
    When "AINSERT_AQCORDERA"     : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AINSERT_AQCORDERA)
    When "AUPDATE_AQCORDERA"     : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AUPDATE_AQCORDERA)
    When "PARENT_SET_PROPERTY"   : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_PARENT_SET_PROPERTY)
    When "AINIT_CHILD_AQCORDERLA": Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AINIT_CHILD_AQCORDERLA)
    When "ADELLINE_AQCORDERA"    : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_ADELLINE_AQCORDERA)
    When "AUPDATE2"              : Call COMPARE_MESSAGES(EVENT,MESSAGES,EVENTS_AUPDATE2)
    When Default:  : Call CHECK_EQUAL("["+EVENT+"] not found","") From AXUNIT
  Endcase
  # clear messages
  Callmet INS1.ADELETEERROR(CST_ALLERRORS)
Return

Subprog COMPARE_MESSAGES(EVENT,GOT_MSG, EXPECTED_MSG)
Value Char EVENT
Value Instance GOT_MSG(1..) Using C_AERROR
Variable Char EXPECTED_MSG()()
Local Char GOT_MESSAGE(255), EXPECTED_MESSAGE(255)
Local Integer  I

If 0
 Openo filpath('TRA',"QLFAC_I_EVENTS",'log') Using [TMP]
 Wrseq "#{ "+EVENT Using [TMP]
 Wrseq "Local Integer IDX : IDX=0" Using [TMP]
 Wrseq "Local Char EVENTS_"+EVENT+"(255)(0..)" Using [TMP]
 Wrseq 'EVENTS_'+EVENT+'(IDX)= "'+EVENT+'"' Using [TMP]
 For I=1 To maxtab(GOT_MSG)
  Wrseq 'IDX+=1 : EVENTS_'+EVENT+'(IDX)= "'+GOT_MSG(I).MES + '"' Using [TMP]
 Next I
 Wrseq "#} "+EVENT Using [TMP]
 Wrseq "" Using [TMP]
 Openo Using [TMP]
Endif


For I=1 To maxtab(GOT_MSG)
  If I<=maxtab(EXPECTED_MSG)
    If len(EXPECTED_MSG(I))
     Call CHECK_EQUAL(seg$(GOT_MSG(I).MES,0,140), seg$(EXPECTED_MSG(I),0,140)) From AXUNIT
    Else
     Call CHECK_EQUAL(GOT_MSG(I).MES,GOT_MSG(I).MES) From AXUNIT
    Endif
    #Call CHECK_NOTEQUAL('IDX+=1 : EVENTS_AUPDATE_AQCORDERA(IDX)= "'+GOT_MSG(I).MES+'"','') From AXUNIT
  Else
    Call CHECK_EQUAL("More got messages than expected messages ["+GOT_MSG(I).MES+"]",num$(maxtab(GOT_MSG))-num$(maxtab(EXPECTED_MSG))) From AXUNIT
  Endif
Next
If I<maxtab(EXPECTED_MSG)
  Call CHECK_EQUAL("More expected messages than got messages ["+EXPECTED_MSG(I)+"]","") From AXUNIT
Endif
End

Subprog TC_CLASS_COLLECTION
Local Char TC(255),EVENT(50)
Local Integer MY_STATUS,ASTATUS,LINE
Local Instance INS1 Using C_AQCORDERA
INS1 = NewInstance C_AQCORDERA

#Clean the environment :
Gosub EVENTS_DATA           # Get events data
[L]MY_STATUS = fmet INS1.AREAD("5454545")
If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  [L]MY_STATUS = fmet INS1.ADELETE()
Endif

FreeGroup INS1
INS1 = NewInstance C_AQCORDERA
[L]MY_STATUS = fmet INS1.AINIT()
EVENT="AINIT_AQCORDERA"
Gosub DISPLAY_MESSAGES
Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT

If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  INS1.YORDNUM = "5454545"
  INS1.CUR     = "EUR"
  INS1.YBPCNUM = "ELISA"
  EVENT="PARENT_SET_PROPERTY"
  Gosub DISPLAY_MESSAGES

  [L]LINE = fmet INS1.ADDLINE("AQCORDL",[V]CST_ALASTPOS)
  #NEW ETNA SYNTAX :
  #[L]LINE = fmet INS1.AQCORDL.AADD([V]CST_ALASTPOS)

  EVENT="AINIT_CHILD_AQCORDERLA"
  Gosub DISPLAY_MESSAGES

  INS1.AQCORDL([L]LINE).YORDLIN = 1
  INS1.AQCORDL([L]LINE).YITMREF = "ARTCCL"
  [L]MY_STATUS = fmet INS1.AINSERT()
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT

 EVENT="AINSERT_AQCORDERA"
 Gosub DISPLAY_MESSAGES

Endif


FreeGroup INS1
INS1 = NewInstance C_AQCORDERA
[L]MY_STATUS = fmet INS1.AREAD("5454545")

#Trigger the lazy loading if etna
Call CHECK_EQUAL(maxtab(INS1.AQCORDL),1) From AXUNIT
EVENT="AREAD_AQCORDERA"
Gosub DISPLAY_MESSAGES

Local Instance INS1 Using C_AQCORDERA
INS1 = NewInstance C_AQCORDERA
[L]MY_STATUS = fmet INS1.AREAD("5454545")
If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
    Call CHECK_EQUAL(maxtab(INS1.AQCORDL),1) From AXUNIT

    [L]LINE = fmet INS1.ADDLINE("AQCORDL",[V]CST_ALASTPOS)
    INS1.AQCORDL([L]LINE).YORDLIN = 1                 # This will generate abort due to duplicate
    INS1.AQCORDL([L]LINE).YITMREF = "ARTCCL"
    INS1.YBPCNUM = "ELISA"
    Call CHECK_EQUAL(maxtab(INS1.AQCORDL),2) From AXUNIT

    [L]MY_STATUS = fmet INS1.AUPDATE()

    EVENT="AUPDATE_AQCORDERA"
    Gosub DISPLAY_MESSAGES

    Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AERROR) From AXUNIT
Endif

FreeGroup INS1
INS1 = NewInstance C_AQCORDERA
[L]MY_STATUS = fmet INS1.AREAD("5454545")
#Trigger the lazy loading if etna
Call CHECK_EQUAL(maxtab(INS1.AQCORDL),1) From AXUNIT

# clear read messages
Callmet INS1.ADELETEERROR(CST_ALLERRORS)
If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
    [L]MY_STATUS = fmet INS1.ADELLINE("AQCORDL",[V]CST_ALASTPOS)
Endif
EVENT="ADELLINE_AQCORDERA"
Gosub DISPLAY_MESSAGES

FreeGroup INS1
INS1 = NewInstance C_AQCORDERA

[L]MY_STATUS = fmet INS1.AREAD("5454545")
#Trigger the lazy loading if etna in order to get AREAD messages before delete messages:
Call CHECK_EQUAL(maxtab(INS1.AQCORDL),1) From AXUNIT

If find([L]MY_STATUS ,[V]CST_AOK,[V]CST_AINFO)
  # clear read messages
  Callmet INS1.ADELETEERROR(CST_ALLERRORS)

  [L]MY_STATUS = fmet INS1.ADELETE()
  EVENT="ADELETE_AQCORDERA"
  Gosub DISPLAY_MESSAGES
  Call CHECK_EQUAL([L]MY_STATUS,[V]CST_AINFO) From AXUNIT
Endif

End

