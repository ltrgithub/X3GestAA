function Category(name, description, minmax, opt) {
	this.$name = name;
	this.$description = description;
	this.$min = ~~minmax[0];
	this.$max = ~~(minmax[1] || minmax[0]);
	opt = opt || {};
	_extend(this, opt || {});
}

