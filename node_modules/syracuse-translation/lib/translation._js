}

function readIndex(_, indexpath, chapter, options) {
	tracer && tracer("indexpath: " + indexpath);
	var fsModule = options && options.indexFiler || fs,
		diags = options && options.$tracking,
		index;

	var path = options.masterIndexPath || "../translations/data/masterIndex.json";
	if (!fsModule.exists(path, _)) {
		path = fsp.join(options.indexdir, "masterIndex.json");
	}
	var masterIndex;
	var data;
	if (!fsModule.exists(path, _)) {
		log.warn("Master index node found: " + path);
	} else {
		data = fsModule.readFile(path, "utf8", _);
	}
	try {
		masterIndex = data && JSON.parse(data);
		// tracer && tracer("index data " + JSON.stringify(index, null, 2));
	} catch (e) {
		throw new Error(localizedText("incorrectJsonFormat", indexpath));
	}

	var mapping = masterIndex && masterIndex.mapping || {};
	var mkeys = masterIndex && masterIndex.keys;
	!mkeys && console.error("**** No master index keys *****");

	data = fsModule.exists(indexpath, _) && fsModule.readFile(indexpath, "utf8", _);
	if (!data) {
		index = {};
		tracer && tracer("data is empty ");
	} else {
		try {
			index = JSON.parse(data);
			// tracer && tracer("index data " + JSON.stringify(index, null, 2));
		} catch (e) {
			throw new Error(localizedText("incorrectJsonFormat", indexpath));
		}
		_traverse(index, function(context, gv, ev, tv) {
			if (mkeys) {
				var mi, id;
				// console.error("traverse:", gv && gv.$key, ev && ev.$key, context.groupKey, id);
				if (!ev && !tv) {
					mi = mkeys[gv.$key];
					if (mi) {
						id = mi.gid;
						// context.groupKeyNorm = _getMapping(mapping, _normalizeGroup(context.groupKey));
						context.groupKeyNorm = id;
						if (id != context.groupKey) {
							// console.error("reassign group:", context.groupKey, id);
							// gv.$id = id;
							gv.$oldId = context.groupKey;
							index[context.groupKeyNorm] = gv;
							delete index[context.groupKey];
						}
					}
				} else if (!tv) {
					mi = mkeys[ev.$key];
					if (mi) {
						id = mi.uid;
						// ev.$oldId = context.entryKey;
						context.entryKeyNorm = id;

						var eid = id.split("/").slice(-1);
						if (eid != context.entryKey) {
							// console.error("reassign entry:", context.entryKey, id);
							gv[eid] = ev;
							delete gv[eid];
						}
						// context.groupKeyNorm = _getMapping(mapping, _normalizeGroup(context.groupKey));
					}
				}
			}
			if (ev != null) {
				if (ev.chp != null) delete ev.chp;
				if (ev.id != null) delete ev.id;
				return false;
			}
		});
	}

	function _newEntry(index, group, ident, val, language) {
		var segment = index[group] || (index[group] = {}),
			e;
		segment.$key = sha1(group);
		// segment.$id = _getMapping(mapping, _normalizeGroup(group));

		e = {
			group: group,
			ident: ident,
			val: val || placeholders.equal,
			language: language || defaultLanguage,
			index: index,
			segment: segment,
			$key: sha1(segment.$key + "/" + ident),
			$id: _getMapping(mapping, _normalizeGroup(group + "/" + ident))
		};
		e.keyString = e.group + "/" + e.ident + "['" + e.language.iso + "']";
		return e;
	}

	function _traverse(index, cb) {
		if (typeof(cb) != "function") return;
		var context = {};
		forEachKey(index, function(gk, gv) {
			context.groupKey = gk;
			if (gk[0] === "$") return;
			if (cb(context, gv) === false) return;
			forEachKey(gv, function(mk, mv) {
				context.entryKey = mk;
				if (mk[0] === "$") return;
				if (cb(context, gv, mv) === false) return;
				forEachKey(mv.text, function(tk, tv) {
					context.langKey = tk;
					cb(context, gv, mv, tv);
				});
			});
		});
	}


	index.newEntry = _newEntry.bind(null, index);
	index.traverse = _traverse.bind(null, index);

	return {
		index: _initIndexChapter(index, chapter),
		masterIndex: masterIndex
	};
}

function _initIndexChapter(index, chapter) {
	chapter = _clone(chapter);
	index.$config = index.$config || {};
	index.$config.$createDate = index.$config.$createDate || datetime.now().toString();
	index.$config.$lastModified = index.$config.$lastModified || index.$config.$createDate;
	index.$config.$chapter = _extend(chapter, index.$config.$chapter, true, true);
	index.$config.$target = index.$config.$target || {};
	index.$config.$source = index.$config.$source || {};

	delete index.$config.$chapter.$current;
	delete index.$config.$chapter.$;

	index.$chapter = index.$config.$chapter;
	return index;
}

