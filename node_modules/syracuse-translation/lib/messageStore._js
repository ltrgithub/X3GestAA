function Dictionnary() {
	var _tm = {};

	this.smartAssign = function(key, lan, text) {
		if (!key) return;
		var tmu = _tm[key] || (_tm[key] = new DictionnaryEntry(key)),
			mtl = tmu.getText(lan);
		// tmu.$count++;
		text = _cleanString(text);
		tmu.setText(lan, text);
	};

	this.getEntry = function(key) {
		return _tm[key];
	};
	this.getText = function(key, lan) {
		var tmu = _tm[key];
		return tmu && tmu.getText(lan);
	};
}

function DictionnaryEntry(key) {
	this.$key = key;
	this.$count = 0;
	this.text = {};
}

DictionnaryEntry.prototype = {
	getText: function(lan) {
		return this.text[lan];
	},
	setText: function(lan, text) {
		text = _cleanString(text);
		var mtl = this.text[lan];
		if (mtl !== undefined && text !== mtl && text !== this.$key) {
			// mtl.alt = mtl.alt || (mtl.alt = []);
			// mtl.alt.push(text);
			// console.error("  various translations for '" + this.$key + "' [" + lan + "] count=" + this.$count + "\n--- " + text + "\n+++ " + mtl);
			// various translations for '" + this.$key + "' [" + l + "] count=" + this.$count + "\n--- " + text + "\n+++ " + mtl);
		} else if (this.text[lan] == null) {
			this.text[lan] = text;
		}
	}
};

