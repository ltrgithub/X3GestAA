"use strict";
var locale = require('syracuse-core/lib/locale');

var setCurrent = locale.setCurrent;
locale.setCurrent = function(_, value, localePreferences) {
	return setCurrent(~_, value, localePreferences);
}

var getResourcesHook = locale.getResourcesHook;
locale.getResourcesHook = function(_, filename, accept) {
	return getResourcesHook(~_, filename, accept);
}