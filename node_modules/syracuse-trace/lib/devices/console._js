"use strict";

var helpers = require('syracuse-core/lib/helpers');
var ez = require('ez-streams');

// Default implementation for log tracing simply outputs all information directly to the console

exports.Console = helpers.defineClass(function() {
	this.type = "console";
}, null, {
	initialise: function(_) {
		this.device = ez.devices.console.log;
	},
	preprocess: function(_) {},
	write: function(_, data) {
		this.device.write(_, data.output);
	},
	close: function(_) {},
	toJSON: function() {
		return this.type;
	}
});