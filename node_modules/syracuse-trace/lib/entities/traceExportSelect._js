"use strict";

var jsurl = require("jsurl");

exports.entity = {
	$titleTemplate: "Select import traces",
	$valueTemplate: "",
	$isPersistent: false,
	$canSave: false,
	$capabilities: "",
	$properties: {
		parameters: {
			$title: "Parameters",
			$isHidden: true,
			$compute: function(_, instance) {
				return jsurl.stringify(instance.toJson(_));
			}
		},
		compress: {
			$title: "Compress",
			$type: "boolean",
			$default: false
		}
	},
	$functions: {
		toJson: function(_) {
			var instance = this;
			return {
				compress: instance.compress(_),
			};
		}
	}
};