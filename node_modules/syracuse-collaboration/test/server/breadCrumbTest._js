"use strict";

var helpers = require('syracuse-core/lib/helpers');
var locale = require('syracuse-core/lib/locale');

var config = require('syracuse-main/lib/nodeconfig').config; // must be first syracuse require
var adminHelper = require("syracuse-collaboration/lib/helpers").AdminHelper;
var menuItemMapUnitTest = require("syracuse-collaboration/lib/helpers").menuItemMapUnitTest;
var dataModel = require("syracuse-orm/lib/dataModel");
var registry = require("syracuse-sdata/lib/sdataRegistry");
var mongodb = require('streamline-mongodb');
var jsonImport = require("syracuse-import/lib/jsonImport");
var adminTestFixtures = require("syracuse-collaboration/test/fixtures/adminTestFixtures");
locale.setCurrent(_, "en-US");

//force basic auth
config.session = config.session || {};
config.session.auth = "basic";
//no integration server
config.integrationServer = null;

helpers.pageFileStorage = false;

var tracer; // = console.log;


var port = (config.unit_test && config.unit_test.serverPort) || 3004;
var baseUrl = "http://localhost:" + port;

var done;
QUnit.module(module.id, {
	setup: function() {},
	teardown: function() {}

});



//start syracuse server
asyncTest("initialize syracuse test server", 1, function(_) {
	require('syracuse-main/lib/syracuse').startServers(_, port);
	ok(true, "server initialized");
	start();
});

asyncTest("Test breadcrumb generation ", function(_) {
	var date = new Date();
	var id = "syracuse.collaboration.user";
	var breadCrumb = adminHelper.getBreadcrumb(_, date, id);

	strictEqual(JSON.stringify(breadCrumb), '[{"title":"All"},{"title":"Administration"},{"title":"Administration"},{"title":"Users"}]', "bread crumb for user ok (syracuse collaboration)");
	strictEqual(menuItemMapUnitTest.lastModified && menuItemMapUnitTest.lastModified.getTime(), date.getTime(), "check updated date for menuMap");

	// unknow key
	var breadCrumb = adminHelper.getBreadcrumb(_, date, "");
	strictEqual(JSON.stringify(breadCrumb), '[]', "bread crumb for user ok (syracuse collaboration)");
	strictEqual(menuItemMapUnitTest.lastModified && menuItemMapUnitTest.lastModified.getTime(), date.getTime(), "check updated date for menuMap");

	// id for erp
	id = "x3.erp.GESAUS";
	var breadCrumb = adminHelper.getBreadcrumb(_, date, id);

	strictEqual(JSON.stringify(breadCrumb), '[{"title":"All"},{"title":"Setup"},{"title":"Users"}]', "bread crumb for user ok (erp x3 endpoint)");
	strictEqual(menuItemMapUnitTest.lastModified && menuItemMapUnitTest.lastModified.getTime(), date.getTime(), "check updated date for menuMap");

	id = "syracuse.collaboration.user";
	date = new Date();
	var breadCrumb = adminHelper.getBreadcrumb(_, date, id);

	strictEqual(JSON.stringify(breadCrumb), '[{"title":"All"},{"title":"Administration"},{"title":"Administration"},{"title":"Users"}]', "bread crumb for user ok (syracuse collaboration)");
	strictEqual(menuItemMapUnitTest.lastModified && menuItemMapUnitTest.lastModified.getTime(), date.getTime(), "check updated date for menuMap");

	menuItemMapUnitTest = null;
	start();
});




asyncTest("stop  tests", 0, function(_) {
	done = true;
	start();
});