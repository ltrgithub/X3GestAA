	"collaborationArea": {
		"title": "Teams, documents and storage, Office integration",
		"entities": {
	        "team": {
				"createCondition": {
					"filter": "(administrator ne null and administrator.$uuid eq \"{$profile.user.$uuid}\")"
				},
				"readCondition": {
					"filter": "(administrator ne null and administrator.$uuid eq \"{$profile.user.$uuid}\") or (authors ne null and authors in (\"{$profile.user.$uuid}\")) or (members ne null and members in (\"{$profile.user.$uuid}\"))"
				},
				"updateCondition": {
					"filter": "(administrator ne null and administrator.$uuid eq \"{$profile.user.$uuid}\")"
				},
				"deleteCondition": {
					"deny": true
				},
				"properties": ["description", "isPublic", "explorer", "tags", "administrator", "authors", "members", "documents", "templateDocuments"]
			
			},
	        "document": {
				"createCondition": {
					"filter": "(teams eq null) or (teams ne null and (teams in {$profile.user.adminTeams} or (owner.$uuid eq \"{$profile.user.$uuid}\" and teams in {$profile.user.authorTeams})))"
				},
				"readCondition": {
					"filter": "(teams eq null) or ((teams ne null) and (teams in {$profile.user.adminTeams} or teams in {$profile.user.authorTeams} or teams in {$profile.user.memberTeams}))"
				},
				"updateCondition": {
					"filter": "(teams eq null) or (teams ne null and (teams in {$profile.user.adminTeams} or (owner.$uuid eq \"{$profile.user.$uuid}\" and teams in {$profile.user.authorTeams})))"
				},
				"deleteCondition": {
					"filter": "(teams eq null) or (teams ne null and (teams in {$profile.user.adminTeams} or (teams in {$profile.user.authorTeams} and owner.$uuid eq \"{$profile.user.$uuid}\")))"
				},
				"properties": ["description", "documentType", "documentDate", "fileName", "content", "expiration", "uri", "isReadOnly", "className", "x3Keys", "representationName", "volume", "teams", "owner", "tags", "endpoint"]
			
			},
	        "storageVolume": {
	        	"createCondition":{
	        		"deny" : true
	        	},
	        	"readCondition": {
	        	},
	        	"updateCondition": {
	        		"deny": true
	        	},
	        	"deleteCondition":{
	        		"deny":true
	        	},
	        	"properties": ["code","description","storageType","path","dynamic"]
	        },
	        "storageVolumeItem": {},
	        "storageVolumeQuery": {},
	        "documentTag": {},
	        "documentTagCategory": {},
	        "documentInternalTag": {},
	        "excelBrowseDatasource": {},
	        "excelOrderAttr": {},
	        "msoMailMergeDS": {},
	        "msoMailMergeDSSel": {},
	        "msoMailMergeDocSel": {},
	        "msoReportMode": {},
	        "msoWordTemplateDocument": {},
	        "msoExcelReportMode": {},
	        "msoExcelTemplateDocument": {},
	        "uploadVolumeItem": {},
			"printDocument": {
				"condition": {
					"filter": "owner.$uuid eq \"{$profile.user.$uuid}\""
				},
				"properties": ["description", "documentType", "documentDate", "fileName", "content", "expiration", "owner"]			
	        }
		}
	},
