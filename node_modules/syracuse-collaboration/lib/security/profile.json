{
	"myProfile": {
		"title": "Personal user profile",
		"entities": {
            "cvgSession": {
                "condition": {
                    "filter": "syralogin eq \"{$profile.user.login}\""
                },
                "properties": ["syralogin", "x3solution", "x3folder", "x3user", "x3lang", "lastAccess", "timeout", "reused", "open", "creationDate", "sid", "syraid", "cid"]
            },
            "sessionInfo": {
                "condition": {
                    "filter": "userName eq \"{$profile.user.login}\""
                },
                "properties": ["sid", "userName", "lastAccess", "badge", "clientId", "lastUrl", "x3Sessions"]
            },
			"user": {
				"condition": {
					"filter": "$uuid eq \"{$profile.user.$uuid}\""
				},
				"properties": ["login", "title", "firstName", "lastName", "fullName", "password", "oldPassword", "photo", "email", "explorer", "changePassword", "infov6", "userv6", "passwordv6"]
			},
			"role": {
				"createCondition": {
					"deny": true
				},
				"readCondition": {
					"filter": "$uuid in {$profile.roles}"
				},
				"updateCondition": {
					"deny": true
				},
				"deleteCondition": {
					"deny": true
				},
				"properties": ["code", "description"]
			},
			"endPoint": {
				"createCondition": {
					"deny": true
				},
				"readCondition": {
					"filter": "$uuid in {$profile.endpoints}"
				},
				"updateCondition": {
					"deny": true
				},
				"deleteCondition": {
					"deny": true
				},
				"properties": ["description", "dataset", "application", "contract"]
			},
			"localePreference": {
				"createCondition": {
					"deny": true
				},
				"readCondition": {
				},
				"updateCondition": {
					"deny": true
				},
				"deleteCondition": {
					"deny": true
				}
			},
			"page": {},
			"portlet": {
				"readCondition": {
					"allow": true
				},
				"executeCondition": {
					"allow": true
				}
			},
			"dashboardDef": {
				"readCondition": {
					"allow": true
				},
				"executeCondition": {
					"allow": true
				}
			},
			"traceRecord": {
			    "readCondition": {
                    "filter": "$creUser eq \"{$profile.user.login}\""
                },
                "properties": ["name", "stamp", "status", "ticket", "transcript", "contents", "configs", "sessionId", "isSessionConcerned"]
            },
			"traceContent": {
			    "readCondition": {
                    "filter": "$creUser eq \"{$profile.user.login}\""
                },
                "properties": ["start", "size"]
            }			
		},
		"entityServices": {
			"traceRecord": {
				"download": {
					"condition": {
						"deny": true
					}
				}
			}
		}
	},
	"users": {
		"title": "Users, groups and roles management",
