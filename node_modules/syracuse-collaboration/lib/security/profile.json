{
	"myProfile": {
		"title": "Personal user profile",
		"entities": {
			"user": {
				"condition": {
					"filter": "$uuid eq \"{$profile.user.$uuid}\""
				},
				"properties": ["login", "title", "firstName", "lastName", "fullName", "password", "oldPassword", "photo", "email", "explorer", "changePassword"]
			},
			"role": {
				"createCondition": {
					"deny": true
				},
				"readCondition": {
					"filter": "$uuid in {$profile.roles}"
				},
				"updateCondition": {
					"deny": true
				},
				"deleteCondition": {
					"deny": true
				},
				"properties": ["description"]
			},
			"endPoint": {
				"createCondition": {
					"deny": true
				},
				"readCondition": {
					"filter": "$uuid in {$profile.endpoints}"
				},
				"updateCondition": {
					"deny": true
				},
				"deleteCondition": {
					"deny": true
				},
				"properties": ["description", "dataset"]
			},
			"localePreference": {
				"createCondition": {
					"deny": true
				},
				"readCondition": {
				},
				"updateCondition": {
					"deny": true
				},
				"deleteCondition": {
					"deny": true
				},
				"properties": ["code", "description"]
			},
			"page": {}
		}
	},
