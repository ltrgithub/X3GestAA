                }
			},
			"user": {},
	        "securityProfile": {
			    "readCondition": {
                    "filter": "securityLevel gt {$profile.securityProfile.level} or code eq {$profile.securityProfile.code}"
                },
			    "createCondition": {
                    "filter": "securityLevel gt {$profile.securityProfile.level}"
                },
			    "updateCondition": {
                    "filter": "securityLevel gt {$profile.securityProfile.level}"
                },
			    "deleteCondition": {
                    "filter": "securityLevel gt {$profile.securityProfile.level}"
                }
	        },
	        "securityProfileItem": {},
			"team": {},
			"endPoint": {
				"readCondition": {
