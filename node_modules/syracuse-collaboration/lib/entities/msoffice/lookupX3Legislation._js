"use strict";

var util = require('./util');

exports.entity = {
	$properties: {
		LEG: {
			$title: "Legislation"
		}
	},
	$fetchInstances: function(_, context, parameters) {
		var self = this;
		var codes = [];
		var res = util.query(_, context, parameters, "LEG", "$query", function(_, inst) {
			var instance = self.factory.createInstance(_, null, context.db, context);
			instance.LEG(_, inst.LEG(_));
			codes.push(instance);
		});
		return codes;
	},
	$defaultOrder: [["LEG", true]]
}