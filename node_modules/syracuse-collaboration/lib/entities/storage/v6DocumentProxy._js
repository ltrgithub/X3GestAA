"use strict";

exports.entity = {
	$isPersistent: false,
	$properties: {
		content: {
			$title: "Document",
			$type: "binary",
			$storage: function(_, instance) {
				return "x3";
			},
			$url: function(_, instance) {
				return instance._contentUrl;
			}
		}
	},
	$functions: {
		$setParameters: function(_, context) {
			var self = this;
			var params = (context && context.parameters) || {};
			self._contentUrl = params.v6DocUrl;
		},
		$setId: function(_, context, id) {
			var self = this;
			var params = (context && context.parameters) || {};
			self._contentUrl = params.v6DocUrl;
		}
	}
};