var SEQFILE_TYPES = [
	["sepField", 1],
	["sepRecord", 2],
	["delimited", 3],
	["fixedLength", 4],
	["xml", 5],
	["flat", 6],
	["header", 7]
];
exports.SEQFILE = SEQFILE_TYPES.reduce(function(types, desc) {
	types[desc[0]] = desc[1];
	return types;
}, {});

exports.SEQFILE_LIB = SEQFILE_TYPES.reduce(function(types, desc) {
	types[desc[1]] = desc[0];
	return types;
}, {});

