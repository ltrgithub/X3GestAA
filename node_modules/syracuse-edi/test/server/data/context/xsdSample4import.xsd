<xs:schema xmlns:tns="http://schemas.sage.com/sc/2009" xmlns:sme="http://schemas.sage.com/sdata/sme/2007" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.sage.com/sc/2009" elementFormDefault="qualified" targetNamespace="http://schemas.sage.com/sc/2009" id="sc">
    <xs:element sme:role="resourceKind" sme:pluralName="operatingCompanies" sme:compliance="must" sme:canGet="true" name="operatingCompany" type="operatingCompany--type" />
    <xs:complexType name="operatingCompany--type">
        <xs:all>
            <xs:element sme:label="Operating Company Application ID" sme:isUniqueKey="true" minOccurs="0" name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The application specific identifier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Active Flag" minOccurs="0" name="active" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if the Company is an ‘active’ Company . Default = True
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Deleted" minOccurs="0" name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if Company is (soft) deleted.  Default = False
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Label" minOccurs="0" name="label" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The label for the resource to be presented in the user interface. To support application-specific or language-specific descriptions of resources. “Company”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Name" sme:isMandatory="true" minOccurs="0" name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The entity name (e.g. “Homestyle Kitchens”)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Short Name" minOccurs="0" name="shortName" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A shorter name for the Company.

                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Currency" minOccurs="0" name="currency" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The Company currency (e.g. GBP, USD, EUR).

                        ISO 4217.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Identifier" sme:isUniqueKey="true" minOccurs="0" name="identifier" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier(s) of the Company as used by the third parties (e.g. Company Registration Number, DUNS number, 1099 type, CIS number, siret number, Trading Number, etc).

                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Tax Locale" minOccurs="0" name="taxLocale" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The tax-locale applicable to the Operating Company (e.g. District)

                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Tax reference" sme:isUniqueKey="true" minOccurs="0" name="taxReference" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Tax reference associated with the Company (e.g. VAT number, numero intracommnunautaire, FEIN)

                        NIF (Sp)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Operating Company Language" sme:isExtension="true" minOccurs="0" name="language" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Language used by the Company (e.g. EN-GB)

                        ISO 639-1
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Postal Addresses" sme:isCollection="true" sme:relationship="association" name="postalAddresses" type="postalAddress--list"></xs:element>
            <xs:element sme:label="Phone Numbers" sme:isCollection="true" sme:relationship="association" name="phoneNumbers" type="phoneNumber--list"></xs:element>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="operatingCompany--list">
        <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="operatingCompany" type="operatingCompany--type" />
        </xs:sequence>
    </xs:complexType>
    <xs:element sme:role="resourceKind" sme:pluralName="people" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" name="person" type="person--type" />
    <xs:complexType name="person--type">
        <xs:all>
            <xs:element sme:label="Person Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
                <xs:annotation>
                    <xs:documentation>The application specific identifier.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    <xs:element sme:role="resourceKind" sme:pluralName="postalAddresses" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" sme:isSyncSource="true" sme:isSyncTarget="true" name="postalAddress" type="postalAddress--type" />
    <xs:complexType name="postalAddress--type">
        <xs:all>
            <xs:element sme:label="PostalAddress Global ID" sme:isUniqueKey="true" sme:isGlobalId="true" minOccurs="0" name="uuid" type="uuid--type">
                <xs:annotation>
                    <xs:documentation> PostalAddress Global ID Globally unique identifier </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
                <xs:annotation>
                    <xs:documentation> The application specific identifier.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Active Flag" minOccurs="0" name="active" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if the Postal Address is an ‘active’ one .   Default =
                        True
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Deleted" minOccurs="0" name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if Postal Address is (soft) deleted.   Default = False
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Label" minOccurs="0" name="label" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The label for the resource to be presented in the user interface. To
                        support application-specific or language-specific descriptions of resources.   “Postal
                        Address”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Reference" minOccurs="0" name="reference" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Reference for the Postal Address.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Name" minOccurs="0" name="name" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A name for the Postal Address (e.g. “Billing Address”).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Description" minOccurs="0" name="description" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A longer description of the postal Postal Address.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Address 1" minOccurs="0" name="address1" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> 1st Line of Postal Address   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Address 2" minOccurs="0" name="address2" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> 2nd Line of Postal Address   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Address 3" minOccurs="0" name="address3" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> 3rd Line of Postal Address   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Address 4" minOccurs="0" name="address4" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> 4th Line of Postal Address   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress TownCity" minOccurs="0" name="townCity" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Town or City   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress County" minOccurs="0" name="county" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> County   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress StateRegion" minOccurs="0" name="stateRegion" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        State/Region   For USA addresses use ISO 3166-2:US for state codes. For
                        UK addresses use ISO 3166-2:GB. For French addresses use ISO 3166-2:FR. For German
                        addresses use ISO 3166-2:DE. For Canada addresses use ISO 3166-2:CA.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress ZipPost Code" minOccurs="0" name="zipPostCode" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Zip or Post code. Should be formatted as raw text no formatting
                        included.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Country" minOccurs="0" name="country" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Country   ISO 3166-1 alpha-2 country codes only </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Primacy Indicator" minOccurs="0" name="primacyIndicator" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        A flag to indicate if the Address is the primary/default Address.   One
                        ony Postal Address can have Primacy against an associated entity (e.g. Contact, Company,
                        etc). Default = False
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Type" sme:isMandatory="true" minOccurs="1" name="type" type="postalAddressType--enum">
                <xs:annotation>
                    <xs:documentation>
                        The type of the Postal Address, e.g., billing, sales address, home,
                        etc.   A new Postal Address will be required for each type, even though the address
                        itself may be the same. Default = “Correspondance”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PostalAddress Note" sme:relationship="child" sme:isCollection="true" minOccurs="0" name="note" nillable="true" type="note--list">
                <xs:annotation>
                    <xs:documentation> Notes associated with the Postal Address.   </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="postalAddress--list">
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="postalAddress" type="postalAddress--type" />
        </xs:sequence>
    </xs:complexType>
    <xs:element sme:role="resourceKind" sme:pluralName="emails" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" sme:isSyncSource="true" sme:isSyncTarget="true" name="email" type="email--type" />
    <xs:complexType name="email--type">
        <xs:all>
            <xs:element sme:label="Email Global ID" sme:isUniqueKey="true" minOccurs="0" name="uuid" type="uuid--type">
                <xs:annotation>
                    <xs:documentation>
                        The Email unique identifier.   Globally unique identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
                <xs:annotation>
                    <xs:documentation> The application specific identifier.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Active Flag" minOccurs="0" name="active" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if the Email is an ‘active’ one .   Default = True
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Deleted" minOccurs="0" name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if email is (soft) deleted.   Default = False
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Label" minOccurs="0" name="label" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The label for the resource to be presented in the user interface. To
                        support application-specific or language-specific descriptions of resources.   “Email”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email reference" sme:isUniqueKey="true" minOccurs="0" name="reference" type="xs:string">
                <xs:annotation>
                    <xs:documentation> reference associated with the Email.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Reference2" sme:isExtension="true" minOccurs="0" name="reference2" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Another reference associated with the email.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Type" minOccurs="0" name="type" type="emailType--enum">
                <xs:annotation>
                    <xs:documentation>
                        A simple string value used to label this email address. It allows for
                        email addresses such as "Work", "Personal", "Preferred”, etc. Can also be used to
                        distinguish ‘instant messaging’ type addresses.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Address" sme:isMandatory="true" minOccurs="0" name="address" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The Email address itself (e.g. joe.bloggs@acme.com).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="mailto:URL" sme:isUniqueKey="true" minOccurs="0" name="uRL" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        URL that allows an email to be automatically generated that is to be
                        set to the email address.   Could be created automatically.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Primacy Indicator" minOccurs="0" name="primacyIndicator" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if this Email is the primary one associated with the
                        linked resource.   Default = “False”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Email Note" sme:relationship="child" sme:isCollection="false" minOccurs="0" name="notes" nillable="true" type="note--type">
                <xs:annotation>
                    <xs:documentation> Notes associated with email.   </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="email--list">
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="email" type="email--type" />
        </xs:sequence>
    </xs:complexType>
    <xs:element sme:role="resourceKind" sme:pluralName="phoneNumbers" sme:compliance="must" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:canDelete="true" sme:isSyncSource="true" sme:isSyncTarget="true" name="phoneNumber" type="phoneNumber--type" />
    <xs:complexType name="phoneNumber--type">
        <xs:all>
            <xs:element sme:label="PhoneNumber Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
                <xs:annotation>
                    <xs:documentation> The application specific identifier.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Active Flag" minOccurs="0" name="active" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if the phone number is an ‘active’ one .   Default =
                        True
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Deleted" minOccurs="0" name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if phone number is (soft) deleted.   Default = False
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Label" minOccurs="0" name="label" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The label for the resource to be presented in the user interface. To
                        support application-specific or language-specific descriptions of resources.   “Phone
                        Number”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber reference" sme:isUniqueKey="true" minOccurs="0" name="reference" type="xs:string">
                <xs:annotation>
                    <xs:documentation> reference associated with the phone number.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Reference2" sme:isExtension="true" minOccurs="0" name="reference2" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Another reference associated with the phone number.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber URI" sme:isUniqueKey="true" minOccurs="0" name="uRI" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A phone unique reference number used to represent the number in a
                        formal way. Useful for programmatic access, such as a VoIP/PSTN/CTI.   Could be derived
                        by concatenating attributes.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Type" minOccurs="0" name="type" type="phoneNumberType--enum">
                <xs:annotation>
                    <xs:documentation>
                        A value that identifies the type of phone number (general, fax, car,
                        home, internal, mobile, other, pager, satellite, voip, work).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Name" minOccurs="0" name="name" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A simple string value used to label this phone number. In most cases,
                        “Label” is not necessary as “Phone Type” uniquely identifies a number and allows UIs to
                        display a proper label such as "Mobile", "Home", "Work", etc. However, in the case where
                        one person has (for example) multiple mobile phones, this property can be used to
                        disambiguate them.   Could be derived from Phone Type
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Text" sme:isMandatory="true" minOccurs="0" name="text" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Human-readable phone number, containing all attributes.   Could be
                        derived by concatenating other properties.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Country Code" minOccurs="0" name="countryCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Country code for the phone number based on ITU-T recommendation E.164
                        country codes.   ITU-T E. 164 country codes only.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Area Code" minOccurs="0" name="areaCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Area code for the phone number   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Number" minOccurs="0" name="number" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Telephone number.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Extension" minOccurs="0" name="extension" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Any extension applicable to the phone number.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Primacy Indicator" minOccurs="0" name="primacyIndicator" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if this Phone Number is the primary one associated
                        with the linked resource.   Default = “False”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="PhoneNumber Note" sme:relationship="child" sme:isCollection="true" minOccurs="0" name="notes" nillable="true" type="note--list">
                <xs:annotation>
                    <xs:documentation> Notes associated with phone number.   </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="phoneNumber--list">
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="phoneNumber" type="phoneNumber--type" />
        </xs:sequence>
    </xs:complexType>
    <xs:element sme:role="resourceKind" sme:pluralName="notes" sme:compliance="may" sme:canGet="true" name="note" type="note--type" />
    <xs:complexType name="note--type">
        <xs:all>
            <xs:element sme:label="Note Global ID" sme:isUniqueKey="true" minOccurs="0" name="uuid" type="uuid--type">
                <xs:annotation>
                    <xs:documentation>
                        The note unique identifier.   Globally unique identifier
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
                <xs:annotation>
                    <xs:documentation> The application specific identifier.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Active Flag" minOccurs="0" name="active" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if the note is an ‘active’ one .   Default = True
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Deleted" minOccurs="0" name="deleted" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if note is (soft) deleted.   Default = False
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Label" minOccurs="0" name="label" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The label for the resource to be presented in the user interface. To
                        support application-specific or language-specific descriptions of resources.   “Note”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note reference" sme:isUniqueKey="true" minOccurs="0" name="reference" type="xs:string">
                <xs:annotation>
                    <xs:documentation> reference associated with the note.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="NoteReference2" sme:isExtension="true" minOccurs="0" name="reference2" nillable="true" type="xs:string">
                <xs:annotation>
                    <xs:documentation> Another reference associated with the note.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Text" sme:isMandatory="true" minOccurs="0" name="text" type="xs:string">
                <xs:annotation>
                    <xs:documentation> The note text itself.   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Privacy Flag" minOccurs="0" name="privacyFlag" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if the note is public or private.   Default = “No”
                        (not private).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Date Stamp" minOccurs="0" name="dateStamp" type="xs:date">
                <xs:annotation>
                    <xs:documentation> Date and time of note   </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element sme:label="Note Primacy Indicator" minOccurs="0" name="primacyIndicator" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Flag to indicate if this Note is the primary one associated with the
                        linked resource.   Default = “False”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
