  </xs:complexType>
  <xs:element sme:role="resourceKind" sme:pluralName="salesReturnLines" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:isSyncSource="true" name="salesReturnLine" type="salesReturnLine--type" />
  <xs:complexType name="salesReturnLine--type">
    <xs:all>
      <xs:element sme:label="SalesReturnLine Global ID" sme:isUniqueKey="true" minOccurs="0" name="uuid" type="sc:uuid--type">
        <xs:annotation>
          <xs:documentation>
