          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturn Return Additional Discount1 Amount" minOccurs="0" name="returnAdditionalDiscount1Amount" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
