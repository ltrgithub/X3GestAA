  <xs:element sme:role="resourceKind" sme:pluralName="salesInvoices" sme:compliance="should" sme:canGet="true" sme:canPut="true" sme:canPost="true" sme:isSyncSource="true" name="salesInvoice" type="salesInvoice--type" />
