          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseRequisition Seller Contact" sme:relationship="reference" sme:isCollection="false" minOccurs="0" name="sellerContact" nillable="true" type="contact--type">
