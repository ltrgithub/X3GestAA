        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseInvoice Type" minOccurs="0" name="type" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation> Type of Purchase Invoice.   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseInvoice Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false" minOccurs="0" name="tradingAccount" type="tradingAccount--type">
        <xs:annotation>
