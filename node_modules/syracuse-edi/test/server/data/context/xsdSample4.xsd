      <xs:element sme:label="PurchaseRequisition Order Additional Discount2 Amount" sme:isExtension="true" minOccurs="0" name="orderAdditionalDiscount2Amount" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            Discount applicable to the Purchase Requisition expressed as an amount.
             
          </xs:documentation>
        </xs:annotation>
