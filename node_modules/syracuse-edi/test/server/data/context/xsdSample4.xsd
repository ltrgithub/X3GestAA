          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="Receipt ProjectLine" sme:relationship="association" sme:isCollection="true" minOccurs="0" name="projectLines" nillable="true" type="projectLine--list">
        <xs:annotation>
          <xs:documentation>
            Project Line value for this receipt.   See “ProjectLine” definition.
            Receipt Line values may be used instead. Some applications may not have Project Lines.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="Receipt Interaction" sme:relationship="association" sme:isCollection="true" minOccurs="0" name="interactions" nillable="true" type="interaction--list">
        <xs:annotation>
          <xs:documentation>
            Interaction(s) associated with the receipt.   See “Interaction”
            definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="Receipt Note" sme:relationship="child" sme:isCollection="true" minOccurs="0" name="notes" nillable="true" type="sc:note--list">
        <xs:annotation>
          <xs:documentation> Notes associated with the Receipt.   </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
