      <xs:element sme:label="SalesQuotationLine Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
        <xs:annotation>
          <xs:documentation> The application specific identifier.   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="Sales Quotation" sme:relationship="parent" sme:isCollection="false" minOccurs="0" name="salesQuotation" type="salesQuotation--type">
        <xs:annotation>
          <xs:documentation> The parent Sales Quotation </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesQuotationLine Label" minOccurs="0" name="label" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            The label for the resource to be presented in the user interface. To
