            fulfilled.   See “Location” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseReturnDeliveryLine Fulfilment Sub-Location" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" minOccurs="0" name="fulfillmentSubLocation" nillable="true" type="location--type">
        <xs:annotation>
          <xs:documentation>
