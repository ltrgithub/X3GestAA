    <xs:all>
      <xs:element sme:label="PurchaseInvoice Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
        <xs:annotation>
          <xs:documentation> The application specific identifier.   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseInvoice Active Flag" minOccurs="0" name="active" type="xs:boolean">
