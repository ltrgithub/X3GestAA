      <xs:element sme:label="PurchaseOrderLine PaymentLine" sme:relationship="association" sme:isCollection="true" minOccurs="0" name="paymentLines" nillable="true" type="paymentLine--list">
        <xs:annotation>
          <xs:documentation>
