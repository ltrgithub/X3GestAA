          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseRequisition Delivery Terms" minOccurs="0" name="deliveryTerms" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation> The terms for Delivery.   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseRequisition Delivery Date" minOccurs="0" name="deliveryDate" nillable="true" type="xs:date">
        <xs:annotation>
