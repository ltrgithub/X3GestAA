      <xs:element sme:label="PurchaseInvoice Carrier Tax Price" minOccurs="0" name="carrierTaxPrice" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            The tax on Delivery/carriage.   In Invoice currency.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseInvoice Carrier Gross Price" minOccurs="0" name="carrierTotalPrice" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
