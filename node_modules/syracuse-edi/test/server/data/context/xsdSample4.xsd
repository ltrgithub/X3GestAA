          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturn Supplier TradingAccount" sme:relationship="reference" sme:isCollection="false" minOccurs="0" name="supplierTradingAccount" nillable="true" type="tradingAccount--type">
