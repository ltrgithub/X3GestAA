        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesCreditLine PaymentLine" sme:relationship="association" sme:isCollection="true" minOccurs="0" name="paymentLines" nillable="true" type="paymentLine--list">
        <xs:annotation>
          <xs:documentation>
            A link to the (cash) payment line(s) associated with this Sales Credit
            Line   See “PaymentLine” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesCreditLine FinancialAccount" sme:relationship="association" sme:isCollection="true" minOccurs="0" name="financialAccounts" nillable="true" type="sc:financialAccount--list">
        <xs:annotation>
          <xs:documentation>
