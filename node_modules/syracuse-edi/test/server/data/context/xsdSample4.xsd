        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseReturnDeliveryLine Commodity Dimension" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" minOccurs="0" name="commodityDimension" nillable="true" type="commodityDimension--type">
        <xs:annotation>
