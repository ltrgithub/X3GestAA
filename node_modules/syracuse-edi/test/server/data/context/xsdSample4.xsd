          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Carrier SalesInvoice" sme:relationship="reference" sme:isCollection="false" sme:isExtension="true" minOccurs="0" name="carrierSalesInvoiceLine" nillable="true" type="salesInvoice--type">
