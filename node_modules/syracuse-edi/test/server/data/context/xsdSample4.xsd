      <xs:element sme:label="PurchaseInvoice Carrier Tax Code" sme:relationship="association" sme:isCollection="true" sme:isExtension="true" minOccurs="0" name="carrierTaxCodes" nillable="true" type="taxCode--list">
        <xs:annotation>
          <xs:documentation>
