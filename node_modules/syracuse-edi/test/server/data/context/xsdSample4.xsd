        <xs:annotation>
          <xs:documentation>
            The quantity of the Commodity Returned on the Sales Return Line.  
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Initial Price" minOccurs="0" name="initialPrice" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            The initial price of the Commodity Returned on the Sales Return Line
            (e.g. before discounts).   In Return currency.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine PriceList" sme:relationship="reference" sme:isCollection="false" minOccurs="0" name="pricelist" nillable="true" type="priceList--type">
        <xs:annotation>
          <xs:documentation>
            A reference to the Price List used to derive the price on the Sales
            Return Line.   See “PriceList” definition.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Return Line Discount Type" minOccurs="0" name="returnLineDiscountType" nillable="true" type="discountType--enum">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate the type of discount applicable to this Sales Return
            Line (percent or amount).   From the Sales Return.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Return Line Discount Amount" minOccurs="0" name="returnLineDiscountAmount" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            Discount applicable to the Sales Return Line, expressed as an amount.  
            From the Sales Return.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Return Line Discount Percent" minOccurs="0" name="returnLineDiscountPercent" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            Discount applicable to the Sales Return Line, expressed as a
            percentage.   From the Sales Return.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Return Line Subtotal Discount Type" sme:isExtension="true" minOccurs="0" name="returnLineSubtotalDiscountType" nillable="true" type="discountType--enum">
        <xs:annotation>
          <xs:documentation>
            Flag to indicate the type of discount applicable to a subtotal of Sales
            Return Lines (percent or amount) of which this Line is part.  
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Return Line Subtotal Discount Amount" sme:isExtension="true" minOccurs="0" name="returnLineSubtotalDiscountAmount" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            Discount applicable to the Sales Return Line, expressed as an amount.  
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesReturnLine Return Line Subtotal Discount Percent" sme:isExtension="true" minOccurs="0" name="returnLineSubtotalDiscountPercent" nillable="true" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            Discount applicable to the Sales Return Line, expressed as a
            percentage.  
          </xs:documentation>
        </xs:annotation>
      </xs:element>
