  <xs:element sme:role="resourceKind" sme:pluralName="receipts" sme:compliance="may" sme:canGet="true" name="receipt" type="receipt--type" />
  <xs:complexType name="receipt--type">
    <xs:all>
      <xs:element sme:label="Receipt Application ID" sme:isUniqueKey="true" minOccurs="0" name="applicationID" type="xs:string">
        <xs:annotation>
          <xs:documentation> The application specific identifier.   </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="Receipt Active Flag" minOccurs="0" name="active" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>
