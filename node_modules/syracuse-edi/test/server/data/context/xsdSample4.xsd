        <xs:annotation>
          <xs:documentation>
            Discount applicable to the Sales Invoice if the Sales Invoice is paid
            within a certain timeframe, expressed as a percentage.   From the Trading Account.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="SalesInvoice Settlement Discount Terms" minOccurs="0" name="settlementDiscountTerms" nillable="true" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            Terms applicable to the settlement discount, often expressed as a
            number of days.   From the Trading Account.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
