          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseRequisition PriceList" sme:relationship="reference" sme:isCollection="false" minOccurs="0" name="pricelist" nillable="true" type="priceList--type">
