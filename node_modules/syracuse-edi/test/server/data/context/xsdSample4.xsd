          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseInvoice Supplier TradingAccount Currency Exchange Rate Date" minOccurs="0" name="supplierTradingAccountCurrencyExchangeRateDate" nillable="true" type="xs:date">
