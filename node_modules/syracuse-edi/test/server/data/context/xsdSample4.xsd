        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseReturnDeliveryLine Delivered Quantity" minOccurs="0" name="deliveredQuantity" type="xs:decimal">
        <xs:annotation>
          <xs:documentation>
            The quantity of the Commodity delivered on the Purchase Return Delivery
            Line.  
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseReturnDeliveryLine Note" sme:relationship="child" sme:isCollection="false" minOccurs="0" name="notes" nillable="true" type="sc:note--type">
        <xs:annotation>
          <xs:documentation>
