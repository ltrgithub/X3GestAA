          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element sme:label="PurchaseRequisition Supplier TradingAccount Currency Exchange Rate" minOccurs="0" name="supplierTradingAccountCurrencyExchangeRate" nillable="true" type="xs:decimal">
