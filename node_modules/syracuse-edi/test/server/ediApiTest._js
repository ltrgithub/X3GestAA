			"seqFile": {}
		},
		type: EdiType.SEQFILE,
		id: "test",
		etag: "etagDummy",
		db: db
	}));
	uuid.push(EdiEntity.createEdiCacheEntity(_, {
		json: {
			"protocol": {}
		},
		type: EdiType.PROTOCOL,
		id: "test",
		etag: "etagDummy",
