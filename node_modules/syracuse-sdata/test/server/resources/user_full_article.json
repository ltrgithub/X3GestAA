{
   "$layout":{
      "$items":[
         {
            "$category":"section",
            "$layout":{
               "$items":[
                  {
                     "$layoutType":"row",
                     "$items":[
                        {
                           "$items":[
                              {
                                 "$title":"{@LoginSectionTitle1} {@LoginSectionTitle2} section",
                                 "$category":"block",
                                 "$layout":{
                                    "$items":[
                                       {
                                          "$bind":"login"
                                       },
                                       {
                                          "$bind":"password"
                                       }
                                    ]
                                 }
                              },
                              {
                                 "$title":"{@InformationsSectionTitle}",
                                 "$category":"block",
                                 "$layout":{
                                    "$items":[
                                       {
                                          "$bind":"title"
                                       },
                                       {
                                          "$bind":"firstName"
                                       },
                                       {
                                          "$bind":"lastName"
                                       },
                                       {
                                          "$bind":"email"
                                       },
                                       {
                                          "$bind":"photo"
                                       },
                                       {
                                          "$bind":"active"
                                       }
                                    ]
                                 }
                              }
                           ]
                        },
                        {
                           "$items":[
                              {
                                 "$title":"{@ExplorerSectionTitle}",
                                 "$category":"block",
                                 "$layout":{
                                    "$items":[
                                       {
                                          "$bind":"explorer",
                                          "$isTitleHidden":true
                                       }
                                    ]
                                 }
                              }
                           ]
                        }
                     ]
                  },
                  {
                     "$title":"{@AdministrationSectionTitle}",
                     "$category":"block",
                     "$layout":{
                        "$layoutType":"row",
                        "$items":[
                           {
                              "$items":[
                                 {
                                    "$bind":"groups"
                                 },
                                 {
                                    "$bind":"endpoints"
                                 }
                              ]
                           },
                           {
                              "$items":[
                                 {
                                    "$bind":"adminTeams"
                                 },
                                 {
                                    "$bind":"authorTeams"
                                 },
                                 {
                                    "$bind":"memberTeams"
                                 }
                              ]
                           }
                        ]
                     }
                  },
                  {
                     "$title":"Personalisations",
                     "$category":"block",
                     "$layout":{
                        "$layoutType":"row",
                        "$items":[
                           {
                              "$items":[
                                 {
                                    "$bind":"defaultRole"
                                 },
                                 {
                                    "$bind":"defaultLocale"
                                 }
                              ]
                           }
                        ]
                     }
                  },
                  {
                     "$title":"Comments",
                     "$category":"block",
                     "$layout":{
                        "$items":[
                           {
                              "$bind":"comments",
                              "$isTitleHidden":true
                           },
                           {
                              "$bind":"comments2",
                              "$isTitleHidden":true
                           }
                        ]
                     }
                  },
                  {
                     "$title":"{@LocalesSectionTitle}",
                     "$category":"block",
                     "$layout":{
                        "$items":[
                           {
                              "$bind":"locales",
                              "$isTitleHidden":true,
                              "$format":"cards",
                               "$layout":{
                                    "$layoutType":"row",
                                    "$items":[
                                       {
                                          "$layoutType":"stack",
                                          "$items":[
                                             {
                                                "$bind":"code"
                                             },
                                             {
                                                "$bind":"description"
                                             },
                                             {
                                                "$bind":"enabled"
                                             },
                                             {
                                                "$bind":"shortDate"
                                             },
                                             {
                                                "$bind":"shortTime"
                                             },
                                             {
                                                "$bind":"shortDatetime"
                                             }
                                          ]
                                       },
                                       {
                                          "$layoutType":"stack",
                                          "$items":[
                                             {
                                                "$bind":"longDate"
                                             },
                                             {
                                                "$bind":"longTime"
                                             },
                                             {
                                                "$bind":"longDatetime"
                                             },
                                             {
                                                "$bind":"firstDayOfWeek"
                                             },
                                             {
                                                "$bind":"twoDigitYearMax"
                                             }
                                          ]
                                       },
                                       {
                                          "$layoutType":"stack",
                                          "$items":[
                                             {
                                                "$bind":"numberDecimalSeparator"
                                             },
                                             {
                                                "$bind":"numberGroupSeparator"
                                             },
                                             {
                                                "$bind":"numberGroupSize"
                                             }
                                          ]
                                       }
                                    ]
                                 }
                           }
                        ]
                     }
                  }
               ]
            }
         }
      ]
   }
}