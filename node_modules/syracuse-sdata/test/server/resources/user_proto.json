{
  "$descriptor": "prototype user.$details",
  "$type": "application/json",
  "$baseType": "application/json;vnd.sage=syracuse",
  "$baseUrl": "http://localhost:8124/sdata/syracuse/collaboration/syracuse",
  "$url": "{$baseUrl}/{$pluralType}('{$key}')",
  "$value": "{firstName} {lastName}",
  "$title": "User",
  "$description": "{firstName} {lastName} user profile",
  "$pluralType": "users",
  "$representation": "user",
  "$": {
    "login": {
      "$title": "Default Account",
      "$type": "application/x-string",
      "$constraints": {
        "$isUnique": true
      },
      "$isMandatory": true,
      "$capabilities": "sort,filter,alphaTab"
    },
    "title": {
      "$title": "Title",
      "$type": "application/x-choice",
      "$value": {
        "$type": "application/x-string",
        "$constraints": {
          "$enum": [{
            "$value": "mr",
            "$title": "Mr."
          }, {
            "$value": "mrs",
            "$title": "Mme."
          }]
        }
      },
      "$capabilities": "sort,filter,alphaTab"
    },
    "firstName": {
      "$title": "First Name",
      "$type": "application/x-string",
      "$constraints": {},
      "$capabilities": "sort,filter,alphaTab"
    },
    "lastName": {
      "$title": "Last Name",
      "$type": "application/x-string",
      "$constraints": {},
      "$isMandatory": true,
      "$capabilities": "sort,filter,alphaTab",
      "$links": {
        "$details": {
          "$type": "application/json;vnd.sage=syracuse",
          "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$representation}.$details&role={$role}"
        }
      }
    },
    "fullName": {
      "$title": "Full Name",
      "$type": "application/x-string",
      "$constraints": {},
      "$isDisabled": true,
      "$capabilities": "sort,filter,alphaTab"
    },
    "active": {
      "$title": "Active",
      "$type": "application/x-boolean",
      "$constraints": {},
      "$capabilities": "sort,filter"
    },
    "authentication": {
      "$title": "Authentication",
      "$type": "application/x-choice",
      "$value": {
        "$type": "application/x-string",
        "$constraints": {
          "$enum": [{
            "$value": "",
            "$title": "Standard"
          }, {
            "$value": "db",
            "$title": "DB"
          }, {
            "$value": "ldap",
            "$title": "LDAP"
          }, {
            "$value": "oauth2",
            "$title": "OAuth2"
          }]
        }
      },
      "$capabilities": "sort,filter,alphaTab"
    },
    "authenticationName": {
      "$title": "Authentication name",
      "$type": "application/x-string",
      "$constraints": {},
      "$capabilities": "sort,filter,alphaTab"
    },
    "email": {
      "$title": "Email",
      "$type": "application/x-string",
      "$format": "$email",
      "$constraints": {},
      "$capabilities": "sort,filter,alphaTab"
    },
    "photo": {
      "$title": "Photo",
      "$type": "image",
      "$url": "{$baseUrl}/{$pluralType}('{$key}')/photo",
      "$constraints": {},
      "$capabilities": ""
    },
    "explorer": {
      "$title": "Explorer",
      "$type": "application/x-graph",
      "$format": "d3.nodeChart",
      "$constraints": {},
      "$capabilities": "sort,filter",
      "$url": "{$baseUrl}/{$pluralType}('{$key}')/$graphs/explorer",
      "$linkCategories": [{
        "$name": "groups",
        "$title": "Groups"
      }, {
        "$name": "groups.endPoints",
        "$title": "Endpoints"
      }, {
        "$name": "groups.endPoints.applicationRef",
        "$title": "Application"
      }, {
        "$name": "groups.role",
        "$title": "Role"
      }, {
        "$name": "adminTeams",
        "$title": "Teams administrator"
      }, {
        "$name": "authorTeams",
        "$title": "Teams author"
      }, {
        "$name": "memberTeams",
        "$title": "Teams member"
      }],
      "$nodeCategories": [{
        "$name": "user",
        "$title": "User",
        "$selected": true,
        "$prototype": {
          "$": {
            "$value": "{firstName} {lastName}"
          }
        }
      }, {
        "$name": "group",
        "$title": "Group",
        "$selected": true,
        "$prototype": {
          "$": {
            "$value": "{description}"
          },
          "$links": {
            "$default": {
              "$url": "{$baseUrl}/groups('{$uuid}')?representation=group.$details",
              "$target": "blank"
            }
          }
        }
      }, {
        "$name": "endPoint",
        "$title": "Endpoint",
        "$selected": true,
        "$prototype": {
          "$": {
            "$value": "{description}"
          },
          "$links": {
            "$default": {
              "$url": "{$baseUrl}/endPoints('{$uuid}')?representation=endPoint.$details",
              "$target": "blank"
            }
          }
        }
      }, {
        "$name": "application",
        "$title": "Applications",
        "$selected": false,
        "$prototype": {
          "$": {
            "$value": "{description}"
          },
          "$links": {
            "$default": {
              "$url": "{$baseUrl}/applications('{$uuid}')?representation=application.$details",
              "$target": "blank"
            }
          }
        }
      }, {
        "$name": "role",
        "$title": "Role",
        "$selected": true,
        "$prototype": {
          "$": {
            "$value": "{description}"
          },
          "$links": {
            "$default": {
              "$url": "{$baseUrl}/roles('{$uuid}')?representation=role.$details",
              "$target": "blank"
            }
          }
        }
      }, {
        "$name": "team",
        "$title": "Teams",
        "$selected": true,
        "$prototype": {
          "$": {
            "$value": "{description}"
          },
          "$links": {
            "$default": {
              "$url": "{$baseUrl}/teams('{$uuid}')?representation=team.$details",
              "$target": "blank"
            }
          }
        }
      }]
    },
    "groups": {
      "$title": "Groups",
      "$capabilities": "sort,filter,add,delete",
      "$type": "application/x-collection",
      "$item": {
        "$type": "application/x-reference",
        "$url": "{$baseUrl}/{$pluralType}('{$key}')?representation={$representation}.$thumb",
        "$value": "{description}",
        "$description": "Access administration, groups associates users with roles and endpoints",
        "$key": "{$uuid}",
        "$": {
          "description": {
            "$type": "application/x-string"
          }
        },
        "$links": {
          "$details": {
            "$type": "application/json;vnd.sage=syracuse",
            "$url": "{$baseUrl}/groups('{$key}')?representation=group.$details&role={$role}"
          },
          "$select": {
            "$type": "application/json;vnd.sage=syracuse",
            "$title": "Ajouter",
            "$url": "{$baseUrl}/groups?representation=group.$select&role={$role}&trackingId={$trackingId}&binding=groups"
          }
        }
      }
    },
    "endpoints": {
      "$title": "Endpoints login",
      "$capabilities": "sort,filter,add,delete",
      "$type": "application/x-collection",
