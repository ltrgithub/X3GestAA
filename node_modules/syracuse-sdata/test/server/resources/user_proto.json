            "$constraints": {},
            "$isMandatory": true,
            "$capabilities": "sort,alphaTab"
          },
          "firstDayOfWeek": {
            "$title": "First day of week",
            "$type": "application/x-choice",
            "$value": {
              "$type": "application/x-integer",
              "$constraints": {
                "$enum": [{
                  "$value": 0,
                  "$title": "Sunday"
                }, {
                  "$value": 1,
                  "$title": "Monday"
                }, {
                  "$value": 2,
                  "$title": "Tuesday"
                }, {
                  "$value": 3,
                  "$title": "Wednesday"
                }, {
                  "$value": 4,
                  "$title": "Thursday"
                }, {
                  "$value": 5,
                  "$title": "Friday"
                }, {
                  "$value": 6,
                  "$title": "Saturday"
                }]
              }
            },
            "$capabilities": "sort"
          },
          "twoDigitYearMax": {
            "$title": "Two digit date's max year",
            "$type": "application/x-integer",
            "$constraints": {},
            "$isMandatory": true,
            "$capabilities": "sort"
          },
          "numberDecimalSeparator": {
            "$title": "Number decimal separator",
            "$type": "application/x-choice",
            "$value": {
              "$type": "application/x-string",
              "$constraints": {
                "$enum": [{
                  "$value": ".",
                  "$title": ". (dot)"
                }, {
                  "$value": ",",
                  "$title": ", (comma)"
                }]
              }
            },
            "$capabilities": "sort,alphaTab"
          },
          "numberGroupSeparator": {
            "$title": "Number group separator",
            "$type": "application/x-choice",
            "$value": {
              "$type": "application/x-string",
              "$constraints": {
                "$enum": [{
                  "$value": " ",
                  "$title": "Space"
                }, {
                  "$value": ".",
                  "$title": ". (dot)"
                }, {
                  "$value": ",",
                  "$title": ", (comma)"
                }, {
                  "$value": "",
                  "$title": "None"
                }, {
                  "$value": "'",
                  "$title": "' (hyphen)"
                }]
              }
            },
            "$capabilities": "sort,alphaTab"
          },
          "numberGroupSize": {
            "$title": "Number group size",
            "$type": "application/x-integer",
            "$constraints": {},
            "$capabilities": "sort"
          },
          "$creUser": {
            "$title": "Created by",
            "$type": "application/x-string",
            "$isDisabled": true,
            "$isHidden": true,
