            "$isDisabled": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
          },
          "$creDate": {
            "$title": "Created on",
            "$type": "application/x-datetime",
            "$isDisabled": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
          },
          "$updDate": {
            "$title": "Updated on",
            "$type": "application/x-datetime",
            "$isDisabled": true,
            "$isHidden": true,
            "$capabilities": "sort,filter"
          }
        },
        "$links": {}
      }
    },
