        "$items": [{
          "$layoutType": "columns",
          "$layoutSubType": "50,50",
          "$items": [{
            "$items": [{
              "$title": "Login",
              "$category": "block",
              "$layout": {
                "$items": [{
                  "$bind": "login"
                }, {
                  "$bind": "password"
                }]
              }
            }, {
              "$title": "Information",
              "$category": "block",
