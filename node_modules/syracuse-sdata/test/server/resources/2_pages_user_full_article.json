                  "$bind": "title"
                }, {
                  "$bind": "firstName"
                }, {
                  "$bind": "lastName"
                }, {
                  "$bind": "email"
                }, {
                  "$bind": "photo"
                }]
              }
            }]
          }, {
            "$items": [{
              "$title": "Explorer",
              "$category": "block",
              "$layout": {
                "$items": [{
                  "$bind": "explorer",
                  "$isTitleHidden": true
                }]
              }
            }]
          }]
        }, {
          "$title": "Administration",
          "$category": "block",
