{
	"$baseUrl": "http://localhost:8124/sdata/syracuse/collaboration/syracuse",
	"$baseType": "application/json;vnd.sage=syracuse",
	"$url": "{$baseUrl}/users",
	"$type": "application/json",
	"$title": "Liste de: users",
	"$properties": {
		"$resources": {
			"$type": "application/x-collection",
			"$item": {
				"$descriptor": "prototype user.$query",
				"$type": "application/json",
				"$url": "{$baseUrl}/{$pluralType}('{$key}')",
				"$value": "{firstName} {lastName}",
				"$title": "Unicode Table",
				"$description": "",
				"$pluralType": "unicodes",
				"$representation": "unicode",
				"$properties": {
					"unicodeValue": {
						"$title": "Unicode value",
						"$type": "application/x-string",
						
							"$isUnique": true
						,
						"$isMandatory": true,
						"$capabilities": "sort,filter,alphaTab"
					},
					"character": {
						"$title": "Character",
						"$type": "application/x-string",
						"$capabilities": "sort,filter,alphaTab"
					},
					"name": {
						"$title": "Name",
						"$type": "application/x-string",
						"$isMandatory": true,
						"$capabilities": "sort,filter,alphaTab"
					}
				}
			}
		}
	},
	"$pluralType": "unicodes",
	"$representation": "unicode",
	"$links": {},
	"$article": {
		"$layout": {
			"$items": [{
				"$category": "section",
				"$layout": {
					"$items": [{
						"$bind": "$resources",
						"$layout": {
							"$items": [{
								"$bind": "unicodeValue"
							}, {
								"$bind": "character"
							}, {
								"$bind": "name"
							}]
						}
					}]
				}
			}]
		}
	}
}