<script type="text/x-handlebars-template" id="tmplLayout"> 
<div class="sm-layout-slot">
	<header class="sm-layout-header">
	{{#if isTabLayout}}
		<div class="sm-nav-tabs-slot">
			<ul class="nav nav-tabs" role="tablist">
			{{#each navListItem}}
			<li class="{{#if active}}active{{/if}}"><a class="sm-tab" href="#">{{$title}}</a></li>
			{{/each}}
			</ul>
		</div>
	{{else}}
		{{$title}}
	{{/if}}
	</header>
	<div class="sm-layout-core {{$layoutType}}">
		{{#if hasOneChild}}
			{{{childHTML}}}
		{{else}}
			{{#each coreItem}}
				<div class="sm-coreitem-slot sm-{{$layoutType}} 
				{{#if isRowLayout}}
					{{#if $widthMd}}col-md-{{$widthMd}}{{/if}}
					{{#if $widthSm}}col-sm-{{$widthSm}}{{/if}}
					{{#if $widthXs}}col-xs-{{$widthXs}}{{/if}}
					{{#if $widthLd}}col-ld-{{$widthLd}}{{/if}}
				{{/if}}
				" 	{{#if display}}
						style="display:"
					{{else}}
						style="display:none"
				  	{{/if}}
				>
					{{{coreItemContent}}}
				</div>
			{{/each}}
		{{/if}}
	</div>
</div>
</script>
