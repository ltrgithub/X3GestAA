{
	"$itemsPerPage": 100,
	"$title": "Test long query",
	"$type": "{$baseType}.testDummyEntity.$query",
	"$properties":{
		"$resources": 
		{
			"$type": "application/x-array",
			"$url": "{$baseUrl}/testDummyEntity('{$key}')?representation=testDummyEntity.$queryItem",
			"$item": {
				"$properties": {
					"who": {
						"$type": "application/x-string",
						"$title": "Who"
					},
					"desc": {
						"$type": "application/x-string",
						"$title": "Description"
					},
					"phone": {
						"$type": "application/x-string",
						"$title": "Phone",
						"$format": "$phone"
					},
					"date": {
						"$type": "application/x-datetime",
						"$title": "Date"
					}
				},
				"$key": "{$uuid}",
				"$value": "{desc}",
				"$links": {
					"$details": {
						"$url": "{$baseUrl}/$localTodo('{$key}')?representation=$localTodo.$details",
						"$title": "Details"
					},
					"$edit": {
						"$url": "{$baseUrl}/$localTodo('{$key}')?representation=$localTodo.$edit",
						"$title": "Edit"
					}
				}
			},
			"$url": "{$baseUrl}/$localTodo('{$key}')?representation=$localTodo.$queryItem"
		}
	}
}