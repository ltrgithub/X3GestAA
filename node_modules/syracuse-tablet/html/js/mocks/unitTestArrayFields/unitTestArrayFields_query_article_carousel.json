{
  "$layoutType": "stack",
  "$localization": {},
  "$comment": "",
  "$allowDownload": true,
  "$pageActionSettings": [
    {
      "name": "LOCKE",
      "icon": {
        "css": "fa fa-lock",
        "name": "lock"
      },
      "$title": "Bloquer",
      "options": {
        "footerHeader": true,
        "refreshPage": true,
        "multiselection": true
      }
    },
    {
      "name": "UNLOCKE",
      "icon": {
        "css": "fa fa-unlock-alt",
        "name": "unlock-alt"
      },
      "$title": "DÃ©bloquer",
      "options": {
        "footerHeader": true,
        "refreshPage": true,
        "multiselection": true
      }
    }
  ],
  "$items": [
    {
      "$bind": "$resources",
      "$isTitleHidden": true,
      "$arrayOptions": {
        "showPagination": true,
        "showSortFilter": true,
        "rowIndex": true,
        "showSearch": true,
        "showRowDetailLink": false
      },
      "$statusEnabled": "multi",
      "$status": [
        {
          "field": "BPCNUM",
          "operator": "gt",
          "$type": "application/x-string",
          "value": "A",
          "color": {
            "css": "s-m-color-alert",
            "name": "Alert",
            "id": "alert",
            "bgColor": "rgb(203, 53, 0)",
            "color": "rgb(255, 255, 255)",
            "hex": "#cb3500"
          },
          "icon": {
              "css": "fa fa-lock",
              "name": "lock"
             }
        },
        {
            "field": "BPCNAM",
            "operator": "gt",
            "$type": "application/x-string",
            "value": "L",
            "color": {
              "css": "s-m-color-blue",
              "name": "Blue",
              "id": "blue",
              "bgColor": "rgb(0, 159, 218)",
              "color": "rgb(255, 255, 255)",
              "hex": "#009fda"
            },
            "icon": {
             "css": "fa fa-unlock",
             "name": "unlock"
            }
          },
          {
              "field": "BPCNAM",
              "operator": "lt",
              "$type": "application/x-string",
              "value": "Z",
              "color": {
                "css": "s-m-color-warning",
                "name": "Warning",
                "id": "warning",
                "bgColor": "rgb(255, 130, 64)",
                "color": "rgb(255, 255, 255)",
                "hex": "#ff8240"
              },
              "icon": {
               "css": "fa fa-thumbs-down",
               "name": "thumbs-down"
              }
            }
      ],
      "$arrayBgColor": "darkgrey",
      "$display": "carousel",
      "$detailsLink": "$details",
      "$card": {
        "$layoutType": "stack",
        "$localization": {},
        "$items": [
          {
            "$layoutType": "row",
            "$items": [
              {
                "$layoutType": "cell",
                "$bgColor": "",
                "$items": [
                  {
                    "$bind": "BPCNAM",
                    "$labelPosition": "top",
                    "$isTitleHidden": true,
                    "$theme": "largetitle"
                  }
                ]
              }
            ]
          },
          {
            "$layoutType": "row",
            "$items": [
              {
                "$layoutType": "cell",
                "$bgColor": "",
                "$widthXs": "6",
                "$widthSm": "6",
                "$widthMd": "6",
                "$widthLg": "6",
                "$items": [
                  {
                    "$bind": "BPCNUM",
                    "$labelPosition": "top"
                  }
                ]
              },
              {
                "$layoutType": "cell",
                "$bgColor": "",
                "$widthXs": "6",
                "$widthSm": "6",
                "$widthMd": "6",
                "$widthLg": "6",
                "$items": [
                  {
                    "$bind": "RPOSCOD",
                    "$labelPosition": "top"
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}