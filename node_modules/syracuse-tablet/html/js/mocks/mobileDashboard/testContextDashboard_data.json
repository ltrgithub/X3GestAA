{
		"dashboardName": "testContextDashboard",
		"$title": "Test dashboard parameters",
		"$description": "Test navigation to a dashboard with parameters",
		"vignettes": [
		        {
		        	"persistentId": "testRecordLinkToDashboard",
		        	"displayStyle": "$full",
		        	"gadget": {
		        		"$uuid": "testRecordLinkToDashboard"
		        	}
		        },
	            {
	            	"persistentId": "testRecordLinkToDashboardLink",
	            	"displayStyle": "$link",
		            "gadget": {
		                "$uuid": "testRecordLinkToDashboardLink"
		            }
	            },
	            {
	            	"persistentId": "testRecordLinkToDashboardSUPERV",
	            	"displayStyle": "$link",
	            	"gadget": {
	            		"$uuid": "testRecordLinkToDashboardSUPERV"
	            	}
	            }
		],
		"$mobileGadgets":{
			"testRecordLinkToDashboard": {
				"$type": "$representation",
				"$title": "Click to navigate",
				"entity": "testNavigateTo",
				"action": "$query",
				"representation": "testNavigateTo",
				"facet": "$query",
		        "$url": "{$hostPort}/mobile2/$client/$local/$dataset/testNavigateTo&representation=testNavigateTo.$query",
		         "$method": "GET"
			},
			"testRecordLinkToDashboardLink": {
				"$type": "$dashboard",
				"$title": "Open dashboard w/o params",
				"dashboardName": "testContextDashboardChild",
	            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/mobileDashboard('testContextDashboardChild')",
	            "$method": "GET"
			},
			"testRecordLinkToDashboardSUPERV": {
				"$type": "$representation",
				"$title": "Click to navigate",
				"entity": "AQMDEVICE",
				"action": "$query",
				"representation": "AQMDEVICEM",
				"facet": "$query",
				"endpoint": "x3.erp.UITEST"
			}
		}
}
