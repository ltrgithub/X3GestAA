{
	"$dashboardName": "unitTestDashboard",
	"dashboardName": "unitTestDashboard",
	"title": "Unit Tests Dashboard",
	"description": "Unit Tests Dashboard",
	
	"$mobileGadgets": {
	    "unitTestNativeFields": {
	        "$type": "$representation",
	        "$title": "Test native fields",
            "representation": "unitTestNativeFields",
	        "action": "$edit",
	        "facet": "$edit",
	        "$url": "{$hostPort}/mobile2/$client/$local/$dataset/unitTestNativeFields&representation=unitTestNativeFields.$edit",
	        "$method": "GET"
	    },
	    "unitTestCtrlField": {
            "$type": "$representation",
	        "$title": "Control fields",
            "entity": "unitTestCtrlField",
            "action": "$details",
            "representation": "unitTestCtrlField",
            "facet": "$details",
            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/unitTestCtrlField?representation=unitTestCtrlField.$details",
            "$method": "GET"
	    },
	    "unitTestArrayFieldsTable": {
            "$type": "$representation",
	        "$title": "Array Table",
            "entity": "unitTestArrayFields",
            "action": "$query",
            "representation": "unitTestArrayFields",
            "facet": "$query",
            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/unitTestArrayFields?representation=unitTestArrayFields.$query&articleName=table",
            "$method": "GET"
	    },
	    "unitTestArrayFieldsCard": {
            "$type": "$representation",
	        "$title": "Array Card",
            "entity": "unitTestArrayFields",
            "action": "$query",
            "representation": "unitTestArrayFields",
            "facet": "$query",
            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/unitTestArrayFields?representation=unitTestArrayFields.$query&articleName=card",
            "$method": "GET"
	    }
	},
	"vignettes": [{
        "persistentId": "unitTestCtrlField",
        "displayStyle": "$link",
        "gadget": {
            "$uuid": "unitTestCtrlField"
        }
    },{
        "persistentId": "unitTestNativeFields",
        "displayStyle": "$link",
        "gadget": {
            "$uuid": "unitTestNativeFields"
        }
    },{
        "persistentId": "unitTestArrayFieldsCard",
        "displayStyle": "$link",
        "gadget": {
            "$uuid": "unitTestArrayFieldsCard"
        }
    },{
        "persistentId": "unitTestArrayFieldsTableLink",
        "displayStyle": "$link",
        "gadget": {
            "$uuid": "unitTestArrayFieldsTable"
        }
    },{
        "persistentId": "unitTestArrayFieldsTableFull",
        "displayStyle": "$full",
        "gadget": {
            "$uuid": "unitTestArrayFieldsTable"
        }
    }]

}
