									"$bind": "BAC"
								}, {
									"$bind": "PHOTO"
								}]
							}
						}]
					}
				}]

			}, {
				"$layoutType": "tile",
				"$title": "",
				"$size": "medium",
				"$items": [{
					"$bind": "$x3FixedAssetCreateLink"
				}]
			}, {
				"$layoutType": "tile",
				"$title": "",
