				"$size": "full",
				"$items": [{
					"$bind": "$x3fixedassetsList",
					"$article":{
						"$layoutType": "stack",
						"$items": [{
							"$bind": "$resources",
							"$display": "card",
							"$cardsPerRow": 3,
							"$card": {
								"$items": [{
									"$bind": "LCTCOD_REF"
								}, {
									"$bind": "PHYDES1"
								}, {
									"$bind": "PHYREF"
								}, {
									"$bind": "BAC"
								}, {
									"$bind": "PHOTO"
								}]
							},
							"$arrayOptions": {
								"showSearch": true
							}
						}]
	
					}
				}]
			}, {
				"$layoutType": "tile",
				"$title": "Locations - List",
				"$size": "full",
				"$items": [{
					"$bind": "$x3LocationList",
					"$article": {
						"$layoutType": "stack",
						"$items": [{
							"$bind": "$resources",
							"$display": "card",
							"$cardsPerRow": 3,
							"$card": {
								"$layoutType": "stack",
