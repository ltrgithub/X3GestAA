			"$title": "Creations",
			"$items": [{
				"$layoutType": "tile",
				"$title": "Physical Assets - Create",
				"$size": "full",
				"$items": [{
					"$bind": "$x3FixedAssetCreate",
					"$pageActionSettings": [{
						"name": "$save",
						"icon": {
							"css": "fa fa-check",
							"name": "check"
						},
						"$title": "Enregistrer",
						"options": {
							"footerHeader": true,
							"refreshPage": false,
							"multiselection": false
						}
