							"footerHeader": true,
							"refreshPage": false,
							"multiselection": false
						}
					}],
					"$article": {
						"$layoutType": "stack",
						"$items": [{
							"$bind": "BUICOD_REF"
						}, {
							"$bind": "LCTDES"
						}, {
							"$bind": "LCTCOD"
						}, {
							"$bind": "FCY_REF"
						}, {
							"$bind": "BAC"
						}, {
							"$bind": "PHOTO"
						}]

					}
				}]

