{
    "dashboardName": "testLayoutsDashboard",
    "vignettes": [
        {
            "persistentId": "testLayoutHubDashboard",
            "displayStyle": "$link",
            "gadget": {
                "$uuid": "testLayoutHubDashboard"
            }
        },
        {
            "persistentId": "testLayoutRowsDashboard",
            "displayStyle": "$link",
            "gadget": {
                "$uuid": "testLayoutRowsDashboard"
            }
        },
        {
            "persistentId": "testLongQuery",
            "displayStyle": "$link",
            "gadget": {
                "$uuid": "testLongQuery"
            }
        },
        {
            "persistentId": "testHubLayoutEntity",
            "displayStyle": "$link",
            "gadget": {
                "$uuid": "testHubLayoutEntity"
            }
        }
    ],
    "$mobileGadgets": {
        "testLayoutHubDashboard": {
            "$type": "$dashboard",
            "$title": "Hub Layout tests",
            "$description": "Hub Layout tests",
            "dashboardName": "testLayoutHubDashboard",
            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/mobileDashboard('testLayoutHubDashboard')",
            "$method": "GET"
        },
        "testLayoutRowsDashboard": {
            "$type": "$dashboard",
            "$title": "Rows Layout tests",
            "$description": "Rows Layout tests",
            "dashboardName": "testLayoutRowsDashboard",
            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/mobileDashboard('testLayoutRowsDashboard')",
            "$method": "GET"
        },
        "testLongQuery": {
            "$type": "$representation",
            "$title": "Long query",
            "$description": "Query",
            "entity": "testDummyEntity",
            "action": "$query",
            "representation": "testDummyEntity",
            "facet": "$query",
            "$url": "{$hostPort}/mobile2/$client/$local/$dataset/testDummyEntity&representation=testDummyEntity.$query",
            "$method": "GET"
        }
    },
    "title": "Layout test dashboard",
    "description": "Test different kinds of layouts"
}