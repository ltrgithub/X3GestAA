{
		"$dashboardName": "testContextDashboardChild",
		"$title": "Child dashboard",
		"$description": "Child dashboard getting parameters that are applied to the first to vignettes but not to the last one using the same representation to gather data",
		"vignettes": [
		        {
		        	"persistentId": "testRecordWithContext1",
		        	"displayStyle": "$full",
		        	"gadget": {
		        		"$uuid": "testRecordWithContext"
		        	}
		        },
	            {
	            	"persistentId": "testRecordWithContext2",
	            	"displayStyle": "$link",
		            "gadget": {
		                "$uuid": "testRecordWithContext"
		            }
	            },
	            {
	            	"persistentId": "testRecordWithoutContext",
	            	"displayStyle": "$link",
	            	"gadget": {
	            		"$uuid": "testRecordWithoutContext"
	            	}
	            }
		],
		"$mobileGadgets":{
			"testRecordWithContext": {
				"$type": "$representation",
				"$title": "Click to navigate",
				"entity": "testNavigateToChild",
				"action": "$query",
				"representation": "testNavigateToChild",
				"facet": "$query",
				"parameters": {
					"where": {
						"value": "group eq '{group}'"
					}
				},       
				"$url": "{$hostPort}/mobile2/$client/$local/$dataset/testNavigateToChild&representation=testNavigateToChild.$query",
		        "$method": "GET"
			},
			"testRecordWithoutContext": {
				"$type": "$representation",
				"$title": "Click to navigate",
				"entity": "testNavigateToChild",
				"action": "$query",
				"representation": "testNavigateToChild",
				"facet": "$query",       
				"$url": "{$hostPort}/mobile2/$client/$local/$dataset/testNavigateToChild&representation=testNavigateToChild.$query",
		        "$method": "GET"
			}
		}
}
