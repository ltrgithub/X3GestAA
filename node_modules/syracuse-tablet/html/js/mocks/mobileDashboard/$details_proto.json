{
	"$type": "application/json",
	"$prototype": "{$baseUrl}/$prototypes('{$representation}.$details')",
	"$url": "{$baseUrl}/{$pluralType}('{$key}')",
	"$shortUrl": "{$baseUrl}/{$pluralType}('{$key}')",
	"$value": "{title}",
	"$title": "Mobile dashboard",
	"$key": "{$uuid}",
	"$description": "Mobile dashboard definition",
	"$pluralType": "mobileDashboards",
	"$representation": "mobileDashboard",
	"$properties": 
	{
		"dashboardName": 
		{
			"$title": "Dashboard name",
			"$type": "application/x-string"
		},

		"title": 
		{
			"$title": "Title",
			"$type": "application/x-string"
		},

		"description": 
		{
			"$title": "Description",
			"$type": "application/x-string"
		},

		"vignettes": 
		{
			"$title": "Gadgets",
			"$type": "application/x-array",
			"$item": 
			{
				"$type": "application/json",
				"$prototype": "{$baseUrl}/$prototypes('{$representation}.$details')",
				"$key": "{$uuid}",
				"$description": "Mobile dashboard vignette",
				"$pluralType": "mobileDashboardVignettes",
				"$representation": "mobileDashboardVignette",
				"$properties": 
				{
					"displayStyle": 
					{
						"$title": "Display style",
						"$type": "application/x-choice",
						"$value": 
						{
							"$type": "application/x-string",
							"$enum": 
							[
								{
									"$value": "$link",
									"$title": "Always display as link"
								},

								{
									"$value": "$full",
									"$title": "Always display as vignette"
								},

								{
									"$value": "$fullOnBigDeviceOnly",
									"$title": "Display as link on small devices"
								}
							]
						},
						"$isNullable": false
					},

					"persistentId": 
					{
						"$title": "persistentId",
						"$isExcluded": true,
						"$type": "application/x-string"
					},

					"gadget": 
					{
						"$title": "Gadget",
						"$isMandatory": true,
						"$capabilities": "sort",
						"$type": "application/x-reference",
						"$item": 
						{
							"$url": "{$baseUrl}/mobileGadgets('{$key}')?representation={$representation}.$thumb",
							"$shortUrl": "{$baseUrl}/mobileGadgets('{$key}')",
							"$value": "{title}",
							"$description": "Mobile gadget definition",
							"$key": "{$uuid}",
							"$properties": 
							{
								"title": 
								{
									"$type": "application/x-string"
								}
							},

							"$prototype": "{$baseUrl}/$prototypes('{$representation}.$thumb')",
							"$representation": "mobileGadget",
							"$links": 
							{
								"$details": 
								{
									"$type": "application/json;vnd.sage=syracuse",
									"$url": "{$baseUrl}/mobileGadgets('{$key}')?representation=mobileGadget.$details&role={$role}"
								}
							}
						}
					}
				},
				"$shortUrl": "{$shortUrl}/vignettes('{$uuid}')"
			}
		}
	},
	"$descriptor": "prototype mobileDashboard.$details",
	"$baseType": "application/json;vnd.sage=syracuse",
	"$baseUrl": "http://localhost:8124/mobile2/syracuse/collaboration/syracuse",
	"$baseHelpUrl": "http://localhost:8124/mobile2/syracuse/collaboration/syracuse"
}