{
	"$layoutType": "stack",
	"$items": [{
		"$layoutType": "row",
		"$items": [{
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesQueryLink"
			}]
		}, {
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesDetailLink"
			}]
		}, {
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesEditLink"
			}]
		}, {
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesCreateLink"
			}]
		}]
	}, {
		"$layoutType": "row",
		"$items": [{
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesQuery",
				"$article":{
						"$layoutType": "stack",
						"$items": 
						[
							{
								"$bind": "$resources",
								"$format": "grid",
								"$isQuickFilter": true,
								"$alternateStyle": true,
								"$isTopLabelAlignment": true,
								"$layout": 
								{
									"$items": 
									[
										{
											"$bind": "propString"
										}
									]
								}
							}
						]
				}
			}]
		}, {
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesDetail"
			}]
		}, {
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesEdit"
			}]
		}, {
			"$layoutType": "cell",
			"$widthXs": "3",
			"$items": [{
				"$bind": "testDatatypesCreate"
			}]
		}]
	}]
}
		