{
 "$layoutType": "stack",
 "$localization": {},
 "$items": [
  {
   "$bind": "$resources",
   "$isTitleHidden": true,
   "$arrayOptions": {
    "showPagination": true,
    "showSortFilter": true,
    "showSearch": true,
    "rowIndex": true,
    "showRowDetailLink": false
   },
   "$arrayBgColor": "darkgrey",
   "$table": [
    {
     "$bind": "CODE",
     "$type": "application/x-integer",
     "$titleOrig": "{@5296}",
     "$widthType": "fixed",
     "$widthVal": 3,
     "$imgHeight": "small",
     "$title": "Code"
    },
    {
     "$bind": "MODULE",
     "$type": "application/x-choice",
     "$titleOrig": "{@943}",
     "$widthType": "fixed",
     "$widthVal": 12,
     "$imgHeight": "small",
     "$title": "Module"
    },
    {
     "$bind": "CUR_REF",
     "$type": "application/x-reference",
     "$titleOrig": "{@323}",
     "$widthType": "fixed",
     "$widthVal": 6,
     "$imgHeight": "small",
     "$title": "Devise"
    },
    {
     "$bind": "TDCB2",
     "$type": "application/x-decimal",
     "$titleOrig": "{@55367}",
     "$widthType": "fixed",
     "$widthVal": 8,
     "$imgHeight": "small",
     "$title": "4-décimal"
    },
    {
     "$bind": "UNIT_REF",
     "$type": "application/x-reference",
     "$titleOrig": "{@2685}",
     "$widthType": "fixed",
     "$widthVal": 7,
     "$imgHeight": "small",
     "$title": "Unité"
    },
    {
     "$bind": "SITE_REF",
     "$type": "application/x-reference",
     "$titleOrig": "{@1344}",
     "$widthType": "fixed",
     "$widthVal": 6,
     "$refDescFormat": "title",
     "$refDescPosition": "none",
     "$imgHeight": "small",
     "$title": "Site"
    },
    {
     "$bind": "TDATE",
     "$type": "application/x-date",
     "$titleOrig": "{@212}",
     "$widthType": "fixed",
     "$widthVal": 6,
     "$imgHeight": "small",
     "$title": "Date"
    },
    {
     "$bind": "CPY_REF",
     "$type": "application/x-reference",
     "$titleOrig": "{@1343}",
     "$widthType": "fixed",
     "$widthVal": 8,
     "$imgHeight": "small",
     "$refDescPosition": "top",
     "$title": "Société"
    },
    {
     "$bind": "TEXTDES0",
     "$type": "application/x-string",
     "$titleOrig": "{@282}",
     "$widthType": "auto",
     "$widthVal": 0,
     "$imgHeight": "small",
     "$title": "Désignation"
    }
   ],
   "$statusEnabled": "mono",
   "$status": [
    {
     "field": "TDCB2",
     "operator": "lt",
     "$type": "application/x-decimal",
     "$format": "0.0000",
     "value": 10,
     "color": {
      "css": "s-m-color-green",
      "name": "Vert",
      "id": "green",
      "bgColor": "rgb(65, 169, 64)",
      "color": "rgb(255, 255, 255)",
      "hex": "#41a940"
     },
     "icon": {
      "css": "fa fa-check-square",
      "name": "check-square"
     }
    },
    {
     "field": "TDCB2",
     "operator": "lt",
     "$type": "application/x-decimal",
     "$format": "0.0000",
     "value": 100,
     "color": {
      "css": "s-m-color-blue",
      "name": "Bleu",
      "id": "blue",
      "bgColor": "rgb(0, 159, 218)",
      "color": "rgb(255, 255, 255)",
      "hex": "#009fda"
     },
     "icon": {
      "css": "fa fa-plus-circle",
      "name": "plus-circle"
     }
    },
    {
     "field": "TDCB2",
     "operator": "lt",
     "$type": "application/x-decimal",
     "$format": "0.0000",
     "value": 1000,
     "color": {
      "css": "s-m-color-warning",
      "name": "Avertissement",
      "id": "warning",
      "bgColor": "rgb(255, 130, 64)",
      "color": "rgb(255, 255, 255)",
      "hex": "#ff8240"
     },
     "icon": {
      "css": "fa fa-info-circle",
      "name": "info-circle"
     }
    },
    {
     "field": "TDCB2",
     "operator": "lt",
     "$type": "application/x-decimal",
     "$format": "0.0000",
     "value": 10000,
     "color": {
      "css": "s-m-color-purple",
      "name": "Violet",
      "id": "purple",
      "bgColor": "rgb(102, 57, 183)",
      "color": "rgb(255, 255, 255)",
      "hex": "#6639b7"
     },
     "icon": {
      "css": "fa fa-lock",
      "name": "lock"
     }
    },
    {
     "field": "TDCB2",
     "operator": "gt",
     "$type": "application/x-decimal",
     "$format": "0.0000",
     "value": 10000,
     "color": {
      "css": "s-m-color-alert",
      "name": "Alerte",
      "id": "alert",
      "bgColor": "rgb(203, 53, 0)",
      "color": "rgb(255, 255, 255)",
      "hex": "#cb3500"
     },
     "icon": {
      "css": "fa fa-exclamation-triangle",
      "name": "exclamation-triangle"
     }
    }
   ],
   "$filters": "tabs"
  }
 ]
}