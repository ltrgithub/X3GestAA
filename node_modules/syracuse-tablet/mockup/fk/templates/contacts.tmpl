<div class="container">
	{{#each rows}}
	<div class="row">
		{{{{#each columns}}}}
		<div class="col-md-2>
			<div class="thumbnail">
				<img src="../images/customers/customericon.png" class="img-circle">
				<div class="s-caption">
					<div class="s-field-label">{{firstname}} {{lastname}}</div>
					<div class="s-field-icons"></div>
				</div>
			</div>
		</div>
		{{/each}}
	</div>
	{{/each}}
</div>