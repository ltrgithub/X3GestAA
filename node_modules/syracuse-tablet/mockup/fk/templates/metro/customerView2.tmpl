<div class="s-site-container">
<div class="tile-area tile-area-dark">
	<h1 class="tile-area-title fg-white">
	<a href="/syracuse-mobile/surface/fk/metroui.html" data-s-picker="back"><i class="icon-arrow-left-3 fg-white smaller"></i></a>
	Customer List
	</h1>
	{{#each items}}
	<div class="tile bg-green" data-click="transform">
	    <div class="tile-content icon">
	        <img src="images/customers/customericon.png">
	    </div>
	    <div class="brand">
	        <span class="label">{{firstname}} {{lastname}}</span>
	    </div>
	</div>
 	{{/each}}
</div>
</div>