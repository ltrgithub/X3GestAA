	<div class="s-list-core">
	<div class="s-card-grid">
	{{#each rows}}
	<div class="row">
		{{#each columns}}
		<div class="col-md-2">
			<div class="thumbnail s-card-layout">
				<a href="bootstrap_customer_contact.html" class="s-card-link">
					<div class="s-card-layout-core">
					<img src="../images/bootstrap/customericon.png" class="img-circle img-responsive s-card-image">
					</div>
					<div class="s-caption s-card-caption">
						<div class="s-field-label">{{firstname}} {{lastname}}</div>
						<div class="s-field-icons"></div>
					</div>
				</a>
			</div>
		</div>
		{{/each}}
	</div>
	{{/each}}
	</div>
	<div class="s-card-grid" style="display:none">
	{{#each rows}}
	<div class="row">
		{{#each columns}}
		<div class="col-md-2">
			<div class="thumbnail s-card-layout">
				<a href="bootstrap_customer_company.html" class="s-card-link">
					<div class="s-card-layout-core">
					<img src="../images/bootstrap/company.png" class="img-circle img-responsive s-card-image">
					</div>
					<div class="s-caption s-card-caption">
						<div class="s-field-label">{{companyName}}</div>
						<div class="s-field-icons"></div>
					</div>
				</a>
			</div>
		</div>
		{{/each}}
	</div>
	{{/each}}
	</div>
	</div>