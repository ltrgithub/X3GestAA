<script type="text/x-handlebars-template" id="historypanel">
<div class="panel panel-success" id="{{id}}">
	<div class="panel-heading">
		<h3 class="panel-title">Test history</h3>
	</div>
	<div class="panel-body">
		<div class="btn-group">
			{{#each btnList}} 
				<button type="button" class="btn btn-default" data-index="{{idx}}">Page {{idx}}</button>
			{{/each}}
		</div>
		<div id="{{pageid}}"></div>
	</div>
</div>
</script>
<script type="text/x-handlebars-template" id="historypage">
<div class="panel panel-info">
	<div class="panel-heading">
		<h3 class="panel-title">{{title}}</h3>
	</div>
	<div class="panel-body"><h1>Welcome to test page {{idx}} <small>Try#{{tryNum}}</small></h1></div>
</div>
</script>