{
    "$name": "AFB120 2 X3",
    "$variants": {
        "AFB120_01": {
            "$before": [
                "{$out} = {$new('RELBANK')}",
                "{$out}.CODE = {$in}.bank",
                "{$out}.TYPE = 1",
                "{$OPERATION_NUMBER} = 1",
                "{$out}.OPERATION_NUMBER = {$OPERATION_NUMBER}",
                "{$out}.BANK = {$in}.bank",
                "{$out}.DESK = {$in}.desk",
                "{$out}.ACCOUNT = {$in}.account",
                "{$out}.AMOUNT = Math.abs({$in}.amount/100)",
                "{$out}.CURRENCY = {$in}.currency",
                "{$out}.DATE = {$in}.date",
                "{$out}.VALUE_DATE = null",
                "{$out}.DIRECTION = ({$in}.amount >= 0)?1:-1"
              
            ]
        },
        "AFB120_04": {
            "$before": [
                "{$out} = {$new('RELBANK')}",
                "{$out}.CODE = {$in}.bank",
                "{$out}.TYPE = 2",
                "{$out}.OPERATION_NUMBER = ++{$OPERATION_NUMBER}",
                "{$out}.BANK = {$in}.bank",
                "{$out}.DESK = {$in}.desk",
                "{$out}.ACCOUNT = {$in}.account",
                "{$out}.AMOUNT = Math.abs({$in}.amount/100)",
                "{$out}.CURRENCY = {$in}.currency",
                "{$out}.DESCRIPTION = {$in}.operation",
                "{$out}.DATE = {$in}.accounting_date", 
                "{$out}.REFERENCE = {$in}.reference", 
                "{$out}.VALUE_DATE = {$in}.value_date",
                "{$out}.DIRECTION = ({$in}.amount >= 0)?1:-1"
            ]
        },
        "AFB120_07": {
            "$before": [
                "{$out} = {$new('RELBANK')}",
                "{$out}.CODE = {$in}.bank",
                "{$out}.TYPE = 3",
                "{$out}.OPERATION_NUMBER = ++{$OPERATION_NUMBER}",
                "{$out}.BANK = {$in}.bank",
                "{$out}.DESK = {$in}.desk",
                "{$out}.ACCOUNT = {$in}.account",
                "{$out}.AMOUNT = Math.abs({$in}.amount/100)",
                "{$out}.CURRENCY = {$in}.currency",
                "{$out}.DATE = {$in}.date",
                "{$out}.VALUE_DATE = null",
                "{$out}.DIRECTION = ({$in}.amount >= 0)?1:-1"  
            ]
        }
    }
}
