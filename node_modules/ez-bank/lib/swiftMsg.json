{
    "$regex": "{([0-9]:{[^}]*})}|{([0-9]:[^}]*)}",
    "$type": "application/json",
    "$properties": {
        "type": {
            "$after": ["\r\n:",":"],
            "$type": "application/x-number",
            "$format":"0"
        },
        "chunk": {
            "$type": "application/x-string"
        }
    }
}