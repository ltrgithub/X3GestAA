test("buffer TPRM : get info of a remote file", function() {
	var str = "@SUPERV./toto$txt"; // not exists
	var sadFsqFile = new SadFsqFile(str);
	var buff = comm._getBufTPRM(sadFsqFile, "size");
	var bytes = new Buffer([0x00, 0x00, 0x17, 0x07, 0x08, 0x00, 0x13, 0x00, 0x11, 0x41, 0x53, 0x55, 0x50, 0x45, 0x52, 0x56, 0x2e, 0x2f, 0x74, 0x6f, 0x74, 0x6f, 0x24, 0x74, 0x78, 0x74]);
	strictEqual(buff.toString('hex'), bytes.toString('hex'), "file " + str + " TPRM");

});


