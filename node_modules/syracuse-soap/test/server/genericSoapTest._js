function checkStatus(value, expected, operationName) {
	strictEqual(parseInt(value.$value, 10), expected, operationName + " status [" + value.$value + "] OK");
}

function checkMessage(typeValue, typeExpected, msgValue, msgExpected) {
	strictEqual(parseInt(typeValue, 10), typeExpected, "message type [" + typeValue + "] OK");
	strictEqual(msgValue, msgExpected, "message [" + msgValue + "] OK");
}

function checkProcessReport(techInfo) {
	ok(techInfo.processReport && techInfo.processReport.$cdata != null, "process report [" + techInfo.processReport && techInfo.processReport.$cdata + "] OK");
}

