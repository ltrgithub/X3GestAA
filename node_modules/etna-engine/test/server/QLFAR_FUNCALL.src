Local Clbfile RESULT
RESULT = func TESTSUITE()

Call FERME_TRACE From LECFIC
GTRACE = "QLFAR_FUNCALL_ETNA_"+GACTX.USER
Call LEC_TRACE From LECFIC
End


Funprog TESTSUITE()
  Call ADD_TESTCASE("TEST_FUNC", "func call", 2) From AXUNIT
End func AXUNIT.RUN_TESTSUITE("REQ-70693", "Encoding")

Subprog SETUP
End

Subprog TEARDOWN
End

Subprog TEST_FUNC
  Local Char S(250)
  Local Char T(250)

  Call CHECK_EQUAL(func FUN_STRING1(S), S) From AXUNIT
  Call CHECK_EQUAL(func FUN_STRING2(S), "3 b") From AXUNIT
End

Funprog FUN_STRING1(S)
  Value Clbfile S
End S

Funprog FUN_STRING2(S)
  Value Char S( )
  Local Char B(5)
  B = "b"
End "3" - func FUN_STRING3(B)

Funprog FUN_STRING3(S)
  Const Char S
End S
