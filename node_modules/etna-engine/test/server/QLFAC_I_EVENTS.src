Subprog TC_BASIC
Local Instance INS1 Using C_AQCPROC01
INS1 = NewInstance C_AQCPROC01

INS1.C01P01 = "QUNIT"
Call CHECK_EQUAL(INS1.C01P01,"QUNIT") From AXUNIT

INS1.C01P01 = "QUNIT-CONTROL"
Call CHECK_EQUAL(INS1.C01P01,"QUNIT-CONTROL-OK") From AXUNIT
End

