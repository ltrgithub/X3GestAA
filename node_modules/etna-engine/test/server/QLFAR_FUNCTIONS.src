Subprog TC_COS
Integer EX,RESULT
Onerrgo ERROR

# On calcul désormais en radians
[S]adxmda = 1
Call CHECK_EQUAL(ar2(pi), 3.14)
Call CHECK_EQUAL(cos(0), 1)
Call CHECK_EQUAL(cos(pi)  , -1)
Call CHECK_EQUAL(cos(pi/2), 0)
Call CHECK_EQUAL(cos(pi/3), 0.5)
Call CHECK_EQUAL(ar2(cos(pi/4)), ar2(sqr(2)/2))
Call CHECK_EQUAL(ar2(cos(pi/6)), ar2(sqr(3)/2))

# On calcule désormais en degrés
[S]adxmda = 0
Call CHECK_EQUAL(cos(0) , 1)
Call CHECK_EQUAL(cos(90), 0)
Call CHECK_EQUAL(ar2(cos(45)), 0.71 )
Call CHECK_EQUAL(ar2(cos(30)), 0.87 )

# On calcule désormais en grades
[S]adxmda = 2
Call CHECK_EQUAL(cos(0)    , 1)
Call CHECK_EQUAL(cos(100)  , 0)
Call CHECK_EQUAL(ar2(cos(50))  , 0.71 )
Call CHECK_EQUAL(ar2(cos(100/3)), 0.87 )
End

