Subprog TC_DATE0
Local Date AI(0..)
Local Integer BASE
Local Integer RESDIM : RESDIM=32767
Local Integer RESMAXTAB : RESMAXTAB=5
Call CHECK_EQUAL(maxtab(AI), -1)
Call CHECK_EQUAL(dim(AI),RESDIM)
###
Call FCT1_DAT_VAR(AI,RESDIM,0)
Call FCT1_DAT_VAL(AI,RESDIM,0)
Call FCT1_DAT_CONST(AI,RESDIM,0)
Call FCT1_DAT_OBJ_VAR(AI,RESDIM,0)
Call FCT1_DAT_OBJ_VAL(AI,RESDIM,0)
Call FCT1_DAT_OBJ_CONST(AI,RESDIM,0)
###
Call FCT0_DAT_VAR(AI,RESDIM,-1)
Call FCT0_DAT_VAL(AI,RESDIM,-1)
Call FCT0_DAT_CONST(AI,RESDIM,-1)
Call FCT0_DAT_OBJ_VAR(AI,RESDIM,-1)
Call FCT0_DAT_OBJ_VAL(AI,RESDIM,-1)
Call FCT0_DAT_OBJ_CONST(AI,RESDIM,-1)
###
Call FCT_DAT_VAR(AI,RESDIM,-1)
Call FCT_DAT_VAL(AI,RESDIM,-1)
Call FCT_DAT_CONST(AI,RESDIM,-1)
Call FCT_DAT_OBJ_VAR(AI,RESDIM,-1)
Call FCT_DAT_OBJ_VAL(AI,RESDIM,-1)
Call FCT_DAT_OBJ_CONST(AI,RESDIM,-1)
###
Gosub DEF_DATE
Call CHECK_EQUAL(dim(AI),RESDIM)
Call CHECK_EQUAL(maxtab(AI), RESMAXTAB)
###
Call FCT1_DAT_VAR(AI,RESDIM,RESMAXTAB+1)
Call FCT1_DAT_VAL(AI,NBELT,RESMAXTAB+1)
Call FCT1_DAT_CONST(AI,RESDIM,RESMAXTAB+1)
Call FCT1_DAT_OBJ_VAR(AI,RESDIM,RESMAXTAB+1)
Call FCT1_DAT_OBJ_VAL(AI,NBELT,RESMAXTAB+1)
Call FCT1_DAT_OBJ_CONST(AI,RESDIM,RESMAXTAB+1)
###
Call FCT0_DAT_VAR(AI,RESDIM,RESMAXTAB)
Call FCT0_DAT_VAL(AI,NBELT,RESMAXTAB)
Call FCT0_DAT_CONST(AI,RESDIM,RESMAXTAB)
Call FCT0_DAT_OBJ_VAR(AI,RESDIM,RESMAXTAB)
Call FCT0_DAT_OBJ_VAL(AI,NBELT,RESMAXTAB)
Call FCT0_DAT_OBJ_CONST(AI,RESDIM,RESMAXTAB)
###
Call FCT_DAT_VAR(AI,RESDIM,RESMAXTAB)
Call FCT_DAT_VAL(AI,NBELT,RESMAXTAB)
Call FCT_DAT_CONST(AI,RESDIM,RESMAXTAB)
Call FCT_DAT_OBJ_VAR(AI,RESDIM,RESMAXTAB)
Call FCT_DAT_OBJ_VAL(AI,NBELT,RESMAXTAB)
Call FCT_DAT_OBJ_CONST(AI,RESDIM,RESMAXTAB)
###
Call CHECK_EQUAL(min(AI), [02/06/2014])
Call CHECK_EQUAL(max(AI), [09/06/2014])
Call CHECK_EQUAL(find([05/06/2014],AI), 2)
Call CHECK_EQUAL(find([04/06/2014],AI), 0)
Sorta NBELT AI
Call CHECK_EQUAL(find([02/06/2014],AI), 1)
Call CHECK_EQUAL(find([07/06/2014],AI), 4)
Dela 2,1 AI : #delete 5
Call CHECK_EQUAL(find([07/06/2014],AI), 3)
Call CHECK_EQUAL(dim(AI),RESDIM)
Call CHECK_EQUAL(maxtab(AI), RESMAXTAB)
Insa 2,1 AI
AI(2) =  [04/06/2014]
Call CHECK_EQUAL(find([07/06/2014],AI), 4)
Call CHECK_EQUAL(find([04/06/2014],AI), 3)
Call CHECK_EQUAL(dim(AI),RESDIM)
Call CHECK_EQUAL(maxtab(AI), RESMAXTAB+1)
Insa 2,2 AI
AI(2)=[28/06/2014]
AI(3)=[29/06/2014]
Call CHECK_EQUAL(find([07/06/2014],AI), 6)
Call CHECK_EQUAL(find([04/06/2014],AI), 5)
Call CHECK_EQUAL(dim(AI),RESDIM)
Call CHECK_EQUAL(maxtab(AI), RESMAXTAB+3)
Call CHECK_EQUAL(find([29/06/2014],AI), 4)
Dela 2,3 AI
Call CHECK_EQUAL(find([07/06/2014],AI), 3)
Call CHECK_EQUAL(dim(AI),RESDIM)
Call CHECK_EQUAL(maxtab(AI), RESMAXTAB+3)
End

